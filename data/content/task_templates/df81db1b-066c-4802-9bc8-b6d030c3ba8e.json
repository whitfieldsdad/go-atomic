{
  "id": "df81db1b-066c-4802-9bc8-b6d030c3ba8e",
  "name": "Command Prompt read contents from CMD file and execute",
  "description": "Simulate Raspberry Robin using the \"standard-in\" command prompt feature cmd `/R \u003c` to read and execute a file via cmd.exe\nSee https://redcanary.com/blog/raspberry-robin/. \n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "27da9eae-3954-42f3-ab4b-97750d577aed",
      "type": "execute-command",
      "data": {
        "command": "cmd /r cmd\u003c\"#{input_file}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "96f70975-39b2-43b4-946e-a21d7621df25",
      "name": "Check dependency 1/1",
      "description": "CMD file must exist on disk at specified location (#{input_file})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{input_file}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "c249412b-96ed-4bcc-83fc-d56dbe5a6938",
      "name": "Resolve dependency 1/1",
      "description": "CMD file must exist on disk at specified location (#{input_file})\n",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Type Directory (split-path \"#{input_file}\") -ErrorAction ignore | Out-Null\nInvoke-WebRequest \"https://github.com/redcanaryco/atomic-red-team/raw/master/atomics/T1059.003/src/t1059.003_cmd.cmd\" -OutFile \"#{input_file}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "96fe5eb8-9dd1-453d-bbe8-6801511e1732",
      "name": "Re-check dependency 1/1",
      "description": "CMD file must exist on disk at specified location (#{input_file})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{input_file}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "96f70975-39b2-43b4-946e-a21d7621df25",
      "p": "on-success",
      "o": "27da9eae-3954-42f3-ab4b-97750d577aed"
    },
    {
