      "id": "3bb3afd6-d0c8-4f1a-a919-51b3144bebc4",
      "type": "execute-command",
      "data": {
        "command": "sudo systemctl enable falcon-sensor.service\nsudo systemctl start falcon-sensor.service",
        "command_type": "sh"
      },
      "elevation_required": true
    }
  ],
