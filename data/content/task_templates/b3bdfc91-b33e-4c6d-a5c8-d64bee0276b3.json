{
  "id": "b3bdfc91-b33e-4c6d-a5c8-d64bee0276b3",
  "name": "WMI Execute Local Process",
  "description": "This test uses wmic.exe to execute a process on the local host.\nWhen the test completes , a new process will be started locally .A notepad application will be started when input is left on default.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "6470b9c5-34d9-42ba-8468-e0caabb5984e",
      "type": "execute-command",
      "data": {
        "command": "wmic process call create #{process_to_execute}\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "87192318-75fd-4a59-add7-71a284c520ef",
      "type": "execute-command",
      "data": {
        "command": "wmic process where name='#{process_to_execute}' delete \u003enul 2\u003e\u00261\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "6470b9c5-34d9-42ba-8468-e0caabb5984e",
      "p": "on-success",
      "o": "87192318-75fd-4a59-add7-71a284c520ef"
    },
    {
      "s": "6470b9c5-34d9-42ba-8468-e0caabb5984e",
      "p": "on-failure",
      "o": "87192318-75fd-4a59-add7-71a284c520ef"
    }
  ],
  "tags": [
    "T1047"
  ],
  "input_arguments": [
    {
      "name": "process_to_execute",
      "type": "string",
      "description": "Name or path of process to execute.",
      "value": "notepad.exe"
    }
  ]
}