{
  "id": "96f974bb-a0da-4d87-a744-ff33e73367e9",
  "name": "System Discovery using SharpView",
  "description": "Get a listing of network connections, domains, domain users, and etc.  \nsharpview.exe located in the bin folder, an opensource red-team tool.\nUpon successful execution, cmd.exe will execute sharpview.exe \u003cmethod\u003e. Results will output via stdout.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "e44c2b24-e08a-48f6-b596-0c50c1fe88eb",
      "type": "execute-command",
      "data": {
        "command": "$syntaxList = #{syntax}\nforeach ($syntax in $syntaxList) {\n#{SharpView} $syntax -}\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "0bb50c5b-f1ea-480f-a580-432a58cc359e",
      "name": "Check dependency 1/1",
      "description": "Sharpview.exe must exist on disk at specified location (#{SharpView})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{SharpView}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "45d1ebec-5244-44b7-9b10-3621db0a35d1",
      "name": "Resolve dependency 1/1",
      "description": "Sharpview.exe must exist on disk at specified location (#{SharpView})\n",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Type Directory (split-path \"#{SharpView}\") -ErrorAction ignore | Out-Null\nInvoke-WebRequest #{SharpView_url} -OutFile \"#{SharpView}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "3f383656-85c8-465b-8ff8-280b4081517a",
      "name": "Re-check dependency 1/1",
      "description": "Sharpview.exe must exist on disk at specified location (#{SharpView})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{SharpView}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "0bb50c5b-f1ea-480f-a580-432a58cc359e",
      "p": "on-success",
      "o": "e44c2b24-e08a-48f6-b596-0c50c1fe88eb"
    },
    {
      "s": "0bb50c5b-f1ea-480f-a580-432a58cc359e",
      "p": "on-failure",
      "o": "45d1ebec-5244-44b7-9b10-3621db0a35d1"
    },
    {
      "s": "45d1ebec-5244-44b7-9b10-3621db0a35d1",
      "p": "on-success",
      "o": "3f383656-85c8-465b-8ff8-280b4081517a"
    },
    {
      "s": "3f383656-85c8-465b-8ff8-280b4081517a",
      "p": "on-failure",
      "o": "e44c2b24-e08a-48f6-b596-0c50c1fe88eb"
    }
  ],
  "tags": [
    "T1049",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "SharpView_url",
      "type": "url",
      "description": "sharpview download URL",
      "value": "https://github.com/tevora-threat/SharpView/blob/b60456286b41bb055ee7bc2a14d645410cca9b74/Compiled/SharpView.exe?raw=true"
    },
    {
      "name": "SharpView",
      "type": "path",
      "description": "Path of the executable opensource redteam tool used for the performing this atomic.",
      "value": "PathToAtomicsFolder\\..\\ExternalPayloads\\SharpView.exe"
    },
    {
      "name": "syntax",
      "type": "string",
      "description": "Arguements method used along with SharpView to get listing of network connections, domains, domain users, and etc.",
      "value": "\"Invoke-ACLScanner\", \"Invoke-Kerberoast\", \"Find-DomainShare\" \n"
    }
  ]
}