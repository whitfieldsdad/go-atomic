{
  "id": "eb0ba433-63e5-4a8c-a9f0-27c4192e1336",
  "name": "Enable Proxy Settings",
  "description": "A modification registry to enable proxy settings. This technique was seen in DarkGate malware as part of its installation.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "d6b373dd-2777-4870-ae0a-b297209db808",
      "type": "execute-command",
      "data": {
        "command": "reg add \"HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Internet Settings\" /v ProxyEnable /t REG_DWORD /d 1 /f\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "554ddde0-cd55-48bf-9d4b-aaa439a7bf4d",
      "type": "execute-command",
      "data": {
        "command": "reg add \"HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Internet Settings\" /v ProxyEnable /t REG_DWORD /d 0 /f\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "d6b373dd-2777-4870-ae0a-b297209db808",
      "p": "on-success",
      "o": "554ddde0-cd55-48bf-9d4b-aaa439a7bf4d"
    },
    {
      "s": "d6b373dd-2777-4870-ae0a-b297209db808",
      "p": "on-failure",
      "o": "554ddde0-cd55-48bf-9d4b-aaa439a7bf4d"
    }
  ],
  "tags": [
