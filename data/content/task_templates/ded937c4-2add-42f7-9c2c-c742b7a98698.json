{
  "id": "ded937c4-2add-42f7-9c2c-c742b7a98698",
  "name": "Delete an entire folder - Windows cmd",
  "description": "Recursively delete a folder in the temporary directory using cmd.exe.\nUpon execution, no output will be displayed. Use File Explorer to verify the folder was deleted.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "f70e65cd-38f6-4116-a2db-0481dbe2e020",
      "type": "execute-command",
      "data": {
        "command": "rmdir /s /q #{folder_to_delete}\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "fc8f6f7b-dd4f-4e73-b970-d7c94e913e6b",
      "name": "Check dependency 1/1",
      "description": "The file to delete must exist on disk at specified location (#{folder_to_delete})\n",
      "type": "execute-command",
      "data": {
        "command": "IF EXIST \"#{folder_to_delete}\" ( EXIT 0 ) ELSE ( EXIT 1 )\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "b07defe2-2a27-4098-abde-e7bf5f20cf93",
      "name": "Resolve dependency 1/1",
      "description": "The file to delete must exist on disk at specified location (#{folder_to_delete})\n",
      "type": "execute-command",
      "data": {
        "command": "mkdir #{folder_to_delete}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "6d623940-326e-4e30-93e2-38586ef20a24",
      "name": "Re-check dependency 1/1",
      "description": "The file to delete must exist on disk at specified location (#{folder_to_delete})\n",
      "type": "execute-command",
      "data": {
        "command": "IF EXIST \"#{folder_to_delete}\" ( EXIT 0 ) ELSE ( EXIT 1 )\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
