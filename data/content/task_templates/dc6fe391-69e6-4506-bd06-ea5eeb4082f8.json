{
  "id": "dc6fe391-69e6-4506-bd06-ea5eeb4082f8",
  "name": "Deobfuscate/Decode Files Or Information",
  "description": "Encode/Decode executable\nUpon execution a file named T1140_calc_decoded.exe will be placed in the temp folder\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "784c3dd9-b297-4306-9028-3cbff7ef3f52",
      "type": "execute-command",
      "data": {
        "command": "certutil -encode #{executable} %temp%\\T1140_calc.txt\ncertutil -decode %temp%\\T1140_calc.txt %temp%\\T1140_calc_decoded.exe\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "6f62a0d4-25c7-4f5c-9295-67950cb26e5d",
      "type": "execute-command",
      "data": {
        "command": "del %temp%\\T1140_calc.txt \u003enul 2\u003e\u00261\ndel %temp%\\T1140_calc_decoded.exe \u003enul 2\u003e\u00261\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "784c3dd9-b297-4306-9028-3cbff7ef3f52",
      "p": "on-success",
      "o": "6f62a0d4-25c7-4f5c-9295-67950cb26e5d"
    },
    {
      "s": "784c3dd9-b297-4306-9028-3cbff7ef3f52",
      "p": "on-failure",
      "o": "6f62a0d4-25c7-4f5c-9295-67950cb26e5d"
    }
  ],
  "tags": [
    "T1140",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "executable",
      "type": "path",
      "description": "name of executable",
      "value": "C:\\Windows\\System32\\calc.exe"
    }
  ]
}