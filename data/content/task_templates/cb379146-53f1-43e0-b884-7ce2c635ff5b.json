{
  "id": "cb379146-53f1-43e0-b884-7ce2c635ff5b",
  "name": "Automated Collection Command Prompt",
  "description": "Automated Collection. Upon execution, check the users temp directory (%temp%) for the folder T1119_command_prompt_collection\nto see what was collected.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "1b90b561-1ed0-462b-899b-a2c60841bc66",
      "type": "execute-command",
      "data": {
        "command": "mkdir %temp%\\T1119_command_prompt_collection \u003enul 2\u003e\u00261\ndir c: /b /s .docx | findstr /e .docx\nfor /R c:\\ %f in (*.docx) do copy /Y %f %temp%\\T1119_command_prompt_collection\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "3b06e528-e2a3-4408-a17d-608cbbcff095",
      "type": "execute-command",
      "data": {
        "command": "del %temp%\\T1119_command_prompt_collection /F /Q \u003enul 2\u003e\u00261\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "1b90b561-1ed0-462b-899b-a2c60841bc66",
      "p": "on-success",
      "o": "3b06e528-e2a3-4408-a17d-608cbbcff095"
    },
    {
      "s": "1b90b561-1ed0-462b-899b-a2c60841bc66",
      "p": "on-failure",
      "o": "3b06e528-e2a3-4408-a17d-608cbbcff095"
    }
  ],
  "tags": [
    "T1119"
  ]
}