{
  "id": "631ea661-d661-44b0-abdb-7a7f3fc08e50",
  "name": "Modify file timestamps using reference file",
  "description": "Modifies the `modify` and `access` timestamps using the timestamps of a specified reference file.\n\nThis technique was used by the threat actor Rocke during the compromise of Linux web servers.\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "f0e7eb45-be6d-4c7f-a09f-070dd5dd0a2c",
      "type": "execute-command",
      "data": {
        "command": "touch -acmr #{reference_file_path} #{target_file_path}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "e8fd0c3e-4049-4417-aebd-119887a9c3ee",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{target_file_path}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "826a50fc-72c7-49d6-bfec-d6d60e0e814a",
      "name": "Check dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{target_file_path} \u0026\u0026 exit 0 || exit 1",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "ec05436d-ecd0-45eb-956b-40fdce795dd6",
      "name": "Resolve dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "echo 'T1070.006 reference file timestomp test' \u003e #{target_file_path}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "ebcc130c-7516-446b-83a8-07fb99f8436b",
      "name": "Re-check dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{target_file_path} \u0026\u0026 exit 0 || exit 1",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "f0e7eb45-be6d-4c7f-a09f-070dd5dd0a2c",
      "p": "on-success",
      "o": "e8fd0c3e-4049-4417-aebd-119887a9c3ee"
    },
    {
      "s": "f0e7eb45-be6d-4c7f-a09f-070dd5dd0a2c",
      "p": "on-failure",
      "o": "e8fd0c3e-4049-4417-aebd-119887a9c3ee"
    },
    {
      "s": "826a50fc-72c7-49d6-bfec-d6d60e0e814a",
      "p": "on-success",
      "o": "f0e7eb45-be6d-4c7f-a09f-070dd5dd0a2c"
    },
    {
      "s": "826a50fc-72c7-49d6-bfec-d6d60e0e814a",
      "p": "on-failure",
      "o": "ec05436d-ecd0-45eb-956b-40fdce795dd6"
    },
    {
      "s": "ec05436d-ecd0-45eb-956b-40fdce795dd6",
      "p": "on-success",
      "o": "ebcc130c-7516-446b-83a8-07fb99f8436b"
    },
    {
      "s": "ebcc130c-7516-446b-83a8-07fb99f8436b",
      "p": "on-failure",
      "o": "f0e7eb45-be6d-4c7f-a09f-070dd5dd0a2c"
    }
  ],
  "tags": [
    "T1070",
    "T1070.006"
  ],
  "input_arguments": [
    {
      "name": "target_file_path",
      "type": "path",
      "description": "Path of file to modify timestamps of",
      "value": "/tmp/T1070.006-reference.txt"
    },
    {
      "name": "reference_file_path",
      "type": "path",
      "description": "Path of reference file to read timestamps from",
      "value": "/bin/sh"
    }
  ]
}