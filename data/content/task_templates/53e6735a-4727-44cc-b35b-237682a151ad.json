{
  "id": "53e6735a-4727-44cc-b35b-237682a151ad",
  "name": "Encrypt files using 7z (FreeBSD/Linux)",
  "description": "Uses 7z to encrypt a file\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "f5effae8-e84e-443a-869c-dc7c5a764b98",
      "type": "execute-command",
      "data": {
        "command": "$which_7z a -p#{pwd_for_encrypted_file} #{encrypted_file_path} #{input_file_path}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "abed054e-f1e2-4b35-94f5-1b5baeb1308f",
      "type": "execute-command",
      "data": {
        "command": "$which_7z e #{encrypted_file_path}\nrm #{encrypted_file_path}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "ae92fde8-63f3-4373-bac0-9d1ca9fc2ba6",
      "name": "Check dependency 1/1",
      "description": "Finds where 7z is located\n",
      "type": "execute-command",
      "data": {
        "command": "which_7z=`which 7z`\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "4ec90be0-890a-4617-abf3-275b23890e2b",
      "name": "Resolve dependency 1/1",
      "description": "Finds where 7z is located\n",
      "type": "execute-command",
      "data": {
        "command": "(which pkg \u0026\u0026 pkg install -y 7-zip)\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "ef7a9e4f-fc4c-40fa-ad40-39a9466415ff",
      "name": "Re-check dependency 1/1",
      "description": "Finds where 7z is located\n",
      "type": "execute-command",
      "data": {
        "command": "which_7z=`which 7z`\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
