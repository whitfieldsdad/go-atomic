{
  "id": "8a0b1579-5a36-483a-9cde-0236983e1665",
  "name": "Copy and Modify Mailbox Data on macOS",
  "description": "Copies and modifies mail data on macOS\n",
  "platforms": [
    "darwin"
  ],
  "steps": [
    {
      "id": "8fb1614e-30bc-4150-ae70-a67a5c2a4952",
      "type": "execute-command",
      "data": {
        "command": "mkdir ~/Library/Mail/copy\ncp -R ~/Library/Mail/* ~/Library/Mail/copy\necho \"Manipulated data\" \u003e ~/Library/Mail/copy/manipulated.txt\n",
        "command_type": "bash"
      },
      "elevation_required": true
    },
    {
      "id": "3b112bf5-1919-4e3a-a836-b6415c746ac0",
      "type": "execute-command",
      "data": {
        "command": "rm -rf ~/Library/Mail/copy\n",
        "command_type": "bash"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "8fb1614e-30bc-4150-ae70-a67a5c2a4952",
