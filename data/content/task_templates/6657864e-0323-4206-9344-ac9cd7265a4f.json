{
  "id": "6657864e-0323-4206-9344-ac9cd7265a4f",
  "name": "Create a new user in a command prompt",
  "description": "Creates a new user in a command prompt. Upon execution, \"The command completed successfully.\" will be displayed. To verify the\nnew account, run \"net user\" in powershell or CMD and observe that there is a new user named \"T1136.001_CMD\"\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "e1099ff0-e66b-4f61-ae06-0ad5ba280d15",
      "type": "execute-command",
      "data": {
        "command": "net user /add \"#{username}\" \"#{password}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    },
    {
      "id": "49d30998-46b5-4199-afe9-cbb0da9730e1",
      "type": "execute-command",
      "data": {
        "command": "net user /del \"#{username}\" \u003enul 2\u003e\u00261\n",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "e1099ff0-e66b-4f61-ae06-0ad5ba280d15",
      "p": "on-success",
      "o": "49d30998-46b5-4199-afe9-cbb0da9730e1"
    },
    {
      "s": "e1099ff0-e66b-4f61-ae06-0ad5ba280d15",
      "p": "on-failure",
      "o": "49d30998-46b5-4199-afe9-cbb0da9730e1"
    }
  ],
  "tags": [
    "T1136",
    "T1136.001"
  ],
  "input_arguments": [
    {
      "name": "username",
      "type": "string",
      "description": "Username of the user to create",
      "value": "T1136.001_CMD"
    },
    {
      "name": "password",
      "type": "string",
      "description": "Password of the user to create",
      "value": "T1136.001_CMD!"
    }
  ]
}