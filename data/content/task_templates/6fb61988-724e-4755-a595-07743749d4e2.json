    {
      "id": "eae2c06f-b114-4d94-af77-d46a799b5886",
      "type": "execute-command",
      "data": {
        "command": "launchctl remove #{label_name}",
        "command_type": "bash"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "72006d55-087e-4670-bcd9-66dcd98c9297",
