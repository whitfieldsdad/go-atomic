      "type": "execute-command",
      "data": {
        "command": "launchctl remove #{label_name}",
        "command_type": "bash"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "2c4d9e62-61c6-4717-8127-4f1210a99dcd",
