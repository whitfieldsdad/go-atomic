{
  "id": "d3812c4e-30ee-466a-a0aa-07e355b561d6",
  "name": "Delete User via sysadminctl utility",
  "description": "This test deletes the user account using the sysadminctl utility.\n",
  "platforms": [
    "darwin"
  ],
  "steps": [
    {
      "id": "68ccf8f9-9095-40ca-a8c2-f3cc954686d4",
      "type": "execute-command",
      "data": {
        "command": "sysadminctl -deleteUser #{user_account} #enter admin password\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "a6af4fad-4244-451c-8b7e-3afa9f883537",
      "type": "execute-command",
      "data": {
        "command": "sysadminctl -addUser #{user_account} -fullName \"#{user_name}\" -password #{user_password}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "68ccf8f9-9095-40ca-a8c2-f3cc954686d4",
      "p": "on-success",
      "o": "a6af4fad-4244-451c-8b7e-3afa9f883537"
    },
    {
      "s": "68ccf8f9-9095-40ca-a8c2-f3cc954686d4",
      "p": "on-failure",
      "o": "a6af4fad-4244-451c-8b7e-3afa9f883537"
    }
  ],
  "tags": [
    "T1531",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "user_account",
      "type": "string",
      "description": "User account which will be deleted.",
      "value": "ARTUserAccount"
    },
    {
      "name": "user_name",
      "type": "string",
      "description": "New user name.",
      "value": "ARTUser"
    },
    {
      "name": "user_password",
      "type": "string",
      "description": "New user password.",
      "value": "ARTPassword"
    }
  ]
}