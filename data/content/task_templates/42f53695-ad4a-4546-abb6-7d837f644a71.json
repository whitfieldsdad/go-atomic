{
  "id": "42f53695-ad4a-4546-abb6-7d837f644a71",
  "name": "Scheduled task Local",
  "description": "Upon successful execution, cmd.exe will create a scheduled task to spawn cmd.exe at 20:10.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "f318c33c-3811-47df-9522-d2bb14477b8c",
      "type": "execute-command",
      "data": {
        "command": "SCHTASKS /Create /SC ONCE /TN spawn /TR #{task_command} /ST #{time}\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "9379bd93-f863-4468-b13f-7fe0a4626a76",
      "type": "execute-command",
      "data": {
        "command": "SCHTASKS /Delete /TN spawn /F \u003enul 2\u003e\u00261\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "f318c33c-3811-47df-9522-d2bb14477b8c",
      "p": "on-success",
      "o": "9379bd93-f863-4468-b13f-7fe0a4626a76"
    },
    {
      "s": "f318c33c-3811-47df-9522-d2bb14477b8c",
      "p": "on-failure",
      "o": "9379bd93-f863-4468-b13f-7fe0a4626a76"
    }
  ],
  "tags": [
    "T1053.005",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "time",
      "type": "string",
      "description": "What time 24 Hour",
      "value": "20:10"
    },
    {
      "name": "task_command",
      "type": "string",
      "description": "What you want to execute",
      "value": "C:\\windows\\system32\\cmd.exe"
    }
  ]
}