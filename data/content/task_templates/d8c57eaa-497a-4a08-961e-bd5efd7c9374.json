{
  "id": "d8c57eaa-497a-4a08-961e-bd5efd7c9374",
  "name": "LockBit Black - Disable Privacy Settings Experience Using Registry -Powershell",
  "description": "LockBit Black - Disable Privacy Settings Experience Using Registry\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "1b9a3ed0-77a3-4c4e-905a-cc2e197da33d",
      "type": "execute-command",
      "data": {
        "command": "New-ItemProperty \"HKCU:\\Software\\Policies\\Microsoft\\Windows\\OOBE\" -Name DisablePrivacyExperience -PropertyType DWord -Value 1 -Force\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "f19ae80c-6b80-4bca-b72f-fc1fd07dc66b",
      "type": "execute-command",
      "data": {
        "command": "Remove-ItemProperty \"HKCU:\\Software\\Policies\\Microsoft\\Windows\\OOBE\" -Name DisablePrivacyExperience -Force -ErrorAction Ignore\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "1b9a3ed0-77a3-4c4e-905a-cc2e197da33d",
      "p": "on-success",
      "o": "f19ae80c-6b80-4bca-b72f-fc1fd07dc66b"
    },
    {
