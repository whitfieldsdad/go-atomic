{
  "id": "9fd5a74b-ba89-482a-8a3e-a5feaa3697b0",
  "name": "Rundll32 with Ordinal Value",
  "description": "Rundll32.exe loading dll using ordinal value #2 to DLLRegisterServer. \nUpon successful execution, Calc.exe will spawn.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "ce01a0d9-6999-422d-8402-72694821eb22",
      "type": "execute-command",
      "data": {
        "command": "rundll32.exe \"#{input_file}\",#2\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "bbecbf11-3d16-450c-9e41-fa91b72963d1",
      "name": "Check dependency 1/1",
      "description": "DLL file must exist on disk at specified location\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{input_file}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "07c11ebb-a1c9-4d39-a0fe-adf1a819af0a",
      "name": "Resolve dependency 1/1",
      "description": "DLL file must exist on disk at specified location\n",
      "type": "execute-command",
      "data": {
        "command": "Invoke-WebRequest \"#{input_url}\" -OutFile \"#{input_file}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "ff4cd391-233f-42a8-879f-10c81397fdd9",
      "name": "Re-check dependency 1/1",
      "description": "DLL file must exist on disk at specified location\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{input_file}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "bbecbf11-3d16-450c-9e41-fa91b72963d1",
      "p": "on-success",
      "o": "ce01a0d9-6999-422d-8402-72694821eb22"
    },
    {
      "s": "bbecbf11-3d16-450c-9e41-fa91b72963d1",
      "p": "on-failure",
      "o": "07c11ebb-a1c9-4d39-a0fe-adf1a819af0a"
    },
    {
      "s": "07c11ebb-a1c9-4d39-a0fe-adf1a819af0a",
      "p": "on-success",
      "o": "ff4cd391-233f-42a8-879f-10c81397fdd9"
    },
    {
      "s": "ff4cd391-233f-42a8-879f-10c81397fdd9",
      "p": "on-failure",
      "o": "ce01a0d9-6999-422d-8402-72694821eb22"
    }
  ],
  "tags": [
    "T1218.011",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "input_url",
      "type": "url",
      "description": "Url to download the DLL",
      "value": "https://github.com/redcanaryco/atomic-red-team/blob/master/atomics/T1218.010/bin/AllTheThingsx64.dll"
    },
    {
      "name": "input_file",
      "type": "string",
      "description": "DLL File",
      "value": "PathToAtomicsFolder\\T1218.010\\bin\\AllTheThingsx64.dll"
    }
  ]
