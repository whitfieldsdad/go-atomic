{
  "id": "40d8eabd-e394-46f6-8785-b9bfa1d011d2",
  "name": "Create a user account on a Linux system",
  "description": "Create a user via useradd\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "08e33d5b-009b-4343-9da9-d13b265c59a1",
      "type": "execute-command",
      "data": {
        "command": "useradd -M -N -r -s /bin/bash -c evil_account #{username}\n",
        "command_type": "bash"
      },
      "elevation_required": true
    },
    {
      "id": "21ab9ec9-c466-4922-a9e7-e4e44bf1232d",
      "type": "execute-command",
      "data": {
        "command": "userdel #{username}\n",
        "command_type": "bash"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "08e33d5b-009b-4343-9da9-d13b265c59a1",
      "p": "on-success",
      "o": "21ab9ec9-c466-4922-a9e7-e4e44bf1232d"
    },
    {
      "s": "08e33d5b-009b-4343-9da9-d13b265c59a1",
      "p": "on-failure",
      "o": "21ab9ec9-c466-4922-a9e7-e4e44bf1232d"
    }
  ],
  "tags": [
    "T1136",
    "T1136.001"
  ],
  "input_arguments": [
    {
      "name": "username",
      "type": "string",
      "description": "Username of the user to create",
      "value": "evil_user"
    }
  ]
}