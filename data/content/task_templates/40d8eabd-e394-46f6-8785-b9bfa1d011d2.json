{
  "id": "40d8eabd-e394-46f6-8785-b9bfa1d011d2",
  "name": "Create a user account on a Linux system",
  "description": "Create a user via useradd\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "2d43ec98-3355-453e-a411-f67496c8726f",
      "type": "execute-command",
      "data": {
        "command": "useradd -M -N -r -s /bin/bash -c evil_account #{username}",
        "command_type": "bash"
      },
      "elevation_required": true
    },
    {
      "id": "fea0d2e4-2b1e-4785-b046-a6498e117a7a",
      "type": "execute-command",
      "data": {
        "command": "userdel #{username}",
        "command_type": "bash"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "2d43ec98-3355-453e-a411-f67496c8726f",
      "p": "on-success",
      "o": "fea0d2e4-2b1e-4785-b046-a6498e117a7a"
    },
    {
      "s": "2d43ec98-3355-453e-a411-f67496c8726f",
      "p": "on-failure",
      "o": "fea0d2e4-2b1e-4785-b046-a6498e117a7a"
    }
  ],
  "tags": [
    "T1136",
    "T1136.001"
  ],
  "input_arguments": [
    {
      "name": "username",
      "type": "string",
      "description": "Username of the user to create",
      "value": "evil_user"
    }
  ]
}