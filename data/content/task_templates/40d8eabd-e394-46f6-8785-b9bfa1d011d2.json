{
  "id": "40d8eabd-e394-46f6-8785-b9bfa1d011d2",
  "name": "Create a user account on a Linux system",
  "description": "Create a user via useradd\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "239179d6-2abb-4750-8980-2c17cb2ec3d9",
      "type": "execute-command",
      "data": {
        "command": "useradd -M -N -r -s /bin/bash -c evil_account #{username}",
        "command_type": "bash"
      },
      "elevation_required": true
    },
    {
      "id": "13a2e505-9ac0-461c-80c3-cbc93bf37a52",
      "type": "execute-command",
      "data": {
        "command": "userdel #{username}",
        "command_type": "bash"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "239179d6-2abb-4750-8980-2c17cb2ec3d9",
      "p": "on-success",
      "o": "13a2e505-9ac0-461c-80c3-cbc93bf37a52"
    },
    {
      "s": "239179d6-2abb-4750-8980-2c17cb2ec3d9",
      "p": "on-failure",
      "o": "13a2e505-9ac0-461c-80c3-cbc93bf37a52"
    }
  ],
  "tags": [
    "T1136",
    "T1136.001"
  ],
  "input_arguments": [
    {
      "name": "username",
      "type": "string",
      "description": "Username of the user to create",
      "value": "evil_user"
    }
  ]
}