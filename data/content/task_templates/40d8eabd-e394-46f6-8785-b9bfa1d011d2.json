{
  "id": "40d8eabd-e394-46f6-8785-b9bfa1d011d2",
  "name": "Create a user account on a Linux system",
  "description": "Create a user via useradd\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "1de1eb0b-1d3f-4ea5-a933-26f0394ff885",
      "type": "execute-command",
      "data": {
        "command": "useradd -M -N -r -s /bin/bash -c evil_account #{username}\n",
        "command_type": "bash"
      },
      "elevation_required": false
    },
    {
      "id": "c32ee057-001d-4a6c-9451-315244e033a7",
      "type": "execute-command",
      "data": {
        "command": "userdel #{username}\n",
        "command_type": "bash"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "1de1eb0b-1d3f-4ea5-a933-26f0394ff885",
      "p": "on-success",
      "o": "c32ee057-001d-4a6c-9451-315244e033a7"
    },
    {
      "s": "1de1eb0b-1d3f-4ea5-a933-26f0394ff885",
      "p": "on-failure",
      "o": "c32ee057-001d-4a6c-9451-315244e033a7"
    }
  ],
  "tags": [
    "T1136.001",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "username",
      "type": "string",
      "description": "Username of the user to create",
      "value": "evil_user"
    }
  ]
}