      },
      "elevation_required": false
    },
    {
      "id": "69f00c8a-3d47-4760-a43e-587d52773435",
      "type": "execute-command",
      "data": {
        "command": "Stop-Process -Name \"Ammyy\" -force -erroraction silentlycontinue\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "3953472f-943f-4899-99c3-5a35e0c5d537",
