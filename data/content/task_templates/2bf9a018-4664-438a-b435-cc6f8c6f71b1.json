  "name": "Sudo usage (freebsd)",
  "description": "Common Sudo enumeration methods.\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "7947ee81-5fb3-4526-8435-295187a31987",
      "type": "execute-command",
      "data": {
        "command": "sudo -l      \nsudo cat /usr/local/etc/sudoers\nsudo ee /usr/local/etc/sudoers\n",
        "command_type": "sh"
      },
      "elevation_required": true
    },
    {
      "id": "24a31f94-8af3-4d98-8fec-a5ec00b428ad",
      "name": "Check dependency 1/1",
      "description": "Check if sudo is installed.\n",
      "type": "execute-command",
      "data": {
        "command": "if [ ! -x \"$(command -v sudo)\" ]; then exit 1; else exit 0; fi;\n",
        "command_type": ""
      },
      "elevation_required": true
    },
    {
      "id": "55186700-0ee2-4fd2-baa6-30336faaa4e2",
      "name": "Resolve dependency 1/1",
      "description": "Check if sudo is installed.\n",
      "type": "execute-command",
      "data": {
        "command": "(which pkg \u0026\u0026 pkg install -y sudo)\n",
        "command_type": ""
      },
      "elevation_required": true
    },
    {
      "id": "a9153c6a-c65b-4b81-a00d-93f9ded4e137",
      "name": "Re-check dependency 1/1",
      "description": "Check if sudo is installed.\n",
