{
  "id": "127b4afe-2346-4192-815c-69042bec570e",
  "name": "Writes text to a file and displays it.",
  "description": "Writes text to a file and display the results. This test is intended to emulate the dropping of a malicious file to disk.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "c51740f9-cc37-4987-95a8-f1c047a8bc08",
      "type": "execute-command",
      "data": {
        "command": "echo \"#{message}\" \u003e \"#{file_contents_path}\" \u0026 type \"#{file_contents_path}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "ced87423-be38-4879-8cd4-d6d96466e896",
      "type": "execute-command",
      "data": {
        "command": "del \"#{file_contents_path}\" \u003enul 2\u003e\u00261\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "c51740f9-cc37-4987-95a8-f1c047a8bc08",
      "p": "on-success",
      "o": "ced87423-be38-4879-8cd4-d6d96466e896"
    },
    {
      "s": "c51740f9-cc37-4987-95a8-f1c047a8bc08",
      "p": "on-failure",
      "o": "ced87423-be38-4879-8cd4-d6d96466e896"
    }
  ],
  "tags": [
    "T1059",
    "T1059.003"
  ],
  "input_arguments": [
    {
      "name": "file_contents_path",
      "type": "path",
      "description": "Path to the file that the command prompt will drop.",
      "value": "%TEMP%\\test.bin"
    },
    {
      "name": "message",
      "type": "string",
      "description": "Message that will be written to disk and then displayed.",
      "value": "Hello from the Windows Command Prompt!"
    }
  ]
}