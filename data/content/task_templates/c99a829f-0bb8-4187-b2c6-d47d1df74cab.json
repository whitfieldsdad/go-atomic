{
  "id": "c99a829f-0bb8-4187-b2c6-d47d1df74cab",
  "name": "whois file download",
  "description": "Download a remote file using the whois utility\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "776a70a7-b215-45f8-a071-e1a26c9bfac1",
      "type": "execute-command",
      "data": {
        "command": "timeout --preserve-status #{timeout} whois -h #{remote_host} -p #{remote_port} \"#{query}\" \u003e #{output_file}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "fc1f0a65-b813-4ff7-bb25-4a293873434b",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{output_file}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "cf166797-bf52-410d-86ce-0f545db00b03",
      "name": "Check dependency 1/1",
      "description": "The whois and timeout commands must be present\n",
      "type": "execute-command",
      "data": {
        "command": "which whois \u0026\u0026 which timeout",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "25c36b1a-ac9b-4f04-8267-d37bb16798c7",
      "name": "Resolve dependency 1/1",
      "description": "The whois and timeout commands must be present\n",
      "type": "execute-command",
      "data": {
        "command": "echo \"Please install timeout and the whois package\"",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "754921fb-5ac4-4e28-8264-66eac8448158",
      "name": "Re-check dependency 1/1",
      "description": "The whois and timeout commands must be present\n",
      "type": "execute-command",
      "data": {
        "command": "which whois \u0026\u0026 which timeout",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "776a70a7-b215-45f8-a071-e1a26c9bfac1",
      "p": "on-success",
      "o": "fc1f0a65-b813-4ff7-bb25-4a293873434b"
    },
    {
      "s": "776a70a7-b215-45f8-a071-e1a26c9bfac1",
      "p": "on-failure",
      "o": "fc1f0a65-b813-4ff7-bb25-4a293873434b"
    },
    {
      "s": "cf166797-bf52-410d-86ce-0f545db00b03",
      "p": "on-success",
      "o": "776a70a7-b215-45f8-a071-e1a26c9bfac1"
    },
    {
      "s": "cf166797-bf52-410d-86ce-0f545db00b03",
      "p": "on-failure",
      "o": "25c36b1a-ac9b-4f04-8267-d37bb16798c7"
    },
    {
      "s": "25c36b1a-ac9b-4f04-8267-d37bb16798c7",
      "p": "on-success",
      "o": "754921fb-5ac4-4e28-8264-66eac8448158"
    },
    {
      "s": "754921fb-5ac4-4e28-8264-66eac8448158",
      "p": "on-failure",
      "o": "776a70a7-b215-45f8-a071-e1a26c9bfac1"
    }
  ],
  "tags": [
    "T1105"
  ],
  "input_arguments": [
    {
      "name": "remote_port",
      "type": "integer",
      "description": "Remote port to connect to",
      "value": "8443"
    },
    {
      "name": "output_file",
      "type": "path",
      "description": "Path of file to save output to",
      "value": "/tmp/T1105.whois.out"
    },
    {
      "name": "query",
      "type": "string",
      "description": "Query to send to remote server",
      "value": "Hello from Atomic Red Team test T1105"
    },
    {
      "name": "timeout",
      "type": "integer",
      "description": "Timeout period before ending process (seconds)",
      "value": "1"
    },
    {
      "name": "remote_host",
      "type": "string",
      "description": "Remote hostname or IP address",
      "value": "localhost"
    }
  ]
}