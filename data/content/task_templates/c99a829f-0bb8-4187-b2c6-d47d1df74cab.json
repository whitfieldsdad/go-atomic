{
  "id": "c99a829f-0bb8-4187-b2c6-d47d1df74cab",
  "name": "whois file download",
  "description": "Download a remote file using the whois utility\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "a0febbc4-37e5-436f-9ec5-00e772ba3c7b",
      "type": "execute-command",
      "data": {
        "command": "timeout --preserve-status #{timeout} whois -h #{remote_host} -p #{remote_port} \"#{query}\" \u003e #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "1553a6de-b493-4abf-a2b7-d4ac3e744772",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "9559691c-d4bf-4b71-82dd-6e871c3f8b05",
      "name": "Check dependency 1/1",
      "description": "The whois and timeout commands must be present\n",
      "type": "execute-command",
      "data": {
        "command": "which whois \u0026\u0026 which timeout\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "65e4b753-aab8-4992-9944-fed7cd067436",
      "name": "Resolve dependency 1/1",
      "description": "The whois and timeout commands must be present\n",
      "type": "execute-command",
      "data": {
        "command": "echo \"Please install timeout and the whois package\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "74acc4c6-e833-4b34-8a92-941eacd6b40b",
      "name": "Re-check dependency 1/1",
      "description": "The whois and timeout commands must be present\n",
      "type": "execute-command",
      "data": {
        "command": "which whois \u0026\u0026 which timeout\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "a0febbc4-37e5-436f-9ec5-00e772ba3c7b",
      "p": "on-success",
      "o": "1553a6de-b493-4abf-a2b7-d4ac3e744772"
    },
    {
      "s": "a0febbc4-37e5-436f-9ec5-00e772ba3c7b",
      "p": "on-failure",
      "o": "1553a6de-b493-4abf-a2b7-d4ac3e744772"
    },
    {
      "s": "9559691c-d4bf-4b71-82dd-6e871c3f8b05",
      "p": "on-success",
      "o": "a0febbc4-37e5-436f-9ec5-00e772ba3c7b"
    },
    {
      "s": "9559691c-d4bf-4b71-82dd-6e871c3f8b05",
      "p": "on-failure",
      "o": "65e4b753-aab8-4992-9944-fed7cd067436"
    },
    {
      "s": "65e4b753-aab8-4992-9944-fed7cd067436",
      "p": "on-success",
      "o": "74acc4c6-e833-4b34-8a92-941eacd6b40b"
    },
    {
      "s": "74acc4c6-e833-4b34-8a92-941eacd6b40b",
      "p": "on-failure",
      "o": "a0febbc4-37e5-436f-9ec5-00e772ba3c7b"
    }
  ],
  "tags": [
    "T1105"
  ],
  "input_arguments": [
    {
      "name": "remote_port",
      "type": "integer",
      "description": "Remote port to connect to",
      "value": "8443"
    },
    {
      "name": "output_file",
      "type": "path",
      "description": "Path of file to save output to",
      "value": "/tmp/T1105.whois.out"
    },
    {
      "name": "query",
      "type": "string",
      "description": "Query to send to remote server",
      "value": "Hello from Atomic Red Team test T1105"
    },
    {
      "name": "timeout",
      "type": "integer",
      "description": "Timeout period before ending process (seconds)",
      "value": "1"
    },
    {
      "name": "remote_host",
      "type": "string",
      "description": "Remote hostname or IP address",
      "value": "localhost"
    }
  ]
}