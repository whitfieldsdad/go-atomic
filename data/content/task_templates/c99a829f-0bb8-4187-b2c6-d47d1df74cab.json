{
  "id": "c99a829f-0bb8-4187-b2c6-d47d1df74cab",
  "name": "whois file download",
  "description": "Download a remote file using the whois utility\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "fdef5403-146e-49a6-b553-0ee2f7c32760",
      "type": "execute-command",
      "data": {
        "command": "timeout --preserve-status #{timeout} whois -h #{remote_host} -p #{remote_port} \"#{query}\" \u003e #{output_file}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "bab8b235-4d3b-40a7-b477-0dc9ba1e680d",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{output_file}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "add33eaa-9dee-48c8-aa1c-e1c2a0c01dca",
      "name": "Check dependency 1/1",
      "description": "The whois and timeout commands must be present\n",
      "type": "execute-command",
      "data": {
        "command": "which whois \u0026\u0026 which timeout",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "56c057c0-297b-4a9c-add3-114eead9f736",
      "name": "Resolve dependency 1/1",
      "description": "The whois and timeout commands must be present\n",
      "type": "execute-command",
      "data": {
        "command": "echo \"Please install timeout and the whois package\"",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "a0d98a42-9522-4771-ad7b-ed1cd9ee7788",
      "name": "Re-check dependency 1/1",
      "description": "The whois and timeout commands must be present\n",
      "type": "execute-command",
      "data": {
        "command": "which whois \u0026\u0026 which timeout",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "fdef5403-146e-49a6-b553-0ee2f7c32760",
      "p": "on-success",
      "o": "bab8b235-4d3b-40a7-b477-0dc9ba1e680d"
    },
    {
      "s": "fdef5403-146e-49a6-b553-0ee2f7c32760",
      "p": "on-failure",
      "o": "bab8b235-4d3b-40a7-b477-0dc9ba1e680d"
    },
    {
      "s": "add33eaa-9dee-48c8-aa1c-e1c2a0c01dca",
      "p": "on-success",
      "o": "fdef5403-146e-49a6-b553-0ee2f7c32760"
    },
    {
      "s": "add33eaa-9dee-48c8-aa1c-e1c2a0c01dca",
      "p": "on-failure",
      "o": "56c057c0-297b-4a9c-add3-114eead9f736"
    },
    {
      "s": "56c057c0-297b-4a9c-add3-114eead9f736",
      "p": "on-success",
      "o": "a0d98a42-9522-4771-ad7b-ed1cd9ee7788"
    },
    {
      "s": "a0d98a42-9522-4771-ad7b-ed1cd9ee7788",
      "p": "on-failure",
      "o": "fdef5403-146e-49a6-b553-0ee2f7c32760"
    }
  ],
  "tags": [
    "T1105"
  ],
  "input_arguments": [
    {
      "name": "query",
      "type": "string",
      "description": "Query to send to remote server",
      "value": "Hello from Atomic Red Team test T1105"
    },
    {
      "name": "timeout",
      "type": "integer",
      "description": "Timeout period before ending process (seconds)",
      "value": "1"
    },
    {
      "name": "remote_host",
      "type": "string",
      "description": "Remote hostname or IP address",
      "value": "localhost"
    },
    {
      "name": "remote_port",
      "type": "integer",
      "description": "Remote port to connect to",
      "value": "8443"
    },
    {
      "name": "output_file",
      "type": "path",
      "description": "Path of file to save output to",
      "value": "/tmp/T1105.whois.out"
    }
  ]
}