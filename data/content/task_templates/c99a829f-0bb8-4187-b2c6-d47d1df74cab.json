{
  "id": "c99a829f-0bb8-4187-b2c6-d47d1df74cab",
  "name": "whois file download",
  "description": "Download a remote file using the whois utility\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "f4973eaf-2911-4549-b743-bf529fef32c9",
      "type": "execute-command",
      "data": {
        "command": "timeout --preserve-status #{timeout} whois -h #{remote_host} -p #{remote_port} \"#{query}\" \u003e #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "46498c5b-1e4d-4aca-a452-b5bf490ddfcc",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "71e8c5e0-c595-4a18-a27d-13f4c709ce1f",
      "name": "Check dependency 1/1",
      "description": "The whois and timeout commands must be present\n",
      "type": "execute-command",
      "data": {
        "command": "which whois \u0026\u0026 which timeout\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "78c856ee-1540-4c33-9231-fedab705dee8",
      "name": "Resolve dependency 1/1",
      "description": "The whois and timeout commands must be present\n",
      "type": "execute-command",
      "data": {
        "command": "echo \"Please install timeout and the whois package\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "add21651-2bde-4e59-9ff8-c9f30e06027c",
      "name": "Re-check dependency 1/1",
      "description": "The whois and timeout commands must be present\n",
      "type": "execute-command",
      "data": {
        "command": "which whois \u0026\u0026 which timeout\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "f4973eaf-2911-4549-b743-bf529fef32c9",
      "p": "on-success",
      "o": "46498c5b-1e4d-4aca-a452-b5bf490ddfcc"
    },
    {
      "s": "f4973eaf-2911-4549-b743-bf529fef32c9",
      "p": "on-failure",
      "o": "46498c5b-1e4d-4aca-a452-b5bf490ddfcc"
    },
    {
      "s": "71e8c5e0-c595-4a18-a27d-13f4c709ce1f",
      "p": "on-success",
      "o": "f4973eaf-2911-4549-b743-bf529fef32c9"
    },
    {
      "s": "71e8c5e0-c595-4a18-a27d-13f4c709ce1f",
      "p": "on-failure",
      "o": "78c856ee-1540-4c33-9231-fedab705dee8"
    },
    {
      "s": "78c856ee-1540-4c33-9231-fedab705dee8",
      "p": "on-success",
      "o": "add21651-2bde-4e59-9ff8-c9f30e06027c"
    },
