{
  "id": "1ca1f9c7-44bc-46bb-8c85-c50e2e94267b",
  "name": "List Mozilla Firefox Bookmark Database Files on macOS",
  "description": "Searches for Mozilla Firefox's places.sqlite file (on macOS) that contains bookmarks and lists any found instances to a text file.\n",
  "platforms": [
    "darwin"
  ],
  "steps": [
    {
      "id": "a69856bd-9993-4277-a682-45bfbf59559b",
      "type": "execute-command",
      "data": {
        "command": "find / -path \"*/Firefox/Profiles/*/places.sqlite\" -exec echo {} \u003e\u003e #{output_file} \\;\ncat #{output_file} 2\u003e/dev/null\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "b042e1cf-c4a5-4981-a12a-3f7fcc17b442",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{output_file} 2\u003e/dev/null\n",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "a69856bd-9993-4277-a682-45bfbf59559b",
      "p": "on-success",
      "o": "b042e1cf-c4a5-4981-a12a-3f7fcc17b442"
    },
    {
      "s": "a69856bd-9993-4277-a682-45bfbf59559b",
      "p": "on-failure",
      "o": "b042e1cf-c4a5-4981-a12a-3f7fcc17b442"
    }
  ],
  "tags": [
    "T1217",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "output_file",
      "type": "path",
      "description": "Path where captured results will be placed.",
      "value": "/tmp/T1217_Firefox.txt"
    }
  ]
}