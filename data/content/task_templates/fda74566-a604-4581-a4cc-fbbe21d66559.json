{
  "id": "fda74566-a604-4581-a4cc-fbbe21d66559",
  "name": "Create a new Windows admin user",
  "description": "Creates a new admin user in a command prompt.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "725d8282-7087-463e-9c08-911773a8b714",
      "type": "execute-command",
      "data": {
        "command": "net user /add \"#{username}\" \"#{password}\"\nnet localgroup administrators \"#{username}\" /add",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    },
    {
      "id": "622efd70-6ca0-4c48-a3b8-196975e28473",
      "type": "execute-command",
      "data": {
        "command": "net user /del \"#{username}\" \u003enul 2\u003e\u00261",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "725d8282-7087-463e-9c08-911773a8b714",
      "p": "on-success",
      "o": "622efd70-6ca0-4c48-a3b8-196975e28473"
    },
    {
      "s": "725d8282-7087-463e-9c08-911773a8b714",
      "p": "on-failure",
      "o": "622efd70-6ca0-4c48-a3b8-196975e28473"
    }
  ],
  "tags": [
    "T1136",
    "T1136.001"
  ],
  "input_arguments": [
    {
      "name": "username",
      "type": "string",
      "description": "Username of the user to create",
      "value": "T1136.001_Admin"
    },
    {
      "name": "password",
      "type": "string",
      "description": "Password of the user to create",
      "value": "T1136_pass"
    }
  ]
}