{
  "id": "fda74566-a604-4581-a4cc-fbbe21d66559",
  "name": "Create a new Windows admin user",
  "description": "Creates a new admin user in a command prompt.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "b93c1b73-5aa2-41e7-9de3-b36f4c423fa7",
      "type": "execute-command",
      "data": {
        "command": "net user /add \"#{username}\" \"#{password}\"\nnet localgroup administrators \"#{username}\" /add\n",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    },
    {
      "id": "88e4fb40-5a81-4228-a6e9-934edf903fdf",
      "type": "execute-command",
      "data": {
        "command": "net user /del \"#{username}\" \u003enul 2\u003e\u00261\n",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "b93c1b73-5aa2-41e7-9de3-b36f4c423fa7",
      "p": "on-success",
      "o": "88e4fb40-5a81-4228-a6e9-934edf903fdf"
    },
    {
      "s": "b93c1b73-5aa2-41e7-9de3-b36f4c423fa7",
      "p": "on-failure",
      "o": "88e4fb40-5a81-4228-a6e9-934edf903fdf"
    }
  ],
  "tags": [
    "T1136",
    "T1136.001"
  ],
  "input_arguments": [
    {
      "name": "username",
      "type": "string",
      "description": "Username of the user to create",
      "value": "T1136.001_Admin"
    },
    {
      "name": "password",
      "type": "string",
      "description": "Password of the user to create",
      "value": "T1136_pass"
    }
  ]
}