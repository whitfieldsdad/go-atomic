{
  "id": "ed3fa08a-ca18-4009-973e-03d13014d0e8",
  "name": "Msiexec.exe - Execute Local MSI file with an embedded EXE",
  "description": "Executes an MSI containing an embedded EXE using msiexec.exe\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "55a7ce54-7540-4ed3-99a2-c673593fc1f8",
      "type": "execute-command",
      "data": {
        "command": "#{msi_exe} /q /#{action} \"#{msi_payload}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "e5388274-1a67-472e-a564-a4ba4f1dc6c4",
      "name": "Check dependency 1/1",
      "description": "The MSI file must exist on disk at specified location (#{msi_payload})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{msi_payload}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "1cf6f138-2cf3-465c-8897-47fb3476e8c6",
      "name": "Resolve dependency 1/1",
      "description": "The MSI file must exist on disk at specified location (#{msi_payload})\n",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Type Directory (split-path \"#{msi_payload}\") -ErrorAction ignore | Out-Null\nInvoke-WebRequest \"https://github.com/redcanaryco/atomic-red-team/raw/master/atomics/T1218.007/bin/T1218.007_EXE.msi\" -OutFile \"#{msi_payload}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "904578c6-a2b8-4b28-8a2f-4c31c8142c30",
      "name": "Re-check dependency 1/1",
      "description": "The MSI file must exist on disk at specified location (#{msi_payload})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{msi_payload}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "e5388274-1a67-472e-a564-a4ba4f1dc6c4",
      "p": "on-success",
      "o": "55a7ce54-7540-4ed3-99a2-c673593fc1f8"
    },
    {
      "s": "e5388274-1a67-472e-a564-a4ba4f1dc6c4",
      "p": "on-failure",
      "o": "1cf6f138-2cf3-465c-8897-47fb3476e8c6"
    },
    {
      "s": "1cf6f138-2cf3-465c-8897-47fb3476e8c6",
      "p": "on-success",
      "o": "904578c6-a2b8-4b28-8a2f-4c31c8142c30"
    },
    {
      "s": "904578c6-a2b8-4b28-8a2f-4c31c8142c30",
      "p": "on-failure",
      "o": "55a7ce54-7540-4ed3-99a2-c673593fc1f8"
    }
  ],
  "tags": [
    "T1218.007",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "msi_payload",
      "type": "path",
      "description": "MSI file to execute",
      "value": "PathToAtomicsFolder\\T1218.007\\bin\\T1218.007_EXE.msi"
    },
    {
      "name": "msi_exe",
      "type": "path",
      "description": "MSIExec File Path",
      "value": "c:\\windows\\system32\\msiexec.exe"
    },
    {
      "name": "action",
      "type": "string",
      "description": "Specifies the MSI action to perform: i (install), a (admin), j (advertise). The included MSI is designed to support all three action types.\n",
      "value": "i"
    }
  ]
}