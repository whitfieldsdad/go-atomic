      "id": "27a303b1-4d78-4043-a07c-7febe70828f4",
      "type": "execute-command",
      "data": {
        "command": "adidnsdump -u #{user_name} -p #{acct_pass} --print-zones #{host_name}",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    },
