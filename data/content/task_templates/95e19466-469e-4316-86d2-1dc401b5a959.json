      "id": "5dad66cf-9dc9-4109-8af9-920c7ec95560",
      "type": "execute-command",
      "data": {
        "command": "adidnsdump -u #{user_name} -p #{acct_pass} --print-zones #{host_name}\n",
        "command_type": "command_prompt"
      },
      "elevation_required": true
