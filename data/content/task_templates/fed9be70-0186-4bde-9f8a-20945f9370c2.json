{
  "id": "fed9be70-0186-4bde-9f8a-20945f9370c2",
  "name": "View sudoers access",
  "description": "(requires root)\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "dfcf34be-1086-482b-89ca-ef837527f85c",
      "type": "execute-command",
      "data": {
        "command": "if [ -f /etc/sudoers ]; then sudo cat /etc/sudoers \u003e #{output_file}; fi;\nif [ -f /usr/local/etc/sudoers ]; then sudo cat /usr/local/etc/sudoers \u003e #{output_file}; fi;\ncat #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "9e12ed02-531b-4233-a40d-079d8ee99649",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "dfcf34be-1086-482b-89ca-ef837527f85c",
      "p": "on-success",
      "o": "9e12ed02-531b-4233-a40d-079d8ee99649"
    },
    {
      "s": "dfcf34be-1086-482b-89ca-ef837527f85c",
      "p": "on-failure",
      "o": "9e12ed02-531b-4233-a40d-079d8ee99649"
    }
  ],
  "tags": [
    "T1087.001",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "output_file",
      "type": "path",
      "description": "Path where captured results will be placed",
      "value": "/tmp/T1087.001.txt"
    }
  ]
