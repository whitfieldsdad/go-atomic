{
  "id": "65526037-7079-44a9-bda1-2cb624838040",
  "name": "DLL Side-Loading using the Notepad++ GUP.exe binary",
  "description": "GUP is an open source signed binary used by Notepad++ for software updates, and is vulnerable to DLL Side-Loading, thus enabling the libcurl dll to be loaded.\nUpon execution, calc.exe will be opened.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "769d1aa2-75fc-4fec-9300-b6429f7c635d",
      "type": "execute-command",
      "data": {
        "command": "\"#{gup_executable}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "0226fe13-7813-424f-80c0-e6ed03351231",
      "type": "execute-command",
      "data": {
        "command": "taskkill /F /IM #{process_name} \u003enul 2\u003e\u00261\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "5db4d961-f57a-49f5-8ba7-f328a1e3a558",
      "name": "Check dependency 1/1",
      "description": "Gup.exe binary must exist on disk at specified location (#{gup_executable})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{gup_executable}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "a20093b5-29b3-40c2-a967-9a0207a835b8",
      "name": "Resolve dependency 1/1",
      "description": "Gup.exe binary must exist on disk at specified location (#{gup_executable})\n",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Type Directory (split-path \"#{gup_executable}\") -ErrorAction ignore | Out-Null\nInvoke-WebRequest \"https://github.com/redcanaryco/atomic-red-team/blob/master/atomics/T1574.002/bin/GUP.exe?raw=true\" -OutFile \"#{gup_executable}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "1333bff4-b45f-4ed6-b415-8d016eaab908",
      "name": "Re-check dependency 1/1",
      "description": "Gup.exe binary must exist on disk at specified location (#{gup_executable})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{gup_executable}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
