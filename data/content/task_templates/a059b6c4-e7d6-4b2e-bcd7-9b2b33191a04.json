    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "msi_payload",
      "type": "path",
      "description": "MSI file to execute",
      "value": "PathToAtomicsFolder\\T1218.007\\bin\\T1218.007_JScript.msi"
    },
    {
      "name": "msi_exe",
      "type": "path",
      "description": "MSIExec File Path",
      "value": "c:\\windows\\system32\\msiexec.exe"
