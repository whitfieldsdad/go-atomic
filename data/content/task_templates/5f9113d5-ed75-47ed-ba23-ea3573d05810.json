{
  "id": "5f9113d5-ed75-47ed-ba23-ea3573d05810",
  "name": "Set a file's access timestamp",
  "description": "Stomps on the access timestamp of a file\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "474514df-89c8-4edb-83f8-062d7642ba26",
      "type": "execute-command",
      "data": {
        "command": "touch -a -t 197001010000.00 #{target_filename}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "f085498a-e42e-4e84-b429-56bcb833ff9f",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{target_filename}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "59bb0f54-8820-462d-abaa-88d99fe13d02",
      "name": "Check dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{target_filename} \u0026\u0026 exit 0 || exit 1\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "de80d605-5950-448a-8c31-89712d4d0d23",
      "name": "Resolve dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "echo 'T1070.006 file access timestomp test' \u003e #{target_filename}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "f8184152-e4a1-41cd-afe5-8a1692607a08",
      "name": "Re-check dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{target_filename} \u0026\u0026 exit 0 || exit 1\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
