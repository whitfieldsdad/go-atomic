{
  "id": "02ea31cb-3b4c-4a2d-9bf1-e4e70ebcf5d0",
  "name": "Compress Data for Exfiltration With Rar",
  "description": "An adversary may compress data (e.g., sensitive documents) that is collected prior to exfiltration.\nWhen the test completes you should find the txt files from the %USERPROFILE% directory compressed in a file called T1560.001-data.rar in the %USERPROFILE% directory \n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "e09ed1d7-92a9-4a56-846f-f292c5f7ed8a",
      "type": "execute-command",
      "data": {
        "command": "\"#{rar_exe}\" a -r #{output_file} #{input_path}\\*#{file_extension}\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "06abf1be-5ee7-4ee2-bf9a-ee513148cffc",
      "type": "execute-command",
      "data": {
        "command": "del /f /q /s #{output_file} \u003enul 2\u003e\u00261\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "105c78e8-258d-4e54-bdd7-d7033e3c707f",
      "name": "Check dependency 1/1",
      "description": "Rar tool must be installed at specified location (#{rar_exe})\n",
      "type": "execute-command",
      "data": {
        "command": "if not exist \"#{rar_exe}\" (exit /b 1)\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "ee73987c-3fa7-441d-9508-94d03e07769e",
      "name": "Resolve dependency 1/1",
      "description": "Rar tool must be installed at specified location (#{rar_exe})\n",
      "type": "execute-command",
      "data": {
        "command": "echo Downloading Winrar installer\nbitsadmin /transfer myDownloadJob /download /priority normal \"https://www.win-rar.com/fileadmin/winrar-versions/winrar/th/winrar-x64-580.exe\" #{rar_installer}\n#{rar_installer} /S\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "5d98bc40-a17b-4a1b-9143-50ff97cd47ad",
      "name": "Re-check dependency 1/1",
      "description": "Rar tool must be installed at specified location (#{rar_exe})\n",
      "type": "execute-command",
      "data": {
        "command": "if not exist \"#{rar_exe}\" (exit /b 1)\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "e09ed1d7-92a9-4a56-846f-f292c5f7ed8a",
      "p": "on-success",
      "o": "06abf1be-5ee7-4ee2-bf9a-ee513148cffc"
    },
    {
      "s": "e09ed1d7-92a9-4a56-846f-f292c5f7ed8a",
      "p": "on-failure",
      "o": "06abf1be-5ee7-4ee2-bf9a-ee513148cffc"
    },
    {
      "s": "105c78e8-258d-4e54-bdd7-d7033e3c707f",
      "p": "on-success",
      "o": "e09ed1d7-92a9-4a56-846f-f292c5f7ed8a"
    },
    {
      "s": "105c78e8-258d-4e54-bdd7-d7033e3c707f",
      "p": "on-failure",
      "o": "ee73987c-3fa7-441d-9508-94d03e07769e"
    },
    {
      "s": "ee73987c-3fa7-441d-9508-94d03e07769e",
      "p": "on-success",
      "o": "5d98bc40-a17b-4a1b-9143-50ff97cd47ad"
    },
    {
      "s": "5d98bc40-a17b-4a1b-9143-50ff97cd47ad",
      "p": "on-failure",
      "o": "e09ed1d7-92a9-4a56-846f-f292c5f7ed8a"
    }
  ],
  "tags": [
    "T1560.001",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "file_extension",
      "type": "string",
      "description": "Extension of files to compress",
      "value": ".txt"
    },
    {
      "name": "output_file",
      "type": "path",
      "description": "Path where resulting compressed data should be placed",
      "value": "%USERPROFILE%\\T1560.001-data.rar"
    },
    {
      "name": "rar_installer",
      "type": "path",
      "description": "Winrar installer",
      "value": "%TEMP%\\winrar.exe"
    },
    {
      "name": "rar_exe",
      "type": "path",
      "description": "The RAR executable from Winrar",
      "value": "%programfiles%/WinRAR/Rar.exe"
    },
    {
      "name": "input_path",
      "type": "path",
      "description": "Path that should be compressed into our output file",
      "value": "%USERPROFILE%"
    }
  ]
}