{
  "id": "6b8df440-51ec-4d53-bf83-899591c9b5d7",
  "name": "Tamper with Windows Defender ATP PowerShell",
  "description": "Attempting to disable scheduled scanning and other parts of windows defender atp. Upon execution Virus and Threat Protection will show as disabled\nin Windows settings.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "2ec8388d-d681-4ccc-ab88-c61e5e331278",
      "type": "execute-command",
      "data": {
        "command": "Set-MpPreference -DisableRealtimeMonitoring 1\nSet-MpPreference -DisableBehaviorMonitoring 1\nSet-MpPreference -DisableScriptScanning 1\nSet-MpPreference -DisableBlockAtFirstSeen 1\n",
        "command_type": "powershell"
      },
      "elevation_required": true
    },
    {
      "id": "83f1092b-408a-476e-bf59-422b470ead06",
      "type": "execute-command",
      "data": {
        "command": "Set-MpPreference -DisableRealtimeMonitoring 0\nSet-MpPreference -DisableBehaviorMonitoring 0\nSet-MpPreference -DisableScriptScanning 0\nSet-MpPreference -DisableBlockAtFirstSeen 0\n",
        "command_type": "powershell"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "2ec8388d-d681-4ccc-ab88-c61e5e331278",
      "p": "on-success",
      "o": "83f1092b-408a-476e-bf59-422b470ead06"
    },
    {
      "s": "2ec8388d-d681-4ccc-ab88-c61e5e331278",
      "p": "on-failure",
      "o": "83f1092b-408a-476e-bf59-422b470ead06"
    }
  ],
  "tags": [
    "T1562",
    "T1562.001"
  ]
}