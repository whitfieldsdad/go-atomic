{
  "id": "e86f1b4b-fcc1-4a2a-ae10-b49da01458db",
  "name": "Compressing data using tarfile in Python (FreeBSD/Linux)",
  "description": "Uses tarfile from Python to compress files\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "0f451e33-bee2-4dff-a0eb-a1ed979c29e2",
      "type": "execute-command",
      "data": {
        "command": "which_python=`which python || which python3`\n$which_python -c \"from zipfile import ZipFile; ZipFile('#{path_to_output_file}', mode='w').write('#{path_to_input_file}')\" \n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "1389b2c0-efc9-4ed0-b9da-d01bd7e29682",
      "type": "execute-command",
      "data": {
        "command": "rm #{path_to_output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "3e97f905-074c-49a8-95eb-9b27a0fdb2f0",
