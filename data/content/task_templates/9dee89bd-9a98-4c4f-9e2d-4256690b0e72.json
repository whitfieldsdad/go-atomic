{
  "id": "9dee89bd-9a98-4c4f-9e2d-4256690b0e72",
  "name": "Delete a single file - Windows PowerShell",
  "description": "Delete a single file from the temporary directory using Powershell. Upon execution, no output will be displayed. Use File Explorer to verify the file was deleted.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "a064f07a-e430-43e4-ae17-3236da89a989",
      "type": "execute-command",
      "data": {
        "command": "Remove-Item -path #{file_to_delete}\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "3a1efab0-0b7a-4591-adb5-d30db48861d6",
      "name": "Check dependency 1/1",
      "description": "The file to delete must exist on disk at specified location (#{file_to_delete})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path #{file_to_delete}) {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "f69837d7-f41f-48f4-a3e2-15e75331c1a2",
      "name": "Resolve dependency 1/1",
      "description": "The file to delete must exist on disk at specified location (#{file_to_delete})\n",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Path #{file_to_delete} | Out-Null\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "add97e4c-3bc9-4bbf-8036-059aba586fe4",
      "name": "Re-check dependency 1/1",
      "description": "The file to delete must exist on disk at specified location (#{file_to_delete})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path #{file_to_delete}) {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
