{
  "id": "aa6cb8c4-b582-4f8e-b677-37733914abda",
  "name": "Activate Guest Account",
  "description": "The Adversaries can activate the default Guest user. The guest account is inactivated by default\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "a9e6e2ea-e001-4ad3-8c8e-215a42f9690b",
      "type": "execute-command",
      "data": {
        "command": "net user #{guest_user} /active:yes\n",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    },
    {
      "id": "c1bd0f65-b584-4f1d-a263-6ae4378c4972",
      "type": "execute-command",
      "data": {
        "command": "net user #{guest_user} /active:no\n",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "a9e6e2ea-e001-4ad3-8c8e-215a42f9690b",
      "p": "on-success",
      "o": "c1bd0f65-b584-4f1d-a263-6ae4378c4972"
    },
    {
      "s": "a9e6e2ea-e001-4ad3-8c8e-215a42f9690b",
      "p": "on-failure",
      "o": "c1bd0f65-b584-4f1d-a263-6ae4378c4972"
    }
  ],
  "tags": [
    "T1078",
    "T1078.001"
  ],
  "input_arguments": [
    {
      "name": "guest_user",
      "type": "string",
      "description": "Specify the guest account",
      "value": "guest"
    }
  ]
}