{
  "id": "aa6cb8c4-b582-4f8e-b677-37733914abda",
  "name": "Activate Guest Account",
  "description": "The Adversaries can activate the default Guest user. The guest account is inactivated by default\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "4ea66665-3dba-40f8-b434-4387ee3d7bb2",
      "type": "execute-command",
      "data": {
        "command": "net user #{guest_user} /active:yes\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "2e610822-fd73-4f7c-947d-fda6b36a1639",
      "type": "execute-command",
      "data": {
        "command": "net user #{guest_user} /active:no\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "4ea66665-3dba-40f8-b434-4387ee3d7bb2",
      "p": "on-success",
      "o": "2e610822-fd73-4f7c-947d-fda6b36a1639"
    },
    {
      "s": "4ea66665-3dba-40f8-b434-4387ee3d7bb2",
      "p": "on-failure",
      "o": "2e610822-fd73-4f7c-947d-fda6b36a1639"
    }
  ],
  "tags": [
    "T1078.001",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "guest_user",
      "type": "string",
      "description": "Specify the guest account",
      "value": "guest"
    }
  ]
}