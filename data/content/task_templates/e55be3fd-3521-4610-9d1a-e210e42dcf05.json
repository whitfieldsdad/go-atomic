{
  "id": "e55be3fd-3521-4610-9d1a-e210e42dcf05",
  "name": "Reg Key Run",
  "description": "Run Key Persistence\n\nUpon successful execution, cmd.exe will modify the registry by adding \\\"Atomic Red Team\\\" to the Run key. Output will be via stdout. \n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "48186811-6a58-4af6-ad7e-6f964eaadf8c",
      "type": "execute-command",
      "data": {
        "command": "REG ADD \"HKCU\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Run\" /V \"Atomic Red Team\" /t REG_SZ /F /D \"#{command_to_execute}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "1b052075-77f8-429f-9d77-fb5715c4d6ef",
      "type": "execute-command",
      "data": {
        "command": "REG DELETE \"HKCU\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Run\" /V \"Atomic Red Team\" /f \u003enul 2\u003e\u00261\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "48186811-6a58-4af6-ad7e-6f964eaadf8c",
      "p": "on-success",
      "o": "1b052075-77f8-429f-9d77-fb5715c4d6ef"
    },
    {
      "s": "48186811-6a58-4af6-ad7e-6f964eaadf8c",
      "p": "on-failure",
      "o": "1b052075-77f8-429f-9d77-fb5715c4d6ef"
    }
  ],
  "tags": [
    "T1547.001",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "command_to_execute",
      "type": "path",
      "description": "Thing to Run",
      "value": "C:\\Path\\AtomicRedTeam.exe"
    }
  ]
}