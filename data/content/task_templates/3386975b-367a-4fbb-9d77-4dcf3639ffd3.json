{
  "id": "3386975b-367a-4fbb-9d77-4dcf3639ffd3",
  "name": "Map admin share",
  "description": "Connecting To Remote Shares\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "9fd102ee-e2a9-45b2-8546-38cd473a4061",
      "type": "execute-command",
      "data": {
        "command": "cmd.exe /c \"net use \\\\#{computer_name}\\#{share_name} #{password} /u:#{user_name}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "tags": [
    "T1021.002",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "password",
      "type": "string",
      "description": "Password",
      "value": "P@ssw0rd1"
    },
    {
      "name": "computer_name",
      "type": "string",
      "description": "Target Computer Name",
      "value": "Target"
    },
    {
      "name": "user_name",
      "type": "string",
      "description": "Username",
      "value": "DOMAIN\\Administrator"
    },
    {
      "name": "share_name",
      "type": "string",
      "description": "Examples C$, IPC$, Admin$",
      "value": "C$"
    }
  ]
}