{
  "id": "002cca30-4778-4891-878a-aaffcfa502fa",
  "name": "Mount ISO image",
  "description": "Mounts ISO image downloaded from internet to evade Mark-of-the-Web. Upon successful execution, powershell will download the .iso from the Atomic Red Team repo, and mount the image. The provided sample ISO simply has a Reports shortcut file in it. Reference: https://www.microsoft.com/security/blog/2021/05/27/new-sophisticated-email-based-attack-from-nobelium/\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "ac8ee081-77fa-4946-9297-ad6195a6ec5c",
      "type": "execute-command",
      "data": {
        "command": "Mount-DiskImage -ImagePath \"#{path_of_iso}\"\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "91b4951a-bb45-4588-ab07-39574d892b08",
      "type": "execute-command",
      "data": {
        "command": "Dismount-DiskImage -ImagePath \"#{path_of_iso}\" | Out-Null\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "8d3cac6e-c3f0-4054-b441-6cfc075c0bf3",
      "name": "Check dependency 1/1",
      "description": "T1553.005.iso must exist on disk at specified location (#{path_of_iso})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{path_of_iso}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "f92a3ef8-766f-4afb-8477-877fcdbbd420",
      "name": "Resolve dependency 1/1",
      "description": "T1553.005.iso must exist on disk at specified location (#{path_of_iso})\n",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Type Directory (split-path \"#{path_of_iso}\") -ErrorAction ignore | Out-Null\nInvoke-WebRequest https://raw.githubusercontent.com/redcanaryco/atomic-red-team/master/atomics/T1553.005/bin/T1553.005.iso -OutFile \"#{path_of_iso}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "f74f93d1-e370-4a0f-a15c-aa16cfacb905",
      "name": "Re-check dependency 1/1",
      "description": "T1553.005.iso must exist on disk at specified location (#{path_of_iso})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{path_of_iso}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
