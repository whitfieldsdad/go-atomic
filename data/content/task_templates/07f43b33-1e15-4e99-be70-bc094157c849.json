{
  "id": "07f43b33-1e15-4e99-be70-bc094157c849",
  "name": "Disable OpenDNS Umbrella",
  "description": "Disables OpenDNS Umbrella\n",
  "platforms": [
    "darwin"
  ],
  "steps": [
    {
      "id": "93fb4d95-aae0-4e3c-8a15-8966d2abdbb7",
      "type": "execute-command",
      "data": {
        "command": "sudo launchctl unload /Library/LaunchDaemons/com.opendns.osx.RoamingClientConfigUpdater.plist\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "55b16652-8108-4170-87fc-f16128ffc9bf",
      "type": "execute-command",
      "data": {
        "command": "sudo launchctl load -w /Library/LaunchDaemons/com.opendns.osx.RoamingClientConfigUpdater.plist\n",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "93fb4d95-aae0-4e3c-8a15-8966d2abdbb7",
