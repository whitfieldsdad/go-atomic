{
  "id": "812c3ab8-94b0-4698-a9bf-9420af23ce24",
  "name": "Execute a process from a directory masquerading as the current parent directory.",
  "description": "Create and execute a process from a directory masquerading as the current parent directory (`...` instead of normal `..`)\n",
  "platforms": [
    "darwin",
    "linux"
  ],
  "steps": [
    {
      "id": "c604dc5f-7e2d-4f12-9da7-1a015516d318",
      "type": "execute-command",
      "data": {
        "command": "mkdir $HOME/...\ncp $(which sh) $HOME/...\n$HOME/.../sh -c \"echo #{test_message}\"\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "5ee5962c-3119-4ed9-94b1-545a8253e143",
      "type": "execute-command",
      "data": {
        "command": "rm -f $HOME/.../sh\nrmdir $HOME/.../\n",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "c604dc5f-7e2d-4f12-9da7-1a015516d318",
      "p": "on-success",
      "o": "5ee5962c-3119-4ed9-94b1-545a8253e143"
    },
    {
      "s": "c604dc5f-7e2d-4f12-9da7-1a015516d318",
      "p": "on-failure",
      "o": "5ee5962c-3119-4ed9-94b1-545a8253e143"
    }
  ],
  "tags": [
    "T1036.005",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "test_message",
      "type": "string",
      "description": "Test message to echo out to the screen",
      "value": "Hello from the Atomic Red Team test T1036.005#1"
    }
  ]
}