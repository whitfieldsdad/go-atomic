{
  "id": "5fefd767-ef54-4ac6-84d3-751ab85e8aba",
  "name": "Copy in loginwindow.plist for Re-Opened Applications",
  "description": "Copy in new loginwindow.plist to launch Calculator.\n",
  "platforms": [
    "darwin"
  ],
  "steps": [
    {
      "id": "2a91a1e8-91da-4de1-b2e6-1cde304a7d00",
      "type": "execute-command",
      "data": {
        "command": "cp #{calc_plist_path} ~/Library/Preferences/ByHost/com.apple.loginwindow.plist\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "2712384a-49fc-437d-a81d-730f3cc49592",
      "type": "execute-command",
      "data": {
        "command": "rm -f ~/Library/Preferences/ByHost/com.apple.loginwindow.plist\n",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "2a91a1e8-91da-4de1-b2e6-1cde304a7d00",
      "p": "on-success",
      "o": "2712384a-49fc-437d-a81d-730f3cc49592"
    },
    {
      "s": "2a91a1e8-91da-4de1-b2e6-1cde304a7d00",
      "p": "on-failure",
      "o": "2712384a-49fc-437d-a81d-730f3cc49592"
    }
  ],
  "tags": [
    "T1547.007",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "calc_plist_path",
      "type": "path",
      "description": "path to binary plist with entry to open calculator",
      "value": "PathToAtomicsFolder/T1547.007/src/reopen_loginwindow_calc.plist"
    }
  ]
}