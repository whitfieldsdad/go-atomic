{
  "id": "46959285-906d-40fa-9437-5a439accd878",
  "name": "Discover Private SSH Keys",
  "description": "Discover private SSH keys on a FreeBSD, macOS or Linux system.\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "ddc3498a-0d13-46bc-870a-7edfcf7c93cc",
      "type": "execute-command",
      "data": {
        "command": "find #{search_path} -name id_rsa 2\u003e/dev/null \u003e\u003e #{output_file}\nexit 0\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "6b674511-6b4a-483b-a59b-59e9e7f0740d",
      "type": "execute-command",
      "data": {
        "command": "rm #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "ddc3498a-0d13-46bc-870a-7edfcf7c93cc",
      "p": "on-success",
      "o": "6b674511-6b4a-483b-a59b-59e9e7f0740d"
    },
    {
      "s": "ddc3498a-0d13-46bc-870a-7edfcf7c93cc",
      "p": "on-failure",
      "o": "6b674511-6b4a-483b-a59b-59e9e7f0740d"
    }
  ],
  "tags": [
    "T1552",
    "T1552.004"
  ],
  "input_arguments": [
    {
      "name": "output_file",
      "type": "path",
      "description": "Output file containing locations of SSH key files",
      "value": "/tmp/keyfile_locations.txt"
    },
    {
      "name": "search_path",
      "type": "path",
      "description": "Path where to start searching from.",
      "value": "/"
    }
  ]
}