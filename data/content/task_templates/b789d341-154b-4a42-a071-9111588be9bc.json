{
  "id": "b789d341-154b-4a42-a071-9111588be9bc",
  "name": "List Google Chrome Bookmark JSON Files on macOS",
  "description": "Searches for Google Chrome's Bookmark file (on macOS) that contains bookmarks in JSON format and lists any found instances to a text file.\n",
  "platforms": [
    "darwin"
  ],
  "steps": [
    {
      "id": "4afe6d52-6d11-44b1-a4b1-390dc207aeb5",
      "type": "execute-command",
      "data": {
        "command": "find / -path \"*/Google/Chrome/*/Bookmarks\" -exec echo {} \u003e\u003e #{output_file} \\;\ncat #{output_file} 2\u003e/dev/null\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "cdec64ea-1404-41c6-ac42-043aa944c02e",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{output_file} 2\u003e/dev/null\n",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "4afe6d52-6d11-44b1-a4b1-390dc207aeb5",
      "p": "on-success",
      "o": "cdec64ea-1404-41c6-ac42-043aa944c02e"
    },
    {
      "s": "4afe6d52-6d11-44b1-a4b1-390dc207aeb5",
      "p": "on-failure",
      "o": "cdec64ea-1404-41c6-ac42-043aa944c02e"
    }
  ],
  "tags": [
    "T1217"
  ],
  "input_arguments": [
    {
      "name": "output_file",
      "type": "path",
      "description": "Path where captured results will be placed.",
      "value": "/tmp/T1217-Chrome.txt"
    }
  ]
}