{
  "id": "01b20ca8-c7a3-4d86-af59-059f15ed5474",
  "name": "Disable Windows OS Auto Update",
  "description": "Disable Auto Update Windows OS feature. Redline is using this as part of its defense evasion.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "7fe75c99-7c28-48c7-a4df-74a2d5e86c7b",
      "type": "execute-command",
      "data": {
        "command": "reg add \"HKLM\\SOFTWARE\\Policies\\Microsoft\\Windows\\WindowsUpdate\\AU\" /v \"NoAutoUpdate\" /t REG_DWORD /d 1 /f\n",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    },
    {
      "id": "b536d341-65a1-4629-83c0-00a86cb3d67d",
      "type": "execute-command",
      "data": {
        "command": "reg add \"HKLM\\SOFTWARE\\Policies\\Microsoft\\Windows\\WindowsUpdate\\AU\" /v \"NoAutoUpdate\" /t REG_DWORD /d 0 /f\n",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "7fe75c99-7c28-48c7-a4df-74a2d5e86c7b",
      "p": "on-success",
      "o": "b536d341-65a1-4629-83c0-00a86cb3d67d"
    },
    {
      "s": "7fe75c99-7c28-48c7-a4df-74a2d5e86c7b",
      "p": "on-failure",
      "o": "b536d341-65a1-4629-83c0-00a86cb3d67d"
    }
  ],
  "tags": [
    "T1112"
  ]
}