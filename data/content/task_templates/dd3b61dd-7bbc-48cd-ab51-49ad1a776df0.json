{
  "id": "dd3b61dd-7bbc-48cd-ab51-49ad1a776df0",
  "name": "certutil download (urlcache)",
  "description": "Use certutil -urlcache argument to download a file from the web. Note - /urlcache also works!\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "3c235a61-dbd5-4310-9b5e-2fe370f76b57",
      "type": "execute-command",
      "data": {
        "command": "cmd /c certutil -urlcache -split -f #{remote_file} #{local_path}\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "e9871fc0-81a5-41cb-8778-9aab89d0e123",
      "type": "execute-command",
      "data": {
        "command": "del #{local_path} \u003enul 2\u003e\u00261\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "3c235a61-dbd5-4310-9b5e-2fe370f76b57",
      "p": "on-success",
      "o": "e9871fc0-81a5-41cb-8778-9aab89d0e123"
    },
    {
      "s": "3c235a61-dbd5-4310-9b5e-2fe370f76b57",
      "p": "on-failure",
      "o": "e9871fc0-81a5-41cb-8778-9aab89d0e123"
    }
  ],
  "tags": [
    "T1105"
  ],
  "input_arguments": [
    {
      "name": "remote_file",
      "type": "url",
      "description": "URL of file to copy",
      "value": "https://raw.githubusercontent.com/redcanaryco/atomic-red-team/master/LICENSE.txt"
    },
    {
      "name": "local_path",
      "type": "path",
      "description": "Local path to place file",
      "value": "Atomic-license.txt"
    }
  ]
}