{
  "id": "d88a3d3b-d016-4939-a745-03638aafd21b",
  "name": "Set-Up Proxy Server",
  "description": "A modification registry to setup proxy server. This technique was seen in DarkGate malware as part of its installation.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "7c069102-c4ed-42c6-96d5-0f9d9d2b5f52",
      "type": "execute-command",
      "data": {
        "command": "reg add \"HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Internet Settings\" /v ProxyServer /t REG_SZ /d \"proxy.atomic-test.com:8080\" /f\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "b9356bb9-d8a2-4a36-b4f6-8d8d5c5a96f6",
      "type": "execute-command",
      "data": {
        "command": "reg delete \"HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Internet Settings\" /v ProxyServer\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "7c069102-c4ed-42c6-96d5-0f9d9d2b5f52",
      "p": "on-success",
      "o": "b9356bb9-d8a2-4a36-b4f6-8d8d5c5a96f6"
    },
    {
      "s": "7c069102-c4ed-42c6-96d5-0f9d9d2b5f52",
      "p": "on-failure",
      "o": "b9356bb9-d8a2-4a36-b4f6-8d8d5c5a96f6"
    }
  ],
  "tags": [
