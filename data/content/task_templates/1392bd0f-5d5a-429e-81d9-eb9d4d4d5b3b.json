{
  "id": "1392bd0f-5d5a-429e-81d9-eb9d4d4d5b3b",
  "name": "GetCurrent User with PowerShell Script",
  "description": "Use the PowerShell \"GetCurrent\" method of the WindowsIdentity .NET class to identify the logged user.",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "ee7bcb0a-064a-4bf1-a3ad-09a38b275c83",
      "type": "execute-command",
      "data": {
        "command": "[System.Security.Principal.WindowsIdentity]::GetCurrent() | Out-File -FilePath .\\CurrentUserObject.txt\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "34e68e65-d275-478c-8117-7f5aa34ab4f9",
      "type": "execute-command",
      "data": {
        "command": "Remove-Item -Path .\\CurrentUserObject.txt -Force\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "ee7bcb0a-064a-4bf1-a3ad-09a38b275c83",
      "p": "on-success",
      "o": "34e68e65-d275-478c-8117-7f5aa34ab4f9"
    },
    {
      "s": "ee7bcb0a-064a-4bf1-a3ad-09a38b275c83",
      "p": "on-failure",
      "o": "34e68e65-d275-478c-8117-7f5aa34ab4f9"
    }
  ],
