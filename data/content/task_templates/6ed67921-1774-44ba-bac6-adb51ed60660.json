      "id": "e22de555-3c74-4b8f-aa1e-abf58f14ecad",
      "type": "execute-command",
      "data": {
        "command": "copy \\\\#{remote}\\C$\\#{share_file} %TEMP%\\#{local_file}",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    },
    {
