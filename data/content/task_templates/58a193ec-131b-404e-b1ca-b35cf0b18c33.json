    }
  ],
  "tags": [
    "T1218.005",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "script_engine",
      "type": "string",
      "description": "Script Engine to use",
      "value": "JScript"
    },
    {
      "name": "hta_file_path",
      "type": "string",
      "description": "HTA file name and or path to be used",
      "value": "Test.hta"
    }
