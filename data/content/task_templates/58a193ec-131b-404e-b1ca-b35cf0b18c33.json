{
  "id": "58a193ec-131b-404e-b1ca-b35cf0b18c33",
  "name": "Invoke HTML Application - Jscript Engine Simulating Double Click",
  "description": "Executes an HTA Application using JScript script engine simulating double click.",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "4db9eb61-eac8-4e3a-900c-ea3de64b08ca",
      "type": "execute-command",
      "data": {
        "command": "Invoke-ATHHTMLApplication -HTAFilePath #{hta_file_path} -ScriptEngine #{script_engine} -SimulateUserDoubleClick",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "6e8d3f32-02fc-4269-af59-550dd416ef80",
      "name": "Check dependency 1/1",
      "description": "The AtomicTestHarnesses module must be installed and Invoke-ATHHTMLApplication must be exported in the module.",
      "type": "execute-command",
      "data": {
        "command": "$RequiredModule = Get-Module -Name AtomicTestHarnesses -ListAvailable\nif (-not $RequiredModule) {exit 1}\nif (-not $RequiredModule.ExportedCommands['Invoke-ATHHTMLApplication']) {exit 1} else {exit 0}",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "8fd7d88f-422a-4062-a02a-1bb78c739be8",
      "name": "Resolve dependency 1/1",
      "description": "The AtomicTestHarnesses module must be installed and Invoke-ATHHTMLApplication must be exported in the module.",
      "type": "execute-command",
      "data": {
        "command": "Install-Module -Name AtomicTestHarnesses -Scope CurrentUser -Force\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "f6cbcf7b-e40c-4ebf-94f1-5f47f4b031ea",
      "name": "Re-check dependency 1/1",
      "description": "The AtomicTestHarnesses module must be installed and Invoke-ATHHTMLApplication must be exported in the module.",
      "type": "execute-command",
      "data": {
        "command": "$RequiredModule = Get-Module -Name AtomicTestHarnesses -ListAvailable\nif (-not $RequiredModule) {exit 1}\nif (-not $RequiredModule.ExportedCommands['Invoke-ATHHTMLApplication']) {exit 1} else {exit 0}",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "6e8d3f32-02fc-4269-af59-550dd416ef80",
      "p": "on-success",
      "o": "4db9eb61-eac8-4e3a-900c-ea3de64b08ca"
    },
    {
      "s": "6e8d3f32-02fc-4269-af59-550dd416ef80",
      "p": "on-failure",
      "o": "8fd7d88f-422a-4062-a02a-1bb78c739be8"
    },
    {
      "s": "8fd7d88f-422a-4062-a02a-1bb78c739be8",
      "p": "on-success",
      "o": "f6cbcf7b-e40c-4ebf-94f1-5f47f4b031ea"
    },
    {
      "s": "f6cbcf7b-e40c-4ebf-94f1-5f47f4b031ea",
      "p": "on-failure",
      "o": "4db9eb61-eac8-4e3a-900c-ea3de64b08ca"
    }
  ],
  "tags": [
    "T1218.005",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "script_engine",
      "type": "string",
      "description": "Script Engine to use",
      "value": "JScript"
    },
    {
      "name": "hta_file_path",
      "type": "string",
      "description": "HTA file name and or path to be used",
      "value": "Test.hta"
    }
