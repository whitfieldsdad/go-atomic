      "type": "execute-command",
      "data": {
        "command": "echo \"SELinux is not installed\"; exit 1\n",
        "command_type": ""
      },
      "elevation_required": true
    },
    {
      "id": "02130f64-4212-4d8b-9e73-675f47c793ae",
