      "data": {
        "command": "setenforce 1",
        "command_type": "sh"
      },
      "elevation_required": true
    },
    {
      "id": "31b3324b-90f2-483b-8b38-7833df656329",
      "name": "Check dependency 1/1",
      "description": "SELinux must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "which setenforce",
        "command_type": "sh"
      },
      "elevation_required": true
    },
    {
      "id": "f655d81f-ac92-4ae4-89ae-a476e7d7717b",
      "name": "Resolve dependency 1/1",
      "description": "SELinux must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "echo \"SELinux is not installed\"; exit 1",
        "command_type": "sh"
      },
      "elevation_required": true
    },
    {
      "id": "55aefca5-70ac-40cf-ba52-3c163965037d",
      "name": "Re-check dependency 1/1",
      "description": "SELinux must be installed\n",
      "type": "execute-command",
