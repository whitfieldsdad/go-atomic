{
  "id": "c01cad7f-7a4c-49df-985e-b190dcf6a279",
  "name": "iwr or Invoke Web-Request download",
  "description": "Use 'iwr' or \"Invoke-WebRequest\" -URI argument to download a file from the web. Note: without -URI also works in some versions.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "951bc6a8-3963-4553-b7b4-5bdcdb9d227e",
      "type": "execute-command",
      "data": {
        "command": "powershell.exe iwr -URI #{remote_file} -Outfile #{local_path}\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "81d37252-3c42-4f31-89a2-a15fe6ae6606",
      "type": "execute-command",
      "data": {
        "command": "del %temp%\\Atomic-license.txt \u003enul 2\u003e\u00261\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "951bc6a8-3963-4553-b7b4-5bdcdb9d227e",
      "p": "on-success",
      "o": "81d37252-3c42-4f31-89a2-a15fe6ae6606"
    },
    {
      "s": "951bc6a8-3963-4553-b7b4-5bdcdb9d227e",
      "p": "on-failure",
      "o": "81d37252-3c42-4f31-89a2-a15fe6ae6606"
    }
  ],
  "tags": [
    "T1105",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "remote_file",
      "type": "url",
      "description": "URL of file to copy",
      "value": "https://raw.githubusercontent.com/redcanaryco/atomic-red-team/master/LICENSE.txt"
    },
    {
      "name": "local_path",
      "type": "path",
      "description": "Local path to place file",
      "value": "%temp%\\Atomic-license.txt"
    }
  ]
}