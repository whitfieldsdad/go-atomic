{
  "id": "6c2da894-0b57-43cb-87af-46ea3b501388",
  "name": "Remote System Discovery - ip tcp_metrics",
  "description": "Use the ip tcp_metrics command to display the recent cached entries for IPv4 and IPv6 source and destination addresses. \n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "0cccd095-7ab8-48ca-a758-7f9bb9d5645e",
      "type": "execute-command",
      "data": {
        "command": "ip tcp_metrics show |grep --invert-match \"^127\\.\"\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "7a4688f1-9b1e-4e47-964e-effdfd4bb789",
      "name": "Check dependency 1/1",
      "description": "Check if ip command exists on the machine\n",
      "type": "execute-command",
      "data": {
        "command": "if [ -x \"$(command -v ip)\" ]; then exit 0; else exit 1; fi;\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "19d098c5-73f3-4fe9-9e08-ed02f56afb44",
      "name": "Resolve dependency 1/1",
      "description": "Check if ip command exists on the machine\n",
      "type": "execute-command",
      "data": {
        "command": "apt-get install iproute2 -y\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "eda27544-a34a-487c-a331-da2009835172",
      "name": "Re-check dependency 1/1",
      "description": "Check if ip command exists on the machine\n",
      "type": "execute-command",
      "data": {
        "command": "if [ -x \"$(command -v ip)\" ]; then exit 0; else exit 1; fi;\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
