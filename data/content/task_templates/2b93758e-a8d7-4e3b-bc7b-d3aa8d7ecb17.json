{
  "id": "2b93758e-a8d7-4e3b-bc7b-d3aa8d7ecb17",
  "name": "Find AWS credentials",
  "description": "Find local AWS credentials from file, defaults to using / as the look path.\n",
  "platforms": [
    "darwin",
    "linux"
  ],
  "steps": [
    {
      "id": "1140c38a-dc1b-4698-8354-dedf28b01294",
      "type": "execute-command",
      "data": {
        "command": "find #{file_path} -name \"credentials\" -type f -path \"*/.aws/*\" 2\u003e/dev/null\n",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "tags": [
    "T1552.001",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "file_path",
      "type": "string",
      "description": "Path to search",
      "value": "/"
    }
  ]
}