{
  "id": "29d6f0d7-be63-4482-8827-ea77126c1ef7",
  "name": "Invoke CHM with default Shortcut Command Execution",
  "description": "Executes a CHM file with the default Shortcut Command method.",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "6d7f3f61-dd7f-434d-870b-9f622007b060",
      "type": "execute-command",
      "data": {
        "command": "Invoke-ATHCompiledHelp -HHFilePath #{hh_file_path} -CHMFilePath #{chm_file_path}",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "aee1f4d3-98bf-4046-b161-59dcded099d8",
      "name": "Check dependency 1/1",
      "description": "The AtomicTestHarnesses module must be installed and Invoke-ATHCompiledHelp must be exported in the module.",
      "type": "execute-command",
      "data": {
        "command": "$RequiredModule = Get-Module -Name AtomicTestHarnesses -ListAvailable\nif (-not $RequiredModule) {exit 1}\nif (-not $RequiredModule.ExportedCommands['Invoke-ATHCompiledHelp']) {exit 1} else {exit 0}",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "71dba102-c01f-44a8-b720-e9447e640fee",
      "name": "Resolve dependency 1/1",
      "description": "The AtomicTestHarnesses module must be installed and Invoke-ATHCompiledHelp must be exported in the module.",
      "type": "execute-command",
      "data": {
        "command": "Install-Module -Name AtomicTestHarnesses -Scope CurrentUser -Force",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "db614437-eca2-4626-ab61-1030d8777dd6",
      "name": "Re-check dependency 1/1",
      "description": "The AtomicTestHarnesses module must be installed and Invoke-ATHCompiledHelp must be exported in the module.",
      "type": "execute-command",
      "data": {
        "command": "$RequiredModule = Get-Module -Name AtomicTestHarnesses -ListAvailable\nif (-not $RequiredModule) {exit 1}\nif (-not $RequiredModule.ExportedCommands['Invoke-ATHCompiledHelp']) {exit 1} else {exit 0}",
        "command_type": "powershell"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "aee1f4d3-98bf-4046-b161-59dcded099d8",
      "p": "on-success",
      "o": "6d7f3f61-dd7f-434d-870b-9f622007b060"
    },
    {
      "s": "aee1f4d3-98bf-4046-b161-59dcded099d8",
      "p": "on-failure",
      "o": "71dba102-c01f-44a8-b720-e9447e640fee"
    },
    {
      "s": "71dba102-c01f-44a8-b720-e9447e640fee",
      "p": "on-success",
      "o": "db614437-eca2-4626-ab61-1030d8777dd6"
    },
    {
      "s": "db614437-eca2-4626-ab61-1030d8777dd6",
      "p": "on-failure",
      "o": "6d7f3f61-dd7f-434d-870b-9f622007b060"
    }
  ],
  "tags": [
    "T1218",
    "T1218.001"
  ],
  "input_arguments": [
    {
      "name": "chm_file_path",
      "type": "string",
      "description": "Default path of CHM",
      "value": "Test.chm"
    },
    {
      "name": "hh_file_path",
      "type": "path",
      "description": "path of modified HH.exe",
      "value": "$env:windir\\hh.exe"
    }
  ]
}