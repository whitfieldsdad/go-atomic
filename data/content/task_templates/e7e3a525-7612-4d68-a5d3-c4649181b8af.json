    }
  ],
  "tags": [
    "T1218",
    "T1218.005"
  ],
  "input_arguments": [
    {
      "name": "rundll32_file_path",
      "type": "path",
      "description": "Location of rundll32.exe",
      "value": "$env:windir\\system32\\rundll32.exe"
    },
    {
      "name": "script_engine",
      "type": "string",
      "description": "Script Engine to use",
      "value": "JScript"
    },
    {
      "name": "protocol_handler",
      "type": "string",
      "description": "Protocol Handler to use",
      "value": "About"
    }
  ]
