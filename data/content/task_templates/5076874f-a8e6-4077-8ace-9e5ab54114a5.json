      },
      "elevation_required": true
    },
    {
      "id": "b6df735c-ac10-481a-84a2-120e669abddf",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{output_file}",
        "command_type": "sh"
      },
      "elevation_required": true
    }
  ],
  "flows": [
