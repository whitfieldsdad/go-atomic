{
  "id": "7af2b51e-ad1c-498c-aca8-d3290c19535a",
  "name": "Data Compressed - nix - tar Folder or File",
  "description": "An adversary may compress data (e.g., sensitive documents) that is collected prior to exfiltration. This test uses standard gzip compression.\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "f5adcd23-50f8-4b62-bce1-bbbfeaa6cb13",
      "type": "execute-command",
      "data": {
        "command": "tar -cvzf #{output_file} #{input_file_folder}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "bccd1620-df05-407d-917e-a4ccdb072898",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{output_file}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "d21c0fb8-0309-4905-96aa-146165b1d57b",
      "name": "Check dependency 1/1",
      "description": "Folder to zip must exist (#{input_file_folder})\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{input_file_folder}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "b13873f6-55a8-4578-a20f-3b06b77e9c60",
      "name": "Resolve dependency 1/1",
      "description": "Folder to zip must exist (#{input_file_folder})\n",
      "type": "execute-command",
      "data": {
        "command": "mkdir -p #{input_file_folder} \u0026\u0026 touch #{input_file_folder}/file1",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "e8fbbd7f-b130-44f7-9846-e584be8e12f9",
      "name": "Re-check dependency 1/1",
      "description": "Folder to zip must exist (#{input_file_folder})\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{input_file_folder}",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "f5adcd23-50f8-4b62-bce1-bbbfeaa6cb13",
      "p": "on-success",
      "o": "bccd1620-df05-407d-917e-a4ccdb072898"
    },
    {
      "s": "f5adcd23-50f8-4b62-bce1-bbbfeaa6cb13",
      "p": "on-failure",
      "o": "bccd1620-df05-407d-917e-a4ccdb072898"
    },
    {
      "s": "d21c0fb8-0309-4905-96aa-146165b1d57b",
      "p": "on-success",
      "o": "f5adcd23-50f8-4b62-bce1-bbbfeaa6cb13"
    },
    {
      "s": "d21c0fb8-0309-4905-96aa-146165b1d57b",
      "p": "on-failure",
      "o": "b13873f6-55a8-4578-a20f-3b06b77e9c60"
    },
    {
      "s": "b13873f6-55a8-4578-a20f-3b06b77e9c60",
      "p": "on-success",
      "o": "e8fbbd7f-b130-44f7-9846-e584be8e12f9"
    },
    {
      "s": "e8fbbd7f-b130-44f7-9846-e584be8e12f9",
      "p": "on-failure",
      "o": "f5adcd23-50f8-4b62-bce1-bbbfeaa6cb13"
    }
  ],
  "tags": [
    "T1560",
    "T1560.001"
  ],
  "input_arguments": [
    {
      "name": "input_file_folder",
      "type": "path",
      "description": "Path that should be compressed",
      "value": "$HOME/$USERNAME"
    },
    {
      "name": "output_file",
      "type": "path",
      "description": "File that should be output",
      "value": "$HOME/data.tar.gz"
    }
  ]
}