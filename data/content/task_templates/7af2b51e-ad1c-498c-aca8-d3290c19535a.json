{
  "id": "7af2b51e-ad1c-498c-aca8-d3290c19535a",
  "name": "Data Compressed - nix - tar Folder or File",
  "description": "An adversary may compress data (e.g., sensitive documents) that is collected prior to exfiltration. This test uses standard gzip compression.\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "3b974c93-7ec0-4fab-8acb-499edabd2cc5",
      "type": "execute-command",
      "data": {
        "command": "tar -cvzf #{output_file} #{input_file_folder}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "b35653a2-c7f1-4175-ad0b-96a81f7df45d",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{output_file}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "9da704cb-9af2-4e7c-b253-d0e443e08303",
      "name": "Check dependency 1/1",
      "description": "Folder to zip must exist (#{input_file_folder})\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{input_file_folder}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "54ab1bb5-34d4-46dd-94bf-fa2afd71203c",
      "name": "Resolve dependency 1/1",
      "description": "Folder to zip must exist (#{input_file_folder})\n",
      "type": "execute-command",
      "data": {
        "command": "mkdir -p #{input_file_folder} \u0026\u0026 touch #{input_file_folder}/file1",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "098d897b-f33b-4774-b123-cf6fbb258203",
      "name": "Re-check dependency 1/1",
      "description": "Folder to zip must exist (#{input_file_folder})\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{input_file_folder}",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "3b974c93-7ec0-4fab-8acb-499edabd2cc5",
      "p": "on-success",
      "o": "b35653a2-c7f1-4175-ad0b-96a81f7df45d"
    },
    {
      "s": "3b974c93-7ec0-4fab-8acb-499edabd2cc5",
      "p": "on-failure",
      "o": "b35653a2-c7f1-4175-ad0b-96a81f7df45d"
    },
    {
      "s": "9da704cb-9af2-4e7c-b253-d0e443e08303",
      "p": "on-success",
      "o": "3b974c93-7ec0-4fab-8acb-499edabd2cc5"
    },
    {
      "s": "9da704cb-9af2-4e7c-b253-d0e443e08303",
      "p": "on-failure",
      "o": "54ab1bb5-34d4-46dd-94bf-fa2afd71203c"
    },
    {
      "s": "54ab1bb5-34d4-46dd-94bf-fa2afd71203c",
      "p": "on-success",
      "o": "098d897b-f33b-4774-b123-cf6fbb258203"
    },
    {
      "s": "098d897b-f33b-4774-b123-cf6fbb258203",
      "p": "on-failure",
      "o": "3b974c93-7ec0-4fab-8acb-499edabd2cc5"
    }
  ],
  "tags": [
    "T1560",
    "T1560.001"
  ],
  "input_arguments": [
    {
      "name": "input_file_folder",
      "type": "path",
      "description": "Path that should be compressed",
      "value": "$HOME/$USERNAME"
    },
    {
      "name": "output_file",
      "type": "path",
      "description": "File that should be output",
      "value": "$HOME/data.tar.gz"
    }
  ]
}