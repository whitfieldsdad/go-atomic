{
  "id": "7af2b51e-ad1c-498c-aca8-d3290c19535a",
  "name": "Data Compressed - nix - tar Folder or File",
  "description": "An adversary may compress data (e.g., sensitive documents) that is collected prior to exfiltration. This test uses standard gzip compression.\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "7d8d1a3c-0236-4098-bed0-7bffe6116110",
      "type": "execute-command",
      "data": {
        "command": "tar -cvzf #{output_file} #{input_file_folder}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "6ec33f3a-67dc-43d5-84f8-940003910c1b",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "882f52cf-41b9-4614-b9f6-da4d5adfd797",
      "name": "Check dependency 1/1",
      "description": "Folder to zip must exist (#{input_file_folder})\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{input_file_folder}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "f013bfb3-dfef-481c-a6d7-199c36352f1b",
      "name": "Resolve dependency 1/1",
      "description": "Folder to zip must exist (#{input_file_folder})\n",
      "type": "execute-command",
      "data": {
        "command": "mkdir -p #{input_file_folder} \u0026\u0026 touch #{input_file_folder}/file1\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "0d812a91-f9e8-46d8-8623-1b1df95e3412",
      "name": "Re-check dependency 1/1",
      "description": "Folder to zip must exist (#{input_file_folder})\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{input_file_folder}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "7d8d1a3c-0236-4098-bed0-7bffe6116110",
      "p": "on-success",
      "o": "6ec33f3a-67dc-43d5-84f8-940003910c1b"
    },
    {
      "s": "7d8d1a3c-0236-4098-bed0-7bffe6116110",
      "p": "on-failure",
      "o": "6ec33f3a-67dc-43d5-84f8-940003910c1b"
    },
    {
      "s": "882f52cf-41b9-4614-b9f6-da4d5adfd797",
      "p": "on-success",
      "o": "7d8d1a3c-0236-4098-bed0-7bffe6116110"
    },
    {
      "s": "882f52cf-41b9-4614-b9f6-da4d5adfd797",
      "p": "on-failure",
      "o": "f013bfb3-dfef-481c-a6d7-199c36352f1b"
    },
    {
      "s": "f013bfb3-dfef-481c-a6d7-199c36352f1b",
      "p": "on-success",
      "o": "0d812a91-f9e8-46d8-8623-1b1df95e3412"
    },
    {
      "s": "0d812a91-f9e8-46d8-8623-1b1df95e3412",
      "p": "on-failure",
      "o": "7d8d1a3c-0236-4098-bed0-7bffe6116110"
    }
  ],
  "tags": [
    "T1560",
    "T1560.001"
  ],
  "input_arguments": [
    {
      "name": "input_file_folder",
      "type": "path",
      "description": "Path that should be compressed",
      "value": "$HOME/$USERNAME"
    },
    {
      "name": "output_file",
      "type": "path",
      "description": "File that should be output",
      "value": "$HOME/data.tar.gz"
    }
  ]
}