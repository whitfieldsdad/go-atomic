{
  "id": "7af2b51e-ad1c-498c-aca8-d3290c19535a",
  "name": "Data Compressed - nix - tar Folder or File",
  "description": "An adversary may compress data (e.g., sensitive documents) that is collected prior to exfiltration. This test uses standard gzip compression.\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "81856694-fbb7-4267-a199-550a2009f2fa",
      "type": "execute-command",
      "data": {
        "command": "tar -cvzf #{output_file} #{input_file_folder}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "25f36078-9984-48b1-9e51-00e1cddfbf36",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "5c98356b-4432-4342-8af0-85afc43ec319",
      "name": "Check dependency 1/1",
      "description": "Folder to zip must exist (#{input_file_folder})\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{input_file_folder}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "07b4133d-81ec-450e-8b6e-9ccd490c1b2e",
      "name": "Resolve dependency 1/1",
      "description": "Folder to zip must exist (#{input_file_folder})\n",
      "type": "execute-command",
      "data": {
        "command": "mkdir -p #{input_file_folder} \u0026\u0026 touch #{input_file_folder}/file1\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "c7f7766d-7d46-4eeb-b4cd-74b59777f5df",
      "name": "Re-check dependency 1/1",
      "description": "Folder to zip must exist (#{input_file_folder})\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{input_file_folder}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "81856694-fbb7-4267-a199-550a2009f2fa",
      "p": "on-success",
      "o": "25f36078-9984-48b1-9e51-00e1cddfbf36"
    },
    {
      "s": "81856694-fbb7-4267-a199-550a2009f2fa",
      "p": "on-failure",
      "o": "25f36078-9984-48b1-9e51-00e1cddfbf36"
    },
    {
      "s": "5c98356b-4432-4342-8af0-85afc43ec319",
      "p": "on-success",
      "o": "81856694-fbb7-4267-a199-550a2009f2fa"
    },
    {
      "s": "5c98356b-4432-4342-8af0-85afc43ec319",
      "p": "on-failure",
      "o": "07b4133d-81ec-450e-8b6e-9ccd490c1b2e"
    },
    {
      "s": "07b4133d-81ec-450e-8b6e-9ccd490c1b2e",
      "p": "on-success",
      "o": "c7f7766d-7d46-4eeb-b4cd-74b59777f5df"
