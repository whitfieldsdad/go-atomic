{
  "id": "dc7726d2-8ccb-4cc6-af22-0d5afb53a548",
  "name": "Create a new account similar to ANONYMOUS LOGON",
  "description": "Create a new account similar to ANONYMOUS LOGON in a command prompt.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "95b41ab5-3ac7-4a15-9676-176b36b19b3c",
      "type": "execute-command",
      "data": {
        "command": "net user \"#{username}\" \"#{password}\" /add /domain\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "ff7a5e1a-4923-4635-816a-7a9993d0e061",
      "type": "execute-command",
      "data": {
        "command": "net user \"#{username}\" \u003enul 2\u003e\u00261 /del /domain\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "95b41ab5-3ac7-4a15-9676-176b36b19b3c",
