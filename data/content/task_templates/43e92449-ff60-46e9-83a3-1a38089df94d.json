{
  "id": "43e92449-ff60-46e9-83a3-1a38089df94d",
  "name": "Install MS Exchange Transport Agent Persistence",
  "description": "Install a Microsoft Exchange Transport Agent for persistence. This requires execution from an Exchange Client Access Server and the creation of a DLL with specific exports. Seen in use by Turla.\nMore details- https://docs.microsoft.com/en-us/exchange/transport-agents-exchange-2013-help\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "e9d642f9-b557-464b-873e-10b42e71af8b",
      "type": "execute-command",
      "data": {
        "command": "Install-TransportAgent -Name #{transport_agent_identity} -TransportAgentFactory #{class_factory} -AssemblyPath #{dll_path}\nEnable-TransportAgent #{transport_agent_identity}\nGet-TransportAgent | Format-List Name,Enabled\n",
        "command_type": "powershell"
      },
      "elevation_required": true
    },
    {
      "id": "fac8c117-54c5-4d5d-9e75-21f887d13684",
      "type": "execute-command",
      "data": {
        "command": "if(Get-Command \"Get-TransportAgent\" -ErrorAction Ignore){\n  Disable-TransportAgent #{transport_agent_identity}\n  Uninstall-TransportAgent #{transport_agent_identity}\n  Get-TransportAgent\n}\n",
        "command_type": "powershell"
      },
      "elevation_required": true
    },
    {
      "id": "d6aed929-c160-497a-a017-ad2effa3562b",
      "name": "Check dependency 1/1",
      "description": "Microsoft Exchange SnapIn must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "Get-TransportAgent -TransportService FrontEnd\n",
        "command_type": ""
      },
      "elevation_required": true
    },
    {
      "id": "25a06e24-066d-4150-ab04-e5220bedc4dd",
      "name": "Resolve dependency 1/1",
      "description": "Microsoft Exchange SnapIn must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "Add-PSSnapin Microsoft.Exchange.Management.PowerShell.SnapIn\n",
        "command_type": ""
      },
      "elevation_required": true
    },
    {
      "id": "3ed3a89c-b329-4146-8487-0fe5d086f933",
      "name": "Re-check dependency 1/1",
      "description": "Microsoft Exchange SnapIn must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "Get-TransportAgent -TransportService FrontEnd\n",
        "command_type": ""
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "e9d642f9-b557-464b-873e-10b42e71af8b",
      "p": "on-success",
      "o": "fac8c117-54c5-4d5d-9e75-21f887d13684"
    },
    {
      "s": "e9d642f9-b557-464b-873e-10b42e71af8b",
      "p": "on-failure",
      "o": "fac8c117-54c5-4d5d-9e75-21f887d13684"
    },
    {
      "s": "d6aed929-c160-497a-a017-ad2effa3562b",
      "p": "on-success",
      "o": "e9d642f9-b557-464b-873e-10b42e71af8b"
    },
    {
      "s": "d6aed929-c160-497a-a017-ad2effa3562b",
      "p": "on-failure",
      "o": "25a06e24-066d-4150-ab04-e5220bedc4dd"
    },
    {
      "s": "25a06e24-066d-4150-ab04-e5220bedc4dd",
      "p": "on-success",
      "o": "3ed3a89c-b329-4146-8487-0fe5d086f933"
    },
    {
      "s": "3ed3a89c-b329-4146-8487-0fe5d086f933",
      "p": "on-failure",
      "o": "e9d642f9-b557-464b-873e-10b42e71af8b"
    }
  ],
  "tags": [
    "T1505",
    "T1505.002"
  ],
  "input_arguments": [
    {
      "name": "transport_agent_identity",
      "type": "string",
      "description": "Friendly name of transport agent once installed.",
      "value": "Security Interop Agent"
    },
    {
      "name": "class_factory",
      "type": "string",
      "description": "Class factory of transport agent.",
      "value": "Microsoft.Exchange.Security.Interop.SecurityInteropAgentFactory"
    },
    {
      "name": "dll_path",
      "type": "path",
      "description": "Path of DLL to use as transport agent.",
      "value": "c:\\program files\\microsoft\\Exchange Server\\v15\\bin\\Microsoft.Exchange.Security.Interop.dll"
    }
  ]
}