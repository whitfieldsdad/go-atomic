{
  "id": "09210ad5-1ef2-4077-9ad3-7351e13e9222",
  "name": "Remove Network Share",
  "description": "Removes a Network Share utilizing the command_prompt\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "8e6a452c-6081-4c07-a6bc-e1145d73580b",
      "type": "execute-command",
      "data": {
        "command": "net share #{share_name} /delete\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "tags": [
    "T1070",
    "T1070.005"
  ],
  "input_arguments": [
    {
      "name": "share_name",
      "type": "string",
      "description": "Share to remove.",
      "value": "\\\\test\\share"
    }
  ]
}