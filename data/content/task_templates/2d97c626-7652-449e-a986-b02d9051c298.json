      "description": "Requires curl\n",
      "type": "execute-command",
      "data": {
        "command": "if [ -x \"$(command -v curl)\" ]; then exit 0; else exit 1; fi;      \n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
