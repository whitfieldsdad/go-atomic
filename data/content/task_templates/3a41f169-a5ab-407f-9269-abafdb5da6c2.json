{
  "id": "3a41f169-a5ab-407f-9269-abafdb5da6c2",
  "name": "List Mozilla Firefox Bookmark Database Files on FreeBSD/Linux",
  "description": "Searches for Mozilla Firefox's places.sqlite file (on FreeBSD or Linux distributions) that contains bookmarks and lists any found instances to a text file.\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "55f8b27e-3c3a-4710-bda9-fcb87e5c04cf",
      "type": "execute-command",
      "data": {
        "command": "find / -path \"*.mozilla/firefox/*/places.sqlite\" 2\u003e/dev/null -exec echo {} \u003e\u003e #{output_file} \\;\ncat #{output_file} 2\u003e/dev/null\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "b6b5a8bc-ce46-4436-afd0-85444c49cef2",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{output_file} 2\u003e/dev/null\n",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "55f8b27e-3c3a-4710-bda9-fcb87e5c04cf",
      "p": "on-success",
      "o": "b6b5a8bc-ce46-4436-afd0-85444c49cef2"
    },
    {
      "s": "55f8b27e-3c3a-4710-bda9-fcb87e5c04cf",
      "p": "on-failure",
      "o": "b6b5a8bc-ce46-4436-afd0-85444c49cef2"
    }
  ],
  "tags": [
    "T1217"
  ],
  "input_arguments": [
    {
      "name": "output_file",
      "type": "path",
      "description": "Path where captured results will be placed.",
      "value": "/tmp/T1217-Firefox.txt"
    }
  ]
}