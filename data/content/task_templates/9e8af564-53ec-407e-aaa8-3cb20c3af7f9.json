{
  "id": "9e8af564-53ec-407e-aaa8-3cb20c3af7f9",
  "name": "Disable UAC using reg.exe",
  "description": "Disable User Account Conrol (UAC) using the builtin tool reg.exe by changing its registry key\nHKLM\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Policies\\System\\EnableLUA from 1 to 0\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "e3968097-9437-47a3-a2ad-2ca5cecca005",
      "type": "execute-command",
      "data": {
        "command": "reg.exe ADD HKLM\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Policies\\System /v EnableLUA /t REG_DWORD /d 0 /f\n",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    },
    {
      "id": "35904617-5d6f-4906-8bff-d2e8baa98239",
      "type": "execute-command",
      "data": {
        "command": "reg.exe ADD HKLM\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Policies\\System /v EnableLUA /t REG_DWORD /d 1 /f\n",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "e3968097-9437-47a3-a2ad-2ca5cecca005",
      "p": "on-success",
      "o": "35904617-5d6f-4906-8bff-d2e8baa98239"
    },
