{
  "id": "db9de996-441e-4ae0-947b-61b6871e2fdf",
  "name": "Disable syslog (freebsd)",
  "description": "Disables syslog collection\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "d94efd7e-163e-4c47-ac67-1bfb28dbeb9b",
      "type": "execute-command",
      "data": {
        "command": "service syslogd stop\nsysrc syslogd_enable=\"NO\"",
        "command_type": "sh"
      },
      "elevation_required": true
    },
    {
      "id": "a44ddfa3-6c07-4c9a-8421-c04043f385bb",
      "type": "execute-command",
      "data": {
        "command": "sysrc syslogd_enable=\"YES\"\nservice syslogd start",
        "command_type": "sh"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "d94efd7e-163e-4c47-ac67-1bfb28dbeb9b",
      "p": "on-success",
      "o": "a44ddfa3-6c07-4c9a-8421-c04043f385bb"
    },
    {
      "s": "d94efd7e-163e-4c47-ac67-1bfb28dbeb9b",
      "p": "on-failure",
      "o": "a44ddfa3-6c07-4c9a-8421-c04043f385bb"
    }
  ],
