{
  "id": "db9de996-441e-4ae0-947b-61b6871e2fdf",
  "name": "Disable syslog (freebsd)",
  "description": "Disables syslog collection\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "ebd4afbf-c3a4-4576-962f-42fcff97b5d0",
      "type": "execute-command",
      "data": {
        "command": "service syslogd stop\nsysrc syslogd_enable=\"NO\"",
        "command_type": "sh"
      },
      "elevation_required": true
    },
    {
      "id": "a3df2902-6f8f-484d-a7ef-918fb0c6fa0f",
      "type": "execute-command",
      "data": {
        "command": "sysrc syslogd_enable=\"YES\"\nservice syslogd start",
        "command_type": "sh"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "ebd4afbf-c3a4-4576-962f-42fcff97b5d0",
      "p": "on-success",
      "o": "a3df2902-6f8f-484d-a7ef-918fb0c6fa0f"
    },
    {
      "s": "ebd4afbf-c3a4-4576-962f-42fcff97b5d0",
      "p": "on-failure",
      "o": "a3df2902-6f8f-484d-a7ef-918fb0c6fa0f"
    }
  ],
