{
  "id": "db9de996-441e-4ae0-947b-61b6871e2fdf",
  "name": "Disable syslog (freebsd)",
  "description": "Disables syslog collection\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "626d78bd-5f75-4536-a06a-7e4808b02a39",
      "type": "execute-command",
      "data": {
        "command": "service syslogd stop\nsysrc syslogd_enable=\"NO\"\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "94b95919-1e99-4041-9780-e00f81b02895",
      "type": "execute-command",
      "data": {
        "command": "sysrc syslogd_enable=\"YES\"\nservice syslogd start\n",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "626d78bd-5f75-4536-a06a-7e4808b02a39",
