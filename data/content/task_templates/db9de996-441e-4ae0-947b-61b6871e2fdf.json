{
  "id": "db9de996-441e-4ae0-947b-61b6871e2fdf",
  "name": "Disable syslog (freebsd)",
  "description": "Disables syslog collection\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "01d19f83-f5f7-4ec3-956d-fdc5545b5816",
      "type": "execute-command",
      "data": {
        "command": "service syslogd stop\nsysrc syslogd_enable=\"NO\"\n",
        "command_type": "sh"
      },
      "elevation_required": true
    },
    {
      "id": "ee318e57-6d91-4733-b899-cd320e73141f",
      "type": "execute-command",
      "data": {
        "command": "sysrc syslogd_enable=\"YES\"\nservice syslogd start\n",
        "command_type": "sh"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "01d19f83-f5f7-4ec3-956d-fdc5545b5816",
      "p": "on-success",
      "o": "ee318e57-6d91-4733-b899-cd320e73141f"
    },
    {
      "s": "01d19f83-f5f7-4ec3-956d-fdc5545b5816",
      "p": "on-failure",
      "o": "ee318e57-6d91-4733-b899-cd320e73141f"
    }
  ],
