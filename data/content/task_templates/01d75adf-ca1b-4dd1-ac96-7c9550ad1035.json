      "id": "66fc194a-630d-4ef1-9b20-100ba6e15898",
      "type": "execute-command",
      "data": {
        "command": "cscript \"#{jscript}\" \u003e %tmp%\\T1059.007.out.txt\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "66ee05c5-d71b-4f9d-b93c-8d310610abf9",
      "type": "execute-command",
      "data": {
        "command": "del %tmp%\\T1059.007.out.txt \u003enul 2\u003e\u00261",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "3814e2c2-a87b-4652-86f1-7b0cba007dd8",
