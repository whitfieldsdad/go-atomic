{
  "id": "be1a5d70-6865-44aa-ab50-42244c9fd16f",
  "name": "Trap EXIT (freebsd)",
  "description": "Launch bash shell with command arg to create TRAP on EXIT.\nThe trap executes script that writes to /tmp/art-fish.txt\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "9fa30d14-790f-4f2b-92ca-99c292f4c209",
      "type": "execute-command",
      "data": {
        "command": "bash -c 'trap \"nohup sh $PathToAtomicsFolder/T1546.005/src/echo-art-fish.sh\" EXIT'\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "36aa57f3-a73e-43db-b48b-a766e9268f21",
      "type": "execute-command",
      "data": {
        "command": "rm -f /tmp/art-fish.txt\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "36f2eed5-e63b-4525-87d1-fb2fcf84e490",
      "name": "Check dependency 1/1",
      "description": "Check if bash is installed.\n",
      "type": "execute-command",
      "data": {
        "command": "if [ ! -x \"$(command -v bash)\" ]; then exit 1; else exit 0; fi;\n",
        "command_type": ""
      },
