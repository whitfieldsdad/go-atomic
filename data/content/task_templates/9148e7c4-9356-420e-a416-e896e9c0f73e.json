{
  "id": "9148e7c4-9356-420e-a416-e896e9c0f73e",
  "name": "PowerShell Downgrade Attack",
  "description": "This test requires the manual installation of PowerShell V2.\n\nAttempts to run powershell commands in version 2.0 https://www.leeholmes.com/blog/2017/03/17/detecting-and-preventing-powershell-downgrade-attacks/\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "eaa662ad-7178-49fd-af85-ab83b576d3d0",
      "type": "execute-command",
      "data": {
        "command": "powershell.exe -version 2 -Command Write-Host $PSVersion\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "c6e695b8-8cba-4984-9ca9-7ce13bec324d",
      "name": "Check dependency 1/1",
      "description": "PowerShell version 2 must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "if(2 -in $PSVersionTable.PSCompatibleVersions.Major) {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "29476e87-7752-43cd-83ea-2f3f54736ccc",
      "name": "Resolve dependency 1/1",
      "description": "PowerShell version 2 must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "Write-Host  Automated installer not implemented yet, please install PowerShell v2 manually\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "fb4f3c21-5f6e-4cfd-a05f-ed45b2cf21bf",
      "name": "Re-check dependency 1/1",
      "description": "PowerShell version 2 must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "if(2 -in $PSVersionTable.PSCompatibleVersions.Major) {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "c6e695b8-8cba-4984-9ca9-7ce13bec324d",
      "p": "on-success",
      "o": "eaa662ad-7178-49fd-af85-ab83b576d3d0"
    },
    {
      "s": "c6e695b8-8cba-4984-9ca9-7ce13bec324d",
      "p": "on-failure",
      "o": "29476e87-7752-43cd-83ea-2f3f54736ccc"
    },
    {
      "s": "29476e87-7752-43cd-83ea-2f3f54736ccc",
      "p": "on-success",
      "o": "fb4f3c21-5f6e-4cfd-a05f-ed45b2cf21bf"
    },
