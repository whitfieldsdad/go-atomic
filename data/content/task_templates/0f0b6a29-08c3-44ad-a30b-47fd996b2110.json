{
  "id": "0f0b6a29-08c3-44ad-a30b-47fd996b2110",
  "name": "Show if a user account has ever logged in remotely",
  "description": "Show if a user account has ever logged in remotely\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "eab16a93-0daf-4fef-97ad-1dbd485080d2",
      "type": "execute-command",
      "data": {
        "command": "[ \"$(uname)\" = 'FreeBSD' ] \u0026\u0026 cmd=\"lastlogin\" || cmd=\"lastlog\" \n$cmd \u003e #{output_file}\ncat #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "a8cc7cc6-672e-4993-ae71-c27d259ebf45",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "5eea90f7-9eb8-4dd4-9813-d6f52d37aef7",
      "name": "Check dependency 1/1",
      "description": "Check if lastlog command exists on the machine\n",
      "type": "execute-command",
      "data": {
        "command": "if [ -x \"$(command -v lastlog)\" ]; then exit 0; else exit 1; fi\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "aaae30f3-d320-4dbc-8413-740910b12bd4",
      "name": "Resolve dependency 1/1",
      "description": "Check if lastlog command exists on the machine\n",
      "type": "execute-command",
      "data": {
        "command": "sudo apt-get install login; exit 1;\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "5fb245d4-3f08-45fe-9946-7732798a5b3f",
      "name": "Re-check dependency 1/1",
      "description": "Check if lastlog command exists on the machine\n",
