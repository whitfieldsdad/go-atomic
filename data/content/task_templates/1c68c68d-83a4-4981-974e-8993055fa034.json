{
  "id": "1c68c68d-83a4-4981-974e-8993055fa034",
  "name": "Windows - Disable the SR scheduled task",
  "description": "Use schtasks.exe to disable the System Restore (SR) scheduled task\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "6ac5d731-d6cf-4f80-bca3-4107c07607b7",
      "type": "execute-command",
      "data": {
        "command": "schtasks.exe /Change /TN \"\\Microsoft\\Windows\\SystemRestore\\SR\" /disable\n",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    },
    {
      "id": "61064e06-5ae9-40a5-948d-d139b8d737fd",
      "type": "execute-command",
      "data": {
        "command": "schtasks.exe /Change /TN \"\\Microsoft\\Windows\\SystemRestore\\SR\" /enable \u003enul 2\u003e\u00261\n",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "6ac5d731-d6cf-4f80-bca3-4107c07607b7",
      "p": "on-success",
      "o": "61064e06-5ae9-40a5-948d-d139b8d737fd"
    },
    {
      "s": "6ac5d731-d6cf-4f80-bca3-4107c07607b7",
      "p": "on-failure",
      "o": "61064e06-5ae9-40a5-948d-d139b8d737fd"
    }
  ],
