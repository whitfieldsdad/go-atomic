{
  "id": "b1729c57-9384-4d1c-9b99-9b220afb384e",
  "name": "Nimgrab - Transfer Files",
  "description": "Use nimgrab.exe to download a file from the web. \n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "b78df392-6d16-4526-9389-85448fa5e7ba",
      "type": "execute-command",
      "data": {
        "command": "cmd /c \"PathToAtomicsFolder\\..\\ExternalPayloads\\nimgrab.exe\" #{remote_file} #{destination_path}\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "829c9120-48cb-42b8-bee0-86b0bc13a317",
      "type": "execute-command",
      "data": {
        "command": "del #{destination_path} \u003enul 2\u003e\u00261\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "9f89d9ba-95b7-4925-a36e-7adf2e132dc5",
      "name": "Check dependency 1/1",
      "description": "NimGrab must be installed on system.\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"PathToAtomicsFolder\\..\\ExternalPayloads\\nimgrab.exe\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "f61d9666-a381-4371-91c7-508ce29404ff",
      "name": "Resolve dependency 1/1",
      "description": "NimGrab must be installed on system.\n",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Type Directory \"PathToAtomicsFolder\\..\\ExternalPayloads\\\" -ErrorAction Ignore -Force | Out-Null\nInvoke-WebRequest \"https://nim-lang.org/download/nim-1.6.6_x64.zip\" -Outfile \"PathToAtomicsFolder\\..\\ExternalPayloads\\nim.zip\"\nExpand-Archive -Path \"PathToAtomicsFolder\\..\\ExternalPayloads\\nim.zip\" -DestinationPath \"PathToAtomicsFolder\\..\\ExternalPayloads\\nim\" -Force\nCopy-Item \"PathToAtomicsFolder\\..\\ExternalPayloads\\nim\\nim-1.6.6\\bin\\nimgrab.exe\" \"PathToAtomicsFolder\\..\\ExternalPayloads\\nimgrab.exe\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "d3ef9670-558f-46dd-950a-dbe2eab07ee2",
      "name": "Re-check dependency 1/1",
      "description": "NimGrab must be installed on system.\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"PathToAtomicsFolder\\..\\ExternalPayloads\\nimgrab.exe\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "b78df392-6d16-4526-9389-85448fa5e7ba",
      "p": "on-success",
      "o": "829c9120-48cb-42b8-bee0-86b0bc13a317"
    },
    {
      "s": "b78df392-6d16-4526-9389-85448fa5e7ba",
      "p": "on-failure",
      "o": "829c9120-48cb-42b8-bee0-86b0bc13a317"
    },
    {
      "s": "9f89d9ba-95b7-4925-a36e-7adf2e132dc5",
      "p": "on-success",
      "o": "b78df392-6d16-4526-9389-85448fa5e7ba"
    },
    {
      "s": "9f89d9ba-95b7-4925-a36e-7adf2e132dc5",
      "p": "on-failure",
      "o": "f61d9666-a381-4371-91c7-508ce29404ff"
    },
    {
      "s": "f61d9666-a381-4371-91c7-508ce29404ff",
