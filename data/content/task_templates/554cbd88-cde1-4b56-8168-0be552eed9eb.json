{
  "id": "554cbd88-cde1-4b56-8168-0be552eed9eb",
  "name": "Reg Key RunOnce",
  "description": "RunOnce Key Persistence.\n\nUpon successful execution, cmd.exe will modify the registry to load AtomicRedTeam.dll to RunOnceEx. Output will be via stdout. \n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "9602ecfe-3c5f-48ee-9f98-9b46c523ad9a",
      "type": "execute-command",
      "data": {
        "command": "REG ADD HKLM\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\RunOnceEx\\0001\\Depend /v 1 /d \"#{thing_to_execute}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    },
    {
      "id": "659751ae-dd8d-4809-9f64-09aa5f8c9342",
      "type": "execute-command",
      "data": {
        "command": "REG DELETE HKLM\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\RunOnceEx\\0001\\Depend /v 1 /f \u003enul 2\u003e\u00261\n",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "9602ecfe-3c5f-48ee-9f98-9b46c523ad9a",
      "p": "on-success",
      "o": "659751ae-dd8d-4809-9f64-09aa5f8c9342"
    },
    {
      "s": "9602ecfe-3c5f-48ee-9f98-9b46c523ad9a",
      "p": "on-failure",
      "o": "659751ae-dd8d-4809-9f64-09aa5f8c9342"
    }
  ],
  "tags": [
    "T1547",
    "T1547.001"
  ],
  "input_arguments": [
    {
      "name": "thing_to_execute",
      "type": "path",
      "description": "Thing to Run",
      "value": "C:\\Path\\AtomicRedTeam.dll"
    }
  ]
}