{
  "id": "1ae5ea1f-0a4e-4e54-b2f5-4ac328a7f421",
  "name": "Regsvr32 Registering Non DLL",
  "description": "Replicating observed Gozi maldoc behavior registering a dll with an altered extension\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "2846a451-026e-4fb4-a2e0-3365d3fa6be0",
      "type": "execute-command",
      "data": {
        "command": "#{regsvr32path}\\#{regsvr32name} /s #{dll_file}",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "f0c4732b-6e52-4353-8359-845996d48c65",
      "type": "execute-command",
      "data": {
        "command": "#{regsvr32path}\\#{regsvr32name} /U /s #{dll_file}",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "713f0a50-9f51-4d90-9f58-647a5eb47284",
      "name": "Check dependency 1/1",
      "description": "Test requires a renamed dll file\n",
      "type": "execute-command",
      "data": {
        "command": "if exist #{dll_file} ( exit 0 ) else ( exit 1 )",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "f6443b63-5f2b-4271-91a5-7819cc53854e",
      "name": "Resolve dependency 1/1",
      "description": "Test requires a renamed dll file\n",
      "type": "execute-command",
      "data": {
        "command": "copy \"C:\\Windows\\System32\\shell32.dll\" \"#{dll_file}\"",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "86e0fbc6-fafd-44a5-9382-b27194b64d5b",
      "name": "Re-check dependency 1/1",
      "description": "Test requires a renamed dll file\n",
      "type": "execute-command",
      "data": {
        "command": "if exist #{dll_file} ( exit 0 ) else ( exit 1 )",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "2846a451-026e-4fb4-a2e0-3365d3fa6be0",
      "p": "on-success",
      "o": "f0c4732b-6e52-4353-8359-845996d48c65"
    },
    {
      "s": "2846a451-026e-4fb4-a2e0-3365d3fa6be0",
      "p": "on-failure",
      "o": "f0c4732b-6e52-4353-8359-845996d48c65"
    },
    {
      "s": "713f0a50-9f51-4d90-9f58-647a5eb47284",
      "p": "on-success",
      "o": "2846a451-026e-4fb4-a2e0-3365d3fa6be0"
    },
    {
      "s": "713f0a50-9f51-4d90-9f58-647a5eb47284",
      "p": "on-failure",
      "o": "f6443b63-5f2b-4271-91a5-7819cc53854e"
    },
    {
      "s": "f6443b63-5f2b-4271-91a5-7819cc53854e",
      "p": "on-success",
      "o": "86e0fbc6-fafd-44a5-9382-b27194b64d5b"
    },
    {
      "s": "86e0fbc6-fafd-44a5-9382-b27194b64d5b",
      "p": "on-failure",
      "o": "2846a451-026e-4fb4-a2e0-3365d3fa6be0"
    }
  ],
  "tags": [
    "T1218",
    "T1218.010"
  ],
  "input_arguments": [
    {
      "name": "dll_file",
      "type": "path",
      "description": "Path to renamed dll file to be registered",
      "value": "%temp%\\shell32.jpg"
    },
    {
      "name": "regsvr32path",
      "type": "path",
      "description": "Default location of Regsvr32.exe",
      "value": "C:\\Windows\\system32"
    },
    {
      "name": "regsvr32name",
      "type": "string",
      "description": "Default name of Regsvr32.exe",
      "value": "regsvr32.exe"
    }
  ]
}