{
  "id": "1ae5ea1f-0a4e-4e54-b2f5-4ac328a7f421",
  "name": "Regsvr32 Registering Non DLL",
  "description": "Replicating observed Gozi maldoc behavior registering a dll with an altered extension\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "e5bdd723-ab70-4eb9-a701-d38dbe62f52a",
      "type": "execute-command",
      "data": {
        "command": "#{regsvr32path}\\#{regsvr32name} /s #{dll_file}\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "d45d458a-aeed-4b1f-9249-6680ec37f086",
      "type": "execute-command",
      "data": {
        "command": "#{regsvr32path}\\#{regsvr32name} /U /s #{dll_file}\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "3bcec819-de76-47c3-8538-592ad0acac70",
      "name": "Check dependency 1/1",
      "description": "Test requires a renamed dll file\n",
      "type": "execute-command",
      "data": {
        "command": "if exist #{dll_file} ( exit 0 ) else ( exit 1 )\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "23f22397-7a1b-4fb8-b65b-18ef895e9cd5",
      "name": "Resolve dependency 1/1",
      "description": "Test requires a renamed dll file\n",
      "type": "execute-command",
      "data": {
        "command": "copy \"C:\\Windows\\System32\\shell32.dll\" \"#{dll_file}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "d1a3cf25-0076-4d79-ba58-30c137ec4c89",
      "name": "Re-check dependency 1/1",
      "description": "Test requires a renamed dll file\n",
      "type": "execute-command",
      "data": {
        "command": "if exist #{dll_file} ( exit 0 ) else ( exit 1 )\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "e5bdd723-ab70-4eb9-a701-d38dbe62f52a",
      "p": "on-success",
      "o": "d45d458a-aeed-4b1f-9249-6680ec37f086"
    },
    {
      "s": "e5bdd723-ab70-4eb9-a701-d38dbe62f52a",
      "p": "on-failure",
      "o": "d45d458a-aeed-4b1f-9249-6680ec37f086"
    },
    {
      "s": "3bcec819-de76-47c3-8538-592ad0acac70",
      "p": "on-success",
      "o": "e5bdd723-ab70-4eb9-a701-d38dbe62f52a"
    },
    {
      "s": "3bcec819-de76-47c3-8538-592ad0acac70",
      "p": "on-failure",
      "o": "23f22397-7a1b-4fb8-b65b-18ef895e9cd5"
    },
    {
      "s": "23f22397-7a1b-4fb8-b65b-18ef895e9cd5",
      "p": "on-success",
      "o": "d1a3cf25-0076-4d79-ba58-30c137ec4c89"
    },
    {
      "s": "d1a3cf25-0076-4d79-ba58-30c137ec4c89",
      "p": "on-failure",
      "o": "e5bdd723-ab70-4eb9-a701-d38dbe62f52a"
    }
  ],
  "tags": [
    "T1218",
    "T1218.010"
  ],
  "input_arguments": [
    {
      "name": "dll_file",
      "type": "path",
      "description": "Path to renamed dll file to be registered",
      "value": "%temp%\\shell32.jpg"
    },
    {
      "name": "regsvr32path",
      "type": "path",
      "description": "Default location of Regsvr32.exe",
      "value": "C:\\Windows\\system32"
    },
    {
      "name": "regsvr32name",
      "type": "string",
      "description": "Default name of Regsvr32.exe",
      "value": "regsvr32.exe"
    }
  ]
}