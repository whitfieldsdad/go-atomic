{
  "id": "1ae5ea1f-0a4e-4e54-b2f5-4ac328a7f421",
  "name": "Regsvr32 Registering Non DLL",
  "description": "Replicating observed Gozi maldoc behavior registering a dll with an altered extension\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "6d24eb22-d0d0-4f02-a02f-177653a3d268",
      "type": "execute-command",
      "data": {
        "command": "#{regsvr32path}\\#{regsvr32name} /s #{dll_file}\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "f974af67-441e-4696-b5a8-026e04ecc5a4",
      "type": "execute-command",
      "data": {
        "command": "#{regsvr32path}\\#{regsvr32name} /U /s #{dll_file}\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "1db16ccd-91d0-4918-8ab2-3abc00b0c67e",
      "name": "Check dependency 1/1",
      "description": "Test requires a renamed dll file\n",
      "type": "execute-command",
      "data": {
        "command": "if exist #{dll_file} ( exit 0 ) else ( exit 1 )\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "32f5e99b-2f5c-4599-8cfc-8d9a9b166566",
      "name": "Resolve dependency 1/1",
      "description": "Test requires a renamed dll file\n",
      "type": "execute-command",
      "data": {
        "command": "copy \"C:\\Windows\\System32\\shell32.dll\" \"#{dll_file}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "cd29fd06-a99e-4530-841a-88974ae8af7c",
      "name": "Re-check dependency 1/1",
      "description": "Test requires a renamed dll file\n",
      "type": "execute-command",
      "data": {
        "command": "if exist #{dll_file} ( exit 0 ) else ( exit 1 )\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "6d24eb22-d0d0-4f02-a02f-177653a3d268",
      "p": "on-success",
      "o": "f974af67-441e-4696-b5a8-026e04ecc5a4"
    },
    {
      "s": "6d24eb22-d0d0-4f02-a02f-177653a3d268",
      "p": "on-failure",
      "o": "f974af67-441e-4696-b5a8-026e04ecc5a4"
    },
    {
      "s": "1db16ccd-91d0-4918-8ab2-3abc00b0c67e",
      "p": "on-success",
      "o": "6d24eb22-d0d0-4f02-a02f-177653a3d268"
    },
    {
      "s": "1db16ccd-91d0-4918-8ab2-3abc00b0c67e",
      "p": "on-failure",
      "o": "32f5e99b-2f5c-4599-8cfc-8d9a9b166566"
    },
    {
      "s": "32f5e99b-2f5c-4599-8cfc-8d9a9b166566",
      "p": "on-success",
      "o": "cd29fd06-a99e-4530-841a-88974ae8af7c"
    },
    {
      "s": "cd29fd06-a99e-4530-841a-88974ae8af7c",
      "p": "on-failure",
      "o": "6d24eb22-d0d0-4f02-a02f-177653a3d268"
    }
  ],
  "tags": [
    "T1218.010",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "dll_file",
      "type": "path",
      "description": "Path to renamed dll file to be registered",
      "value": "%temp%\\shell32.jpg"
    },
    {
      "name": "regsvr32path",
      "type": "path",
      "description": "Default location of Regsvr32.exe",
      "value": "C:\\Windows\\system32"
    },
    {
      "name": "regsvr32name",
      "type": "string",
      "description": "Default name of Regsvr32.exe",
      "value": "regsvr32.exe"
    }
  ]
}