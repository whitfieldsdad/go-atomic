{
  "id": "1ae5ea1f-0a4e-4e54-b2f5-4ac328a7f421",
  "name": "Regsvr32 Registering Non DLL",
  "description": "Replicating observed Gozi maldoc behavior registering a dll with an altered extension\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "d1f196f3-cd71-4c37-8658-05f7626c34c9",
      "type": "execute-command",
      "data": {
        "command": "#{regsvr32path}\\#{regsvr32name} /s #{dll_file}",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "800b1f9e-c2fa-4d70-a9d0-3d7a881048b1",
      "type": "execute-command",
      "data": {
        "command": "#{regsvr32path}\\#{regsvr32name} /U /s #{dll_file}",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "aac1f081-8b42-46a6-b5d9-2d65bd4da363",
      "name": "Check dependency 1/1",
      "description": "Test requires a renamed dll file\n",
      "type": "execute-command",
      "data": {
        "command": "if exist #{dll_file} ( exit 0 ) else ( exit 1 )",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "b0dab738-a212-4662-a887-62c80fedf273",
      "name": "Resolve dependency 1/1",
      "description": "Test requires a renamed dll file\n",
      "type": "execute-command",
      "data": {
        "command": "copy \"C:\\Windows\\System32\\shell32.dll\" \"#{dll_file}\"",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "54c668a1-81b5-45cb-8496-15a40fb91b80",
      "name": "Re-check dependency 1/1",
      "description": "Test requires a renamed dll file\n",
      "type": "execute-command",
      "data": {
        "command": "if exist #{dll_file} ( exit 0 ) else ( exit 1 )",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "d1f196f3-cd71-4c37-8658-05f7626c34c9",
      "p": "on-success",
      "o": "800b1f9e-c2fa-4d70-a9d0-3d7a881048b1"
    },
    {
      "s": "d1f196f3-cd71-4c37-8658-05f7626c34c9",
      "p": "on-failure",
      "o": "800b1f9e-c2fa-4d70-a9d0-3d7a881048b1"
    },
    {
      "s": "aac1f081-8b42-46a6-b5d9-2d65bd4da363",
      "p": "on-success",
      "o": "d1f196f3-cd71-4c37-8658-05f7626c34c9"
    },
    {
      "s": "aac1f081-8b42-46a6-b5d9-2d65bd4da363",
      "p": "on-failure",
      "o": "b0dab738-a212-4662-a887-62c80fedf273"
    },
    {
      "s": "b0dab738-a212-4662-a887-62c80fedf273",
      "p": "on-success",
      "o": "54c668a1-81b5-45cb-8496-15a40fb91b80"
    },
    {
      "s": "54c668a1-81b5-45cb-8496-15a40fb91b80",
      "p": "on-failure",
      "o": "d1f196f3-cd71-4c37-8658-05f7626c34c9"
    }
  ],
  "tags": [
    "T1218",
    "T1218.010"
  ],
  "input_arguments": [
    {
      "name": "dll_file",
      "type": "path",
      "description": "Path to renamed dll file to be registered",
      "value": "%temp%\\shell32.jpg"
    },
    {
      "name": "regsvr32path",
      "type": "path",
      "description": "Default location of Regsvr32.exe",
      "value": "C:\\Windows\\system32"
    },
    {
      "name": "regsvr32name",
      "type": "string",
      "description": "Default name of Regsvr32.exe",
      "value": "regsvr32.exe"
    }
  ]
}