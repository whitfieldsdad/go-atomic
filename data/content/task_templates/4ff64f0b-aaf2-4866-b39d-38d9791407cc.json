{
  "id": "4ff64f0b-aaf2-4866-b39d-38d9791407cc",
  "name": "Process Discovery - ps",
  "description": "Utilize ps to identify processes.\n\nUpon successful execution, sh will execute ps and output to /tmp/loot.txt.\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "6e90194b-d51d-409b-b194-aa07370b9045",
      "type": "execute-command",
      "data": {
        "command": "ps \u003e\u003e #{output_file}\nps aux \u003e\u003e #{output_file}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "50744588-f40b-4a62-8471-fe124ca4a1a2",
      "type": "execute-command",
      "data": {
        "command": "rm #{output_file}",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "6e90194b-d51d-409b-b194-aa07370b9045",
      "p": "on-success",
      "o": "50744588-f40b-4a62-8471-fe124ca4a1a2"
    },
    {
      "s": "6e90194b-d51d-409b-b194-aa07370b9045",
      "p": "on-failure",
      "o": "50744588-f40b-4a62-8471-fe124ca4a1a2"
    }
  ],
  "tags": [
    "T1057"
  ],
  "input_arguments": [
    {
      "name": "output_file",
      "type": "path",
      "description": "path of output file",
      "value": "/tmp/loot.txt"
    }
  ]
}