{
  "id": "4ff64f0b-aaf2-4866-b39d-38d9791407cc",
  "name": "Process Discovery - ps",
  "description": "Utilize ps to identify processes.\n\nUpon successful execution, sh will execute ps and output to /tmp/loot.txt.\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "7eb100e7-c2d7-49f6-8c9b-7bb99b2336a7",
      "type": "execute-command",
      "data": {
        "command": "ps \u003e\u003e #{output_file}\nps aux \u003e\u003e #{output_file}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "a21b9b47-ceea-49a2-a3a8-33a41a00bb09",
      "type": "execute-command",
      "data": {
        "command": "rm #{output_file}",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "7eb100e7-c2d7-49f6-8c9b-7bb99b2336a7",
      "p": "on-success",
      "o": "a21b9b47-ceea-49a2-a3a8-33a41a00bb09"
    },
    {
      "s": "7eb100e7-c2d7-49f6-8c9b-7bb99b2336a7",
      "p": "on-failure",
      "o": "a21b9b47-ceea-49a2-a3a8-33a41a00bb09"
    }
  ],
  "tags": [
    "T1057"
  ],
  "input_arguments": [
    {
      "name": "output_file",
      "type": "path",
      "description": "path of output file",
      "value": "/tmp/loot.txt"
    }
  ]
}