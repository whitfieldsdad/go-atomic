{
  "id": "4ff64f0b-aaf2-4866-b39d-38d9791407cc",
  "name": "Process Discovery - ps",
  "description": "Utilize ps to identify processes.\n\nUpon successful execution, sh will execute ps and output to /tmp/loot.txt.\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "b6bea265-560d-468b-b7f5-d24bba0acb7d",
      "type": "execute-command",
      "data": {
        "command": "ps \u003e\u003e #{output_file}\nps aux \u003e\u003e #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "acb1094c-d252-4da4-a202-959313627007",
      "type": "execute-command",
      "data": {
        "command": "rm #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "b6bea265-560d-468b-b7f5-d24bba0acb7d",
      "p": "on-success",
      "o": "acb1094c-d252-4da4-a202-959313627007"
    },
    {
      "s": "b6bea265-560d-468b-b7f5-d24bba0acb7d",
      "p": "on-failure",
      "o": "acb1094c-d252-4da4-a202-959313627007"
    }
  ],
  "tags": [
    "T1057",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "output_file",
      "type": "path",
      "description": "path of output file",
      "value": "/tmp/loot.txt"
    }
  ]
}