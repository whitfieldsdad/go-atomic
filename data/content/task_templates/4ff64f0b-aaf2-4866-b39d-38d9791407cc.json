{
  "id": "4ff64f0b-aaf2-4866-b39d-38d9791407cc",
  "name": "Process Discovery - ps",
  "description": "Utilize ps to identify processes.\n\nUpon successful execution, sh will execute ps and output to /tmp/loot.txt.\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "2be59003-53f9-4d00-97ba-41e001df66d6",
      "type": "execute-command",
      "data": {
        "command": "ps \u003e\u003e #{output_file}\nps aux \u003e\u003e #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "4167a1ce-4984-4740-b61b-4eb51e9f5167",
      "type": "execute-command",
      "data": {
        "command": "rm #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "2be59003-53f9-4d00-97ba-41e001df66d6",
      "p": "on-success",
      "o": "4167a1ce-4984-4740-b61b-4eb51e9f5167"
    },
    {
      "s": "2be59003-53f9-4d00-97ba-41e001df66d6",
      "p": "on-failure",
      "o": "4167a1ce-4984-4740-b61b-4eb51e9f5167"
    }
  ],
  "tags": [
    "T1057"
  ],
  "input_arguments": [
    {
      "name": "output_file",
      "type": "path",
      "description": "path of output file",
      "value": "/tmp/loot.txt"
    }
  ]
}