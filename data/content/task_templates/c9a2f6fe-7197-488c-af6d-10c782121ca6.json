{
  "id": "c9a2f6fe-7197-488c-af6d-10c782121ca6",
  "name": "Office 365 - Set Audit Bypass For a Mailbox",
  "description": "Use Exchange Management Shell to Mailbox auditing to bypass. It will prevent any mailbox audit logging entries being generated for the target e-mail box.\nhttps://learn.microsoft.com/en-us/powershell/module/exchange/set-mailboxauditbypassassociation?view=exchange-ps\n",
  "platforms": [
    "office-365"
  ],
  "steps": [
    {
      "id": "f1e343b0-e173-47b9-8330-f54666b0d8a3",
      "type": "execute-command",
      "data": {
        "command": "$secure_pwd = \"#{password}\" | ConvertTo-SecureString -AsPlainText -Force\n$creds = New-Object System.Management.Automation.PSCredential -ArgumentList \"#{username}\", $secure_pwd\nConnect-ExchangeOnline -Credential $creds\nSet-MailboxAuditBypassAssociation -Identity \"#{target_email}\" -AuditBypassEnabled $true\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "1cf3df13-8acf-40d2-a9ee-b5a4307a81a0",
      "type": "execute-command",
      "data": {
        "command": "$secure_pwd = \"#{password}\" | ConvertTo-SecureString -AsPlainText -Force\n$creds = New-Object System.Management.Automation.PSCredential -ArgumentList \"#{username}\", $secure_pwd\nConnect-ExchangeOnline -Credential $creds\nSet-MailboxAuditBypassAssociation -Identity \"#{target_email}\" -AuditBypassEnabled $false\nDisconnect-ExchangeOnline -Confirm:$false\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "4fa44aa7-f14a-4695-be1b-c168b744da1f",
      "name": "Check dependency 1/1",
      "description": "ExchangeOnlineManagement PowerShell module must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "$RequiredModule = Get-Module -Name ExchangeOnlineManagement -ListAvailable\nif (-not $RequiredModule) {exit 1}\nif (-not $RequiredModule.ExportedCommands['Connect-ExchangeOnline']) {exit 1} else {exit 0}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "0f0ea0f9-e857-49d0-b7c8-acaf08018d54",
      "name": "Resolve dependency 1/1",
      "description": "ExchangeOnlineManagement PowerShell module must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "Install-Module -Name ExchangeOnlineManagement         \nImport-Module ExchangeOnlineManagement\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "3fa54af1-9b0e-428f-a695-3c7637b3e3b1",
      "name": "Re-check dependency 1/1",
      "description": "ExchangeOnlineManagement PowerShell module must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "$RequiredModule = Get-Module -Name ExchangeOnlineManagement -ListAvailable\nif (-not $RequiredModule) {exit 1}\nif (-not $RequiredModule.ExportedCommands['Connect-ExchangeOnline']) {exit 1} else {exit 0}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
