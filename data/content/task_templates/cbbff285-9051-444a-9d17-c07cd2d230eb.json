    }
  ],
  "tags": [
    "T1134.004",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "parent_pid",
      "type": "string",
      "description": "PID of process to spawn from",
      "value": "$PID"
    },
    {
      "name": "test_guid",
