{
  "id": "a39ee1bc-b8c1-4331-8e5f-1859eb408518",
  "name": "Create a user account on a FreeBSD system",
  "description": "Create a user via pw\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "eee1b0f0-d46d-4cae-83a7-ca3ca2167abd",
      "type": "execute-command",
      "data": {
        "command": "pw useradd #{username} -s /usr/sbin/nologin -d /nonexistent -c evil_account",
        "command_type": "sh"
      },
      "elevation_required": true
    },
    {
      "id": "751ef0b0-ae3e-4098-86cf-f098695d36af",
      "type": "execute-command",
      "data": {
        "command": "rmuser -y #{username}",
        "command_type": "sh"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "eee1b0f0-d46d-4cae-83a7-ca3ca2167abd",
      "p": "on-success",
      "o": "751ef0b0-ae3e-4098-86cf-f098695d36af"
    },
    {
      "s": "eee1b0f0-d46d-4cae-83a7-ca3ca2167abd",
      "p": "on-failure",
      "o": "751ef0b0-ae3e-4098-86cf-f098695d36af"
    }
  ],
  "tags": [
    "T1136",
    "T1136.001"
  ],
  "input_arguments": [
    {
      "name": "username",
      "type": "string",
      "description": "Username of the user to create",
      "value": "evil_user"
    }
  ]
}