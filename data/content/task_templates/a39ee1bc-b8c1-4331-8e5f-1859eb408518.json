{
  "id": "a39ee1bc-b8c1-4331-8e5f-1859eb408518",
  "name": "Create a user account on a FreeBSD system",
  "description": "Create a user via pw\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "329884db-65ab-46f5-b4a5-7bc3a920ca8d",
      "type": "execute-command",
      "data": {
        "command": "pw useradd #{username} -s /usr/sbin/nologin -d /nonexistent -c evil_account\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "c001cb8a-a41d-41db-99d4-7e716bd41c1c",
      "type": "execute-command",
      "data": {
        "command": "rmuser -y #{username}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "329884db-65ab-46f5-b4a5-7bc3a920ca8d",
      "p": "on-success",
      "o": "c001cb8a-a41d-41db-99d4-7e716bd41c1c"
    },
    {
      "s": "329884db-65ab-46f5-b4a5-7bc3a920ca8d",
      "p": "on-failure",
      "o": "c001cb8a-a41d-41db-99d4-7e716bd41c1c"
    }
  ],
  "tags": [
    "T1136.001",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "username",
      "type": "string",
      "description": "Username of the user to create",
      "value": "evil_user"
    }
  ]
}