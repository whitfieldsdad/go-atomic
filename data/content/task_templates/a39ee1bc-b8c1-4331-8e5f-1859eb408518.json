{
  "id": "a39ee1bc-b8c1-4331-8e5f-1859eb408518",
  "name": "Create a user account on a FreeBSD system",
  "description": "Create a user via pw\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "1a18b898-5a00-425a-9469-9c18720244bb",
      "type": "execute-command",
      "data": {
        "command": "pw useradd #{username} -s /usr/sbin/nologin -d /nonexistent -c evil_account\n",
        "command_type": "sh"
      },
      "elevation_required": true
    },
    {
      "id": "5781a9cd-de0e-4fad-b592-99e3d3f4baf4",
      "type": "execute-command",
      "data": {
        "command": "rmuser -y #{username}\n",
        "command_type": "sh"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "1a18b898-5a00-425a-9469-9c18720244bb",
      "p": "on-success",
      "o": "5781a9cd-de0e-4fad-b592-99e3d3f4baf4"
    },
    {
      "s": "1a18b898-5a00-425a-9469-9c18720244bb",
      "p": "on-failure",
      "o": "5781a9cd-de0e-4fad-b592-99e3d3f4baf4"
    }
  ],
  "tags": [
    "T1136",
    "T1136.001"
  ],
  "input_arguments": [
    {
      "name": "username",
      "type": "string",
      "description": "Username of the user to create",
      "value": "evil_user"
    }
  ]
}