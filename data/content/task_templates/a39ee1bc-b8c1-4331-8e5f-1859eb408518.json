{
  "id": "a39ee1bc-b8c1-4331-8e5f-1859eb408518",
  "name": "Create a user account on a FreeBSD system",
  "description": "Create a user via pw\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "a6555152-d041-422b-8e11-8629bf4cdb4e",
      "type": "execute-command",
      "data": {
        "command": "pw useradd #{username} -s /usr/sbin/nologin -d /nonexistent -c evil_account",
        "command_type": "sh"
      },
      "elevation_required": true
    },
    {
      "id": "ec104633-6668-44ec-9404-b9cce23bca96",
      "type": "execute-command",
      "data": {
        "command": "rmuser -y #{username}",
        "command_type": "sh"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "a6555152-d041-422b-8e11-8629bf4cdb4e",
      "p": "on-success",
      "o": "ec104633-6668-44ec-9404-b9cce23bca96"
    },
    {
      "s": "a6555152-d041-422b-8e11-8629bf4cdb4e",
      "p": "on-failure",
      "o": "ec104633-6668-44ec-9404-b9cce23bca96"
    }
  ],
  "tags": [
    "T1136",
    "T1136.001"
  ],
  "input_arguments": [
    {
      "name": "username",
      "type": "string",
      "description": "Username of the user to create",
      "value": "evil_user"
    }
  ]
}