{
  "id": "1a4ebe70-31d0-417b-ade2-ef4cb3e7d0e1",
  "name": "Remote System Discovery - ip route",
  "description": "Use the ip route command to display the kernels routing tables. \n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "bd63a658-7c79-4e60-ae95-74310fa6621d",
      "type": "execute-command",
      "data": {
        "command": "ip route show\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "a7d3eae5-17c1-4388-8334-657f58b3a041",
      "name": "Check dependency 1/1",
      "description": "Check if ip command exists on the machine\n",
      "type": "execute-command",
      "data": {
        "command": "if [ -x \"$(command -v ip)\" ]; then exit 0; else exit 1; fi;\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "0cd69719-74a9-4503-8c99-971cacb58f9d",
      "name": "Resolve dependency 1/1",
      "description": "Check if ip command exists on the machine\n",
      "type": "execute-command",
      "data": {
        "command": "apt-get install iproute2 -y\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "d3b37dc3-7942-4908-a8da-07f522e1e531",
      "name": "Re-check dependency 1/1",
      "description": "Check if ip command exists on the machine\n",
