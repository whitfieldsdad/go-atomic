{
  "id": "c75612b2-9de0-4d7c-879c-10d7b077072d",
  "name": "Compressing data using bz2 in Python (FreeBSD/Linux)",
  "description": "Uses bz2 from Python to compress files\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "8bb741c2-e8dc-429a-9871-22ae4b0e4307",
      "type": "execute-command",
      "data": {
        "command": "which_python=`which python || which python3`\n$which_python -c \"import bz2;input_file=open('#{path_to_input_file}','rb');content=input_file.read();input_file.close();bz2content=bz2.compress(content,compresslevel=9);output_file=open('#{path_to_output_file}','w+');output_file.write(str(bz2content));output_file.close();\"\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "b28059b7-42c8-49ad-b2cd-2ec4684cbe28",
      "type": "execute-command",
      "data": {
        "command": "rm #{path_to_output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "6277e79f-8540-4a2f-899f-56e694d20f47",
      "name": "Check dependency 1/1",
      "description": "Requires Python\n",
      "type": "execute-command",
      "data": {
        "command": "which python || which python3\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "9b845a28-700c-4ed7-9516-65ff79a7ff13",
      "name": "Resolve dependency 1/1",
      "description": "Requires Python\n",
      "type": "execute-command",
      "data": {
        "command": "echo \"please install python to run this test\"; exit 1\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "044f3c0d-c2ec-4ceb-9349-2f7c6f2e6658",
