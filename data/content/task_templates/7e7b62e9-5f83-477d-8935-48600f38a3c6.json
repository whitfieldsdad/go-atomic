{
  "id": "7e7b62e9-5f83-477d-8935-48600f38a3c6",
  "name": "RDP Authentication Level Override",
  "description": "A modification registry to override RDP Authentication Level. This technique was seen in DarkGate malware as part of its installation.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "42a1fa20-56f1-4d56-be0f-20011933dfb8",
      "type": "execute-command",
      "data": {
        "command": "reg add \"HKCU\\Software\\Microsoft\\Terminal Server Client\" /v AuthenticationLevelOverride /t REG_DWORD /d 0 /f\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "afa234e3-b0fa-463b-b2a5-b54c6d5be5a4",
      "type": "execute-command",
      "data": {
        "command": "reg delete \"HKCU\\Software\\Microsoft\\Terminal Server Client\" /v AuthenticationLevelOverride\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "42a1fa20-56f1-4d56-be0f-20011933dfb8",
      "p": "on-success",
      "o": "afa234e3-b0fa-463b-b2a5-b54c6d5be5a4"
    },
    {
      "s": "42a1fa20-56f1-4d56-be0f-20011933dfb8",
      "p": "on-failure",
      "o": "afa234e3-b0fa-463b-b2a5-b54c6d5be5a4"
    }
  ],
  "tags": [
    "T1112"
  ]
}