{
  "id": "001a042b-859f-44d9-bf81-fd1c4e2200b0",
  "name": "Compressing data using zipfile in Python (FreeBSD/Linux)",
  "description": "Uses zipfile from Python to compress files\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "38496a7a-f755-4578-bf36-46945dcedf4c",
      "type": "execute-command",
      "data": {
        "command": "which_python=`which python || which python3`\n$which_python -c \"from zipfile import ZipFile; ZipFile('#{path_to_output_file}', mode='w').write('#{path_to_input_file}')\"",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "26747b6d-d76a-44bb-9c73-12e0de7cc256",
      "type": "execute-command",
      "data": {
        "command": "rm #{path_to_output_file}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "6a663266-47d5-433f-9ecc-f575f75c0d85",
      "name": "Check dependency 1/1",
      "description": "Requires Python\n",
      "type": "execute-command",
