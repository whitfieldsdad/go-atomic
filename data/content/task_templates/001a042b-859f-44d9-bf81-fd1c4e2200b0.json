{
  "id": "001a042b-859f-44d9-bf81-fd1c4e2200b0",
  "name": "Compressing data using zipfile in Python (FreeBSD/Linux)",
  "description": "Uses zipfile from Python to compress files\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "5d46885b-8677-4998-a531-2f224b297555",
      "type": "execute-command",
      "data": {
        "command": "which_python=`which python || which python3`\n$which_python -c \"from zipfile import ZipFile; ZipFile('#{path_to_output_file}', mode='w').write('#{path_to_input_file}')\"\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "caf960bb-787c-4a22-83b2-c41ac595cbef",
      "type": "execute-command",
      "data": {
        "command": "rm #{path_to_output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
