{
  "id": "158bd4dd-6359-40ab-b13c-285b9ef6fa25",
  "name": "Remote System Discovery - ip neighbour",
  "description": "Use the ip neighbour command to display the known link layer (ARP table) addresses for hosts sharing the same network segment. \n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "5f6b6f28-96e9-40ae-9c21-188884a68d9f",
      "type": "execute-command",
      "data": {
        "command": "ip neighbour show\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "8ead699e-b415-428f-a285-4258e281f04d",
      "name": "Check dependency 1/1",
      "description": "Check if ip command exists on the machine\n",
      "type": "execute-command",
      "data": {
        "command": "if [ -x \"$(command -v ip)\" ]; then exit 0; else exit 1; fi;\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "8c4822d8-6e61-42bd-848b-6676024ace24",
      "name": "Resolve dependency 1/1",
      "description": "Check if ip command exists on the machine\n",
      "type": "execute-command",
      "data": {
        "command": "apt-get install iproute2 -y\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "c4b2fe54-27ae-4ae0-90bc-ef3073340d49",
      "name": "Re-check dependency 1/1",
      "description": "Check if ip command exists on the machine\n",
      "type": "execute-command",
