{
  "id": "1f73af33-62a8-4bf1-bd10-3bea931f2c0d",
  "name": "Set a SetGID flag on file (freebsd)",
  "description": "This test sets the SetGID flag on a file in FreeBSD.\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "810af0e6-e996-4dfb-9f9a-1df5b09cbda9",
      "type": "execute-command",
      "data": {
        "command": "touch #{file_to_setuid}\nchown root #{file_to_setuid}\nchmod g+xs #{file_to_setuid}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "bc210649-f41a-44c2-ad87-84dcc64bbac3",
      "type": "execute-command",
      "data": {
        "command": "rm #{file_to_setuid}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "810af0e6-e996-4dfb-9f9a-1df5b09cbda9",
      "p": "on-success",
      "o": "bc210649-f41a-44c2-ad87-84dcc64bbac3"
    },
    {
      "s": "810af0e6-e996-4dfb-9f9a-1df5b09cbda9",
      "p": "on-failure",
      "o": "bc210649-f41a-44c2-ad87-84dcc64bbac3"
    }
  ],
  "tags": [
    "T1548.001",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "file_to_setuid",
      "type": "path",
      "description": "Path of file to set SetGID flag",
      "value": "/tmp/evilBinary"
    }
  ]
}