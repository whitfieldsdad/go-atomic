{
  "id": "1f73af33-62a8-4bf1-bd10-3bea931f2c0d",
  "name": "Set a SetGID flag on file (freebsd)",
  "description": "This test sets the SetGID flag on a file in FreeBSD.\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "0420fc80-4c3b-42be-ae78-2cbc8b6794c5",
      "type": "execute-command",
      "data": {
        "command": "touch #{file_to_setuid}\nchown root #{file_to_setuid}\nchmod g+xs #{file_to_setuid}\n",
        "command_type": "sh"
      },
      "elevation_required": true
    },
    {
      "id": "b450f510-6d94-4e3d-802c-ceb335645f89",
      "type": "execute-command",
      "data": {
        "command": "rm #{file_to_setuid}\n",
        "command_type": "sh"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "0420fc80-4c3b-42be-ae78-2cbc8b6794c5",
      "p": "on-success",
      "o": "b450f510-6d94-4e3d-802c-ceb335645f89"
    },
    {
      "s": "0420fc80-4c3b-42be-ae78-2cbc8b6794c5",
      "p": "on-failure",
      "o": "b450f510-6d94-4e3d-802c-ceb335645f89"
    }
  ],
  "tags": [
    "T1548",
    "T1548.001"
  ],
  "input_arguments": [
    {
      "name": "file_to_setuid",
      "type": "path",
      "description": "Path of file to set SetGID flag",
      "value": "/tmp/evilBinary"
    }
  ]
}