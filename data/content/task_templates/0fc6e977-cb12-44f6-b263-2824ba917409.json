{
  "id": "0fc6e977-cb12-44f6-b263-2824ba917409",
  "name": "rsync remote file copy (push)",
  "description": "Utilize rsync to perform a remote file copy (push)\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "f9ac331f-967b-4e33-a036-26f88d322a13",
      "type": "execute-command",
      "data": {
        "command": "rsync -r #{local_path} #{username}@#{remote_host}:#{remote_path}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "52c1f255-3056-4908-ad6e-71816f4f4b0c",
      "name": "Check dependency 1/1",
      "description": "rsync must be installed on the machine \n",
      "type": "execute-command",
      "data": {
        "command": "if [ -x \"$(command -v rsync)\" ]; then exit 0; else exit 1; fi\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "9a0b7696-8d39-4e0a-8c64-679606b30853",
      "name": "Resolve dependency 1/1",
      "description": "rsync must be installed on the machine \n",
      "type": "execute-command",
      "data": {
        "command": "(pkg install -y rsync)||(sudo apt-get -y install rsync)\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "69288028-8c4f-4c8e-aac8-55f42255fd1c",
      "name": "Re-check dependency 1/1",
      "description": "rsync must be installed on the machine \n",
      "type": "execute-command",
      "data": {
        "command": "if [ -x \"$(command -v rsync)\" ]; then exit 0; else exit 1; fi\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "52c1f255-3056-4908-ad6e-71816f4f4b0c",
      "p": "on-success",
      "o": "f9ac331f-967b-4e33-a036-26f88d322a13"
    },
    {
      "s": "52c1f255-3056-4908-ad6e-71816f4f4b0c",
      "p": "on-failure",
