{
  "id": "0fc6e977-cb12-44f6-b263-2824ba917409",
  "name": "rsync remote file copy (push)",
  "description": "Utilize rsync to perform a remote file copy (push)\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "115cac27-85a6-4011-9b60-a6c30e8e748c",
      "type": "execute-command",
      "data": {
        "command": "rsync -r #{local_path} #{username}@#{remote_host}:#{remote_path}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "ad04fc4f-4040-48be-9306-b25c20d1589b",
      "name": "Check dependency 1/1",
      "description": "rsync must be installed on the machine \n",
      "type": "execute-command",
      "data": {
        "command": "if [ -x \"$(command -v rsync)\" ]; then exit 0; else exit 1; fi\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "4efea032-84d0-4d30-9426-64223bec6aa0",
      "name": "Resolve dependency 1/1",
      "description": "rsync must be installed on the machine \n",
      "type": "execute-command",
      "data": {
        "command": "(pkg install -y rsync)||(sudo apt-get -y install rsync)\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "6c59ecad-2752-47d1-b32b-6c016c8e77a8",
      "name": "Re-check dependency 1/1",
      "description": "rsync must be installed on the machine \n",
      "type": "execute-command",
      "data": {
        "command": "if [ -x \"$(command -v rsync)\" ]; then exit 0; else exit 1; fi\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "ad04fc4f-4040-48be-9306-b25c20d1589b",
      "p": "on-success",
      "o": "115cac27-85a6-4011-9b60-a6c30e8e748c"
    },
    {
      "s": "ad04fc4f-4040-48be-9306-b25c20d1589b",
      "p": "on-failure",
