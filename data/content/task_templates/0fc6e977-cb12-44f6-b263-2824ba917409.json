{
  "id": "0fc6e977-cb12-44f6-b263-2824ba917409",
  "name": "rsync remote file copy (push)",
  "description": "Utilize rsync to perform a remote file copy (push)\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "88f0d367-30da-4643-80dd-bcaf0c16dda6",
      "type": "execute-command",
      "data": {
        "command": "rsync -r #{local_path} #{username}@#{remote_host}:#{remote_path}",
        "command_type": "sh"
      },
      "elevation_required": true
    },
    {
      "id": "36afd414-a8e6-4e67-8466-3f3032081d69",
      "name": "Check dependency 1/1",
      "description": "rsync must be installed on the machine \n",
      "type": "execute-command",
      "data": {
        "command": "if [ -x \"$(command -v rsync)\" ]; then exit 0; else exit 1; fi",
        "command_type": "sh"
      },
      "elevation_required": true
    },
    {
      "id": "9b041102-c1a6-4be4-ba4c-67888decd309",
      "name": "Resolve dependency 1/1",
      "description": "rsync must be installed on the machine \n",
      "type": "execute-command",
      "data": {
        "command": "(pkg install -y rsync)||(sudo apt-get -y install rsync)",
        "command_type": "sh"
      },
      "elevation_required": true
    },
    {
      "id": "ab7c2071-980c-4202-9426-e902c4cda80f",
      "name": "Re-check dependency 1/1",
      "description": "rsync must be installed on the machine \n",
      "type": "execute-command",
      "data": {
        "command": "if [ -x \"$(command -v rsync)\" ]; then exit 0; else exit 1; fi",
        "command_type": "sh"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "36afd414-a8e6-4e67-8466-3f3032081d69",
      "p": "on-success",
      "o": "88f0d367-30da-4643-80dd-bcaf0c16dda6"
    },
    {
      "s": "36afd414-a8e6-4e67-8466-3f3032081d69",
      "p": "on-failure",
      "o": "9b041102-c1a6-4be4-ba4c-67888decd309"
    },
    {
      "s": "9b041102-c1a6-4be4-ba4c-67888decd309",
      "p": "on-success",
      "o": "ab7c2071-980c-4202-9426-e902c4cda80f"
    },
    {
      "s": "ab7c2071-980c-4202-9426-e902c4cda80f",
      "p": "on-failure",
      "o": "88f0d367-30da-4643-80dd-bcaf0c16dda6"
    }
  ],
  "tags": [
    "T1105"
  ],
  "input_arguments": [
    {
      "name": "remote_path",
      "type": "path",
      "description": "Remote path to receive rsync",
      "value": "/tmp/victim-files"
    },
    {
      "name": "remote_host",
      "type": "string",
      "description": "Remote host to copy toward",
      "value": "victim-host"
    },
    {
      "name": "local_path",
      "type": "path",
      "description": "Path of folder to copy",
      "value": "/tmp/adversary-rsync/"
    },
    {
      "name": "username",
      "type": "string",
      "description": "User account to authenticate on remote host",
      "value": "victim"
    }
  ]
