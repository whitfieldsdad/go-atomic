{
  "id": "0fc6e977-cb12-44f6-b263-2824ba917409",
  "name": "rsync remote file copy (push)",
  "description": "Utilize rsync to perform a remote file copy (push)\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "7e47e798-1e94-4e8c-bd7b-a6637aeff9c1",
      "type": "execute-command",
      "data": {
        "command": "rsync -r #{local_path} #{username}@#{remote_host}:#{remote_path}\n",
        "command_type": "sh"
      },
      "elevation_required": true
    },
    {
      "id": "94f6c43c-1c78-428a-b8c4-21b3087db3d7",
      "name": "Check dependency 1/1",
      "description": "rsync must be installed on the machine \n",
      "type": "execute-command",
      "data": {
        "command": "if [ -x \"$(command -v rsync)\" ]; then exit 0; else exit 1; fi\n",
        "command_type": ""
      },
      "elevation_required": true
    },
    {
      "id": "f5a00209-1735-497a-8b16-d6db435a7b3b",
      "name": "Resolve dependency 1/1",
      "description": "rsync must be installed on the machine \n",
      "type": "execute-command",
      "data": {
        "command": "(pkg install -y rsync)||(sudo apt-get -y install rsync)\n",
        "command_type": ""
      },
      "elevation_required": true
    },
    {
      "id": "b81c9d89-efda-4ed0-8521-199abfafd333",
      "name": "Re-check dependency 1/1",
      "description": "rsync must be installed on the machine \n",
      "type": "execute-command",
      "data": {
        "command": "if [ -x \"$(command -v rsync)\" ]; then exit 0; else exit 1; fi\n",
        "command_type": ""
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "94f6c43c-1c78-428a-b8c4-21b3087db3d7",
      "p": "on-success",
      "o": "7e47e798-1e94-4e8c-bd7b-a6637aeff9c1"
    },
    {
      "s": "94f6c43c-1c78-428a-b8c4-21b3087db3d7",
      "p": "on-failure",
      "o": "f5a00209-1735-497a-8b16-d6db435a7b3b"
    },
    {
      "s": "f5a00209-1735-497a-8b16-d6db435a7b3b",
      "p": "on-success",
      "o": "b81c9d89-efda-4ed0-8521-199abfafd333"
    },
    {
      "s": "b81c9d89-efda-4ed0-8521-199abfafd333",
      "p": "on-failure",
      "o": "7e47e798-1e94-4e8c-bd7b-a6637aeff9c1"
    }
  ],
  "tags": [
    "T1105"
  ],
  "input_arguments": [
    {
      "name": "remote_path",
      "type": "path",
      "description": "Remote path to receive rsync",
      "value": "/tmp/victim-files"
    },
    {
      "name": "remote_host",
      "type": "string",
      "description": "Remote host to copy toward",
      "value": "victim-host"
    },
    {
      "name": "local_path",
      "type": "path",
      "description": "Path of folder to copy",
      "value": "/tmp/adversary-rsync/"
    },
    {
