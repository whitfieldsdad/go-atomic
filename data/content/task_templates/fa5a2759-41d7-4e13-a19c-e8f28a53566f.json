{
  "id": "fa5a2759-41d7-4e13-a19c-e8f28a53566f",
  "name": "svchost writing a file to a UNC path",
  "description": "svchost.exe writing a non-Microsoft Office file to a file with a UNC path.\nUpon successful execution, this will rename cmd.exe as svchost.exe and move it to `c:\\`, then execute svchost.exe with output to a txt file.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "49556df3-8dca-46ca-89ad-aec335f8ba7a",
      "type": "execute-command",
      "data": {
        "command": "copy C:\\Windows\\System32\\cmd.exe C:\\svchost.exe\nC:\\svchost.exe /c echo T1105 \u003e \\\\localhost\\c$\\T1105.txt\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "e78e1c25-5b22-4ddc-b6a7-d6af4064911c",
      "type": "execute-command",
      "data": {
        "command": "del C:\\T1105.txt \u003enul 2\u003e\u00261\ndel C:\\\\svchost.exe \u003enul 2\u003e\u00261\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "49556df3-8dca-46ca-89ad-aec335f8ba7a",
      "p": "on-success",
      "o": "e78e1c25-5b22-4ddc-b6a7-d6af4064911c"
    },
    {
      "s": "49556df3-8dca-46ca-89ad-aec335f8ba7a",
      "p": "on-failure",
      "o": "e78e1c25-5b22-4ddc-b6a7-d6af4064911c"
    }
  ],
