{
  "id": "1864fdec-ff86-4452-8c30-f12507582a93",
  "name": "Export Certificate Item(s)",
  "description": "This command finds all certificate items and sends the output to local file in pem format.\n",
  "platforms": [
    "darwin"
  ],
  "steps": [
    {
      "id": "2a5db528-2c89-4b24-b686-d7f4f6e6d473",
      "type": "execute-command",
      "data": {
        "command": "security find-certificate -a -p \u003e #{cert_export}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "ed8c9b67-4866-4227-82d9-f083d3c0a07d",
      "type": "execute-command",
      "data": {
        "command": "rm #{cert_export}",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "2a5db528-2c89-4b24-b686-d7f4f6e6d473",
      "p": "on-success",
      "o": "ed8c9b67-4866-4227-82d9-f083d3c0a07d"
    },
    {
      "s": "2a5db528-2c89-4b24-b686-d7f4f6e6d473",
      "p": "on-failure",
      "o": "ed8c9b67-4866-4227-82d9-f083d3c0a07d"
    }
  ],
  "tags": [
    "T1555",
    "T1555.001"
  ],
  "input_arguments": [
    {
      "name": "cert_export",
      "type": "path",
      "description": "Specify the path of the certificates to export.",
      "value": "/tmp/certs.pem"
    }
  ]
}