{
  "id": "1864fdec-ff86-4452-8c30-f12507582a93",
  "name": "Export Certificate Item(s)",
  "description": "This command finds all certificate items and sends the output to local file in pem format.\n",
  "platforms": [
    "darwin"
  ],
  "steps": [
    {
      "id": "7e2e0c58-1c78-4da9-9847-580b5f203ef2",
      "type": "execute-command",
      "data": {
        "command": "security find-certificate -a -p \u003e #{cert_export}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "e8cf41de-d1a6-400b-9963-cec6d34db103",
      "type": "execute-command",
      "data": {
        "command": "rm #{cert_export}",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "7e2e0c58-1c78-4da9-9847-580b5f203ef2",
      "p": "on-success",
      "o": "e8cf41de-d1a6-400b-9963-cec6d34db103"
    },
    {
      "s": "7e2e0c58-1c78-4da9-9847-580b5f203ef2",
      "p": "on-failure",
      "o": "e8cf41de-d1a6-400b-9963-cec6d34db103"
    }
  ],
  "tags": [
    "T1555.001",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "cert_export",
      "type": "path",
      "description": "Specify the path of the certificates to export.",
      "value": "/tmp/certs.pem"
    }
  ]
}