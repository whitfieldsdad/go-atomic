{
  "id": "1864fdec-ff86-4452-8c30-f12507582a93",
  "name": "Export Certificate Item(s)",
  "description": "This command finds all certificate items and sends the output to local file in pem format.\n",
  "platforms": [
    "darwin"
  ],
  "steps": [
    {
      "id": "b172c7f8-f4a3-43f9-9bc4-43b343a28605",
      "type": "execute-command",
      "data": {
        "command": "security find-certificate -a -p \u003e #{cert_export}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "b9335c34-4f09-48fd-9da2-8bc9622ca0b6",
      "type": "execute-command",
      "data": {
        "command": "rm #{cert_export}",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "b172c7f8-f4a3-43f9-9bc4-43b343a28605",
      "p": "on-success",
      "o": "b9335c34-4f09-48fd-9da2-8bc9622ca0b6"
    },
    {
      "s": "b172c7f8-f4a3-43f9-9bc4-43b343a28605",
      "p": "on-failure",
      "o": "b9335c34-4f09-48fd-9da2-8bc9622ca0b6"
    }
  ],
  "tags": [
    "T1555.001",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "cert_export",
      "type": "path",
      "description": "Specify the path of the certificates to export.",
      "value": "/tmp/certs.pem"
    }
  ]
}