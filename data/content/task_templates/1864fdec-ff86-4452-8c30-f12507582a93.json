{
  "id": "1864fdec-ff86-4452-8c30-f12507582a93",
  "name": "Export Certificate Item(s)",
  "description": "This command finds all certificate items and sends the output to local file in pem format.\n",
  "platforms": [
    "darwin"
  ],
  "steps": [
    {
      "id": "693b581c-6e1e-49ea-adb6-a4a83b45cd45",
      "type": "execute-command",
      "data": {
        "command": "security find-certificate -a -p \u003e #{cert_export}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "9435aed5-f528-4123-a4ed-b2e0b646c64d",
      "type": "execute-command",
      "data": {
        "command": "rm #{cert_export}",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "693b581c-6e1e-49ea-adb6-a4a83b45cd45",
      "p": "on-success",
      "o": "9435aed5-f528-4123-a4ed-b2e0b646c64d"
    },
    {
      "s": "693b581c-6e1e-49ea-adb6-a4a83b45cd45",
      "p": "on-failure",
      "o": "9435aed5-f528-4123-a4ed-b2e0b646c64d"
    }
  ],
  "tags": [
    "T1555",
    "T1555.001"
  ],
  "input_arguments": [
    {
      "name": "cert_export",
      "type": "path",
      "description": "Specify the path of the certificates to export.",
      "value": "/tmp/certs.pem"
    }
  ]
}