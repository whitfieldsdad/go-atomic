{
  "id": "1864fdec-ff86-4452-8c30-f12507582a93",
  "name": "Export Certificate Item(s)",
  "description": "This command finds all certificate items and sends the output to local file in pem format.\n",
  "platforms": [
    "darwin"
  ],
  "steps": [
    {
      "id": "d14bccf8-69f0-4add-926f-a888553988b1",
      "type": "execute-command",
      "data": {
        "command": "security find-certificate -a -p \u003e #{cert_export}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "c0452ce0-ace6-434c-aaed-52f07b8dcc94",
      "type": "execute-command",
      "data": {
        "command": "rm #{cert_export}",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "d14bccf8-69f0-4add-926f-a888553988b1",
      "p": "on-success",
      "o": "c0452ce0-ace6-434c-aaed-52f07b8dcc94"
    },
    {
      "s": "d14bccf8-69f0-4add-926f-a888553988b1",
      "p": "on-failure",
      "o": "c0452ce0-ace6-434c-aaed-52f07b8dcc94"
    }
  ],
  "tags": [
    "T1555",
    "T1555.001"
  ],
  "input_arguments": [
    {
      "name": "cert_export",
      "type": "path",
      "description": "Specify the path of the certificates to export.",
      "value": "/tmp/certs.pem"
    }
  ]
}