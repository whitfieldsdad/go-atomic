{
  "id": "edbcd8c9-3639-4844-afad-455c91e95a35",
  "name": "psexec.py (Impacket)",
  "description": "Will execute a command on the remote host with Impacket psexec.py script.\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "0cd30d5b-f837-4e8e-a914-bfeeb708b442",
      "type": "execute-command",
      "data": {
        "command": "psexec.py '#{domain}/#{username}:#{password}@#{remote_host}' '#{command}'\n",
        "command_type": "bash"
      },
      "elevation_required": false
    },
    {
      "id": "5417e145-7ef0-40b2-9183-4bd8cbca5c11",
      "name": "Check dependency 1/1",
      "description": "psexec.py (Impacket)\n",
      "type": "execute-command",
      "data": {
        "command": "if [ -x \"$(command -v psexec.py)\" ]; then exit 0; else exit 1; fi;\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "a5fc71a5-b3db-4eaa-b595-7471f565464e",
      "name": "Resolve dependency 1/1",
      "description": "psexec.py (Impacket)\n",
      "type": "execute-command",
      "data": {
        "command": "sudo pip3 install impacket\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "3a6b3982-b2a7-49fd-af66-b5a9d6d6e3ca",
