{
  "id": "97116a3f-efac-4b26-8336-b9cb18c45188",
  "name": "Download a file using wscript",
  "description": "Use wscript to run a local VisualBasic file to download a remote file",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "40672829-0216-420b-8e6c-f50c8ea8df6f",
      "type": "execute-command",
      "data": {
        "command": "wscript.exe \"#{vbscript_file}\"\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "2de2f3c1-2ea3-4787-8d45-fc74940d17e6",
      "type": "execute-command",
      "data": {
        "command": "del Atomic-License.txt \u003enul 2\u003e\u00261",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "74f31a0d-5cfd-4d9c-a65d-30e97485c683",
      "name": "Check dependency 1/1",
      "description": "#{vbscript_file} must be exist on system.\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{vbscript_file}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "73ca1f91-9c58-4496-87ae-fcf362cc4413",
      "name": "Resolve dependency 1/1",
      "description": "#{vbscript_file} must be exist on system.\n",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Type Directory (split-path \"#{vbscript_file}\") -ErrorAction ignore | Out-Null\nInvoke-WebRequest \"https://github.com/redcanaryco/atomic-red-team/raw/master/atomics/T1105/src/T1105-download-file.vbs\" -OutFile \"#{vbscript_file}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "5b918261-3e50-437a-adad-becfe1ebc270",
      "name": "Re-check dependency 1/1",
      "description": "#{vbscript_file} must be exist on system.\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path \"#{vbscript_file}\") {exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
