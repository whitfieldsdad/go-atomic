{
  "id": "4238a7f0-a980-4fff-98a2-dfc0a363d507",
  "name": "Create Hidden User using UniqueID \u003c 500",
  "description": "Add a hidden user on macOS using Unique ID \u003c 500 (users with that ID are hidden by default)\n",
  "platforms": [
    "darwin"
  ],
  "steps": [
    {
      "id": "718ba687-2e4f-42c0-b20c-fddc558d43b8",
      "type": "execute-command",
      "data": {
        "command": "sudo dscl . -create /Users/#{user_name} UniqueID 333",
        "command_type": "sh"
      },
      "elevation_required": true
    },
    {
      "id": "be1774ab-02d4-4fcf-97eb-c16279f8a3ce",
      "type": "execute-command",
      "data": {
        "command": "sudo dscl . -delete /Users/#{user_name}",
        "command_type": "sh"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "718ba687-2e4f-42c0-b20c-fddc558d43b8",
      "p": "on-success",
      "o": "be1774ab-02d4-4fcf-97eb-c16279f8a3ce"
    },
    {
      "s": "718ba687-2e4f-42c0-b20c-fddc558d43b8",
      "p": "on-failure",
      "o": "be1774ab-02d4-4fcf-97eb-c16279f8a3ce"
    }
  ],
  "tags": [
    "T1564",
    "T1564.002"
  ],
  "input_arguments": [
    {
      "name": "user_name",
      "type": "string",
      "description": "username to add",
      "value": "APT"
    }
  ]
}