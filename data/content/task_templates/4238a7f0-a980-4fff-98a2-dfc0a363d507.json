{
  "id": "4238a7f0-a980-4fff-98a2-dfc0a363d507",
  "name": "Create Hidden User using UniqueID \u003c 500",
  "description": "Add a hidden user on macOS using Unique ID \u003c 500 (users with that ID are hidden by default)\n",
  "platforms": [
    "darwin"
  ],
  "steps": [
    {
      "id": "d9de7ba9-2010-4558-88b8-97e881671375",
      "type": "execute-command",
      "data": {
        "command": "sudo dscl . -create /Users/#{user_name} UniqueID 333\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "2dd6f1bb-cad0-440e-aa7d-cea7496f35d1",
      "type": "execute-command",
      "data": {
        "command": "sudo dscl . -delete /Users/#{user_name}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "d9de7ba9-2010-4558-88b8-97e881671375",
      "p": "on-success",
      "o": "2dd6f1bb-cad0-440e-aa7d-cea7496f35d1"
    },
    {
      "s": "d9de7ba9-2010-4558-88b8-97e881671375",
      "p": "on-failure",
      "o": "2dd6f1bb-cad0-440e-aa7d-cea7496f35d1"
    }
  ],
