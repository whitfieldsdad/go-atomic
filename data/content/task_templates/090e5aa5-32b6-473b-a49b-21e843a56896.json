    }
  ],
  "tags": [
    "T1546",
    "T1546.013"
  ],
  "input_arguments": [
    {
      "name": "exe_path",
      "type": "path",
      "description": "Path the malicious executable",
      "value": "calc.exe"
    },
    {
      "name": "ps_profile",
      "type": "string",
      "description": "Powershell profile to use",
      "value": "$profile"
    }
  ]
