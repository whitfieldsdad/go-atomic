{
  "id": "090e5aa5-32b6-473b-a49b-21e843a56896",
  "name": "Append malicious start-process cmdlet",
  "description": "Appends a start process cmdlet to the current user's powershell profile pofile that points to a malicious executable. Upon execution, calc.exe will be launched.\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "0c35d997-638f-4f13-bdef-491d252dab5e",
      "type": "execute-command",
      "data": {
        "command": "Add-Content #{ps_profile} -Value \"\"\nAdd-Content #{ps_profile} -Value \"Start-Process #{exe_path}\"\npowershell -Command exit",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "5b44e1f5-2ff0-4352-b6a5-fd7df1ebc419",
      "type": "execute-command",
      "data": {
        "command": "$oldprofile = cat $profile | Select-Object -skiplast 1\nSet-Content $profile -Value $oldprofile",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "071d9402-6852-4923-8b76-33b19fa94a62",
      "name": "Check dependency 1/1",
      "description": "Ensure a powershell profile exists for the current user\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path #{ps_profile}) {exit 0} else {exit 1}",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "ee85cb82-927f-4b71-ab92-078949dd055d",
      "name": "Resolve dependency 1/1",
      "description": "Ensure a powershell profile exists for the current user\n",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Path #{ps_profile} -Type File -Force",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "291955eb-3eee-4951-b706-efef182c6f40",
      "name": "Re-check dependency 1/1",
      "description": "Ensure a powershell profile exists for the current user\n",
      "type": "execute-command",
      "data": {
        "command": "if (Test-Path #{ps_profile}) {exit 0} else {exit 1}",
        "command_type": "powershell"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "0c35d997-638f-4f13-bdef-491d252dab5e",
      "p": "on-success",
      "o": "5b44e1f5-2ff0-4352-b6a5-fd7df1ebc419"
    },
    {
      "s": "0c35d997-638f-4f13-bdef-491d252dab5e",
      "p": "on-failure",
      "o": "5b44e1f5-2ff0-4352-b6a5-fd7df1ebc419"
    },
    {
      "s": "071d9402-6852-4923-8b76-33b19fa94a62",
      "p": "on-success",
      "o": "0c35d997-638f-4f13-bdef-491d252dab5e"
    },
    {
      "s": "071d9402-6852-4923-8b76-33b19fa94a62",
      "p": "on-failure",
      "o": "ee85cb82-927f-4b71-ab92-078949dd055d"
    },
    {
      "s": "ee85cb82-927f-4b71-ab92-078949dd055d",
      "p": "on-success",
      "o": "291955eb-3eee-4951-b706-efef182c6f40"
    },
    {
      "s": "291955eb-3eee-4951-b706-efef182c6f40",
      "p": "on-failure",
      "o": "0c35d997-638f-4f13-bdef-491d252dab5e"
    }
  ],
  "tags": [
    "T1546",
    "T1546.013"
  ],
  "input_arguments": [
    {
      "name": "exe_path",
      "type": "path",
      "description": "Path the malicious executable",
      "value": "calc.exe"
    },
    {
      "name": "ps_profile",
      "type": "string",
      "description": "Powershell profile to use",
      "value": "$profile"
    }
  ]
}