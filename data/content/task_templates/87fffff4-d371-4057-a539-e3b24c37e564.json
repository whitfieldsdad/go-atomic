{
  "id": "87fffff4-d371-4057-a539-e3b24c37e564",
  "name": "MacOS - Timestomp Date Modified",
  "description": "Stomps on the modification timestamp of a file using MacOS's SetFile utility\n",
  "platforms": [
    "darwin"
  ],
  "steps": [
    {
      "id": "8b644a60-af3f-4f5d-9211-00cfa72d2264",
      "type": "execute-command",
      "data": {
        "command": "SetFile -m #{target_date} #{target_filename}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "40cb983d-0888-49cb-9628-d4c5712396d3",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{target_filename}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "f76f4b0f-7447-40d9-a947-7e0a754e675d",
      "name": "Check dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{target_filename} \u0026\u0026 exit 0 || exit 1\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "08d364ea-b024-41ef-8e8d-6e2732d97505",
      "name": "Resolve dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "echo 'T1070.006 MacOS file modified timestomp test' \u003e #{target_filename}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "a419676f-66aa-4102-a6b5-96170e994964",
      "name": "Re-check dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{target_filename} \u0026\u0026 exit 0 || exit 1\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "8b644a60-af3f-4f5d-9211-00cfa72d2264",
      "p": "on-success",
      "o": "40cb983d-0888-49cb-9628-d4c5712396d3"
    },
    {
      "s": "8b644a60-af3f-4f5d-9211-00cfa72d2264",
      "p": "on-failure",
      "o": "40cb983d-0888-49cb-9628-d4c5712396d3"
    },
    {
      "s": "f76f4b0f-7447-40d9-a947-7e0a754e675d",
      "p": "on-success",
      "o": "8b644a60-af3f-4f5d-9211-00cfa72d2264"
    },
    {
      "s": "f76f4b0f-7447-40d9-a947-7e0a754e675d",
      "p": "on-failure",
      "o": "08d364ea-b024-41ef-8e8d-6e2732d97505"
    },
    {
      "s": "08d364ea-b024-41ef-8e8d-6e2732d97505",
      "p": "on-success",
      "o": "a419676f-66aa-4102-a6b5-96170e994964"
    },
    {
      "s": "a419676f-66aa-4102-a6b5-96170e994964",
      "p": "on-failure",
      "o": "8b644a60-af3f-4f5d-9211-00cfa72d2264"
    }
  ],
  "tags": [
    "T1070.006",
    "atomic-red-team"
  ],
  "input_arguments": [
    {
      "name": "target_date",
      "type": "string",
      "description": "Date to replace original timestamps with",
      "value": "01/01/1970"
    },
    {
      "name": "target_filename",
      "type": "path",
      "description": "Path of file that we are going to stomp on last modified time\n",
      "value": "/tmp/T1070.006-modified.txt"
    }
  ]
}