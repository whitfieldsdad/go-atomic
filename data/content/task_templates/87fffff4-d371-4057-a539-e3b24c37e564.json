{
  "id": "87fffff4-d371-4057-a539-e3b24c37e564",
  "name": "MacOS - Timestomp Date Modified",
  "description": "Stomps on the modification timestamp of a file using MacOS's SetFile utility\n",
  "platforms": [
    "darwin"
  ],
  "steps": [
    {
      "id": "955bd0c5-514e-4c2c-8087-8632cd39f56f",
      "type": "execute-command",
      "data": {
        "command": "SetFile -m #{target_date} #{target_filename}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "c02cced1-ae2e-47ca-a76b-844f72024a2e",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{target_filename}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "d8388f2c-8ab5-4f1a-8c0a-a2ed292f0b88",
      "name": "Check dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{target_filename} \u0026\u0026 exit 0 || exit 1",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "4d36c44f-b82e-4116-a601-3f22f1648bae",
      "name": "Resolve dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "echo 'T1070.006 MacOS file modified timestomp test' \u003e #{target_filename}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "7167bd3c-1646-4e30-8721-743930f7caa3",
      "name": "Re-check dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{target_filename} \u0026\u0026 exit 0 || exit 1",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "955bd0c5-514e-4c2c-8087-8632cd39f56f",
      "p": "on-success",
      "o": "c02cced1-ae2e-47ca-a76b-844f72024a2e"
    },
    {
      "s": "955bd0c5-514e-4c2c-8087-8632cd39f56f",
      "p": "on-failure",
      "o": "c02cced1-ae2e-47ca-a76b-844f72024a2e"
    },
    {
      "s": "d8388f2c-8ab5-4f1a-8c0a-a2ed292f0b88",
      "p": "on-success",
      "o": "955bd0c5-514e-4c2c-8087-8632cd39f56f"
    },
    {
      "s": "d8388f2c-8ab5-4f1a-8c0a-a2ed292f0b88",
      "p": "on-failure",
      "o": "4d36c44f-b82e-4116-a601-3f22f1648bae"
    },
    {
      "s": "4d36c44f-b82e-4116-a601-3f22f1648bae",
      "p": "on-success",
      "o": "7167bd3c-1646-4e30-8721-743930f7caa3"
    },
    {
      "s": "7167bd3c-1646-4e30-8721-743930f7caa3",
      "p": "on-failure",
      "o": "955bd0c5-514e-4c2c-8087-8632cd39f56f"
    }
  ],
  "tags": [
    "T1070",
    "T1070.006"
  ],
  "input_arguments": [
    {
      "name": "target_filename",
      "type": "path",
      "description": "Path of file that we are going to stomp on last modified time\n",
      "value": "/tmp/T1070.006-modified.txt"
    },
    {
      "name": "target_date",
      "type": "string",
      "description": "Date to replace original timestamps with",
      "value": "01/01/1970"
    }
  ]
}