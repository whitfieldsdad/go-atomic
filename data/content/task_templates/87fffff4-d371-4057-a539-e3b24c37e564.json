{
  "id": "87fffff4-d371-4057-a539-e3b24c37e564",
  "name": "MacOS - Timestomp Date Modified",
  "description": "Stomps on the modification timestamp of a file using MacOS's SetFile utility\n",
  "platforms": [
    "darwin"
  ],
  "steps": [
    {
      "id": "e790092a-0644-4462-8370-50b687bc5f3d",
      "type": "execute-command",
      "data": {
        "command": "SetFile -m #{target_date} #{target_filename}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "fc031497-ea79-44d9-974e-f00bdcfed4d4",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{target_filename}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "f4996e60-655c-450d-beb1-b880f73e1285",
      "name": "Check dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{target_filename} \u0026\u0026 exit 0 || exit 1",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "488bad6e-6386-47c8-8cf3-92bffeb5ea0e",
      "name": "Resolve dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "echo 'T1070.006 MacOS file modified timestomp test' \u003e #{target_filename}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "4550df62-d0c3-41e5-bf21-0882b2a90c7b",
      "name": "Re-check dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{target_filename} \u0026\u0026 exit 0 || exit 1",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "e790092a-0644-4462-8370-50b687bc5f3d",
      "p": "on-success",
      "o": "fc031497-ea79-44d9-974e-f00bdcfed4d4"
    },
    {
      "s": "e790092a-0644-4462-8370-50b687bc5f3d",
      "p": "on-failure",
      "o": "fc031497-ea79-44d9-974e-f00bdcfed4d4"
    },
    {
      "s": "f4996e60-655c-450d-beb1-b880f73e1285",
      "p": "on-success",
      "o": "e790092a-0644-4462-8370-50b687bc5f3d"
    },
    {
      "s": "f4996e60-655c-450d-beb1-b880f73e1285",
      "p": "on-failure",
      "o": "488bad6e-6386-47c8-8cf3-92bffeb5ea0e"
    },
    {
      "s": "488bad6e-6386-47c8-8cf3-92bffeb5ea0e",
      "p": "on-success",
      "o": "4550df62-d0c3-41e5-bf21-0882b2a90c7b"
    },
    {
      "s": "4550df62-d0c3-41e5-bf21-0882b2a90c7b",
      "p": "on-failure",
      "o": "e790092a-0644-4462-8370-50b687bc5f3d"
    }
  ],
  "tags": [
    "T1070",
    "T1070.006"
  ],
  "input_arguments": [
    {
      "name": "target_filename",
      "type": "path",
      "description": "Path of file that we are going to stomp on last modified time\n",
      "value": "/tmp/T1070.006-modified.txt"
    },
    {
      "name": "target_date",
      "type": "string",
      "description": "Date to replace original timestamps with",
      "value": "01/01/1970"
    }
  ]
}