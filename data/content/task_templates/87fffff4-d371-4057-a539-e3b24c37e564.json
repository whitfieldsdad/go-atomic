{
  "id": "87fffff4-d371-4057-a539-e3b24c37e564",
  "name": "MacOS - Timestomp Date Modified",
  "description": "Stomps on the modification timestamp of a file using MacOS's SetFile utility\n",
  "platforms": [
    "darwin"
  ],
  "steps": [
    {
      "id": "7100b409-3444-4761-923e-90d5f4359d98",
      "type": "execute-command",
      "data": {
        "command": "SetFile -m #{target_date} #{target_filename}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "3e68a2a9-06e5-4547-a94c-be99218f343c",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{target_filename}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "6648d903-de3d-4859-9d58-d2b4218eee56",
      "name": "Check dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{target_filename} \u0026\u0026 exit 0 || exit 1\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "33805d59-b7d1-4cff-a122-bc7c9633c546",
      "name": "Resolve dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "echo 'T1070.006 MacOS file modified timestomp test' \u003e #{target_filename}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "474683c3-29e8-40ef-8f13-d94742daceaa",
      "name": "Re-check dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{target_filename} \u0026\u0026 exit 0 || exit 1\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "7100b409-3444-4761-923e-90d5f4359d98",
      "p": "on-success",
      "o": "3e68a2a9-06e5-4547-a94c-be99218f343c"
    },
    {
      "s": "7100b409-3444-4761-923e-90d5f4359d98",
      "p": "on-failure",
      "o": "3e68a2a9-06e5-4547-a94c-be99218f343c"
    },
    {
      "s": "6648d903-de3d-4859-9d58-d2b4218eee56",
      "p": "on-success",
      "o": "7100b409-3444-4761-923e-90d5f4359d98"
    },
    {
      "s": "6648d903-de3d-4859-9d58-d2b4218eee56",
      "p": "on-failure",
      "o": "33805d59-b7d1-4cff-a122-bc7c9633c546"
    },
    {
      "s": "33805d59-b7d1-4cff-a122-bc7c9633c546",
      "p": "on-success",
      "o": "474683c3-29e8-40ef-8f13-d94742daceaa"
    },
    {
      "s": "474683c3-29e8-40ef-8f13-d94742daceaa",
      "p": "on-failure",
      "o": "7100b409-3444-4761-923e-90d5f4359d98"
    }
  ],
  "tags": [
    "T1070",
    "T1070.006"
  ],
  "input_arguments": [
    {
      "name": "target_date",
      "type": "string",
      "description": "Date to replace original timestamps with",
      "value": "01/01/1970"
    },
    {
      "name": "target_filename",
      "type": "path",
      "description": "Path of file that we are going to stomp on last modified time\n",
      "value": "/tmp/T1070.006-modified.txt"
    }
  ]
}