{
  "id": "5fc528dd-79de-47f5-8188-25572b7fafe0",
  "name": "List Safari Bookmarks on MacOS",
  "description": "This test searches for Safari's Bookmarks file (on macOS) and lists any found instances to a text file.\n",
  "platforms": [
    "darwin"
  ],
  "steps": [
    {
      "id": "7b50c24d-6774-4d7e-b11b-94431985def8",
      "type": "execute-command",
      "data": {
        "command": "find / -path \"*/Safari/Bookmarks.plist\" 2\u003e/dev/null \u003e\u003e #{output_file} \ncat #{output_file} \n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "27e474d7-a39d-404d-bc04-433645a25284",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{output_file} 2\u003e/dev/null\n",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "7b50c24d-6774-4d7e-b11b-94431985def8",
      "p": "on-success",
      "o": "27e474d7-a39d-404d-bc04-433645a25284"
    },
    {
      "s": "7b50c24d-6774-4d7e-b11b-94431985def8",
      "p": "on-failure",
      "o": "27e474d7-a39d-404d-bc04-433645a25284"
    }
  ],
  "tags": [
    "T1217"
  ],
  "input_arguments": [
    {
      "name": "output_file",
      "type": "path",
      "description": "Path where captured results will be placed.",
      "value": "/tmp/T1217-Safari.txt"
    }
  ]
}