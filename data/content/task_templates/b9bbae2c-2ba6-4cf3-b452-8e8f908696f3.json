{
  "id": "b9bbae2c-2ba6-4cf3-b452-8e8f908696f3",
  "name": "office-365-Disable-AntiPhishRule",
  "description": "Using the Disable-AntiPhishRule cmdlet to disable antiphish rules in your office-365 organization.\n",
  "platforms": [
    "office-365"
  ],
  "steps": [
    {
      "id": "72b5a95c-839b-407c-b396-b52d344641fd",
      "type": "execute-command",
      "data": {
        "command": "$secure_pwd = \"#{password}\" | ConvertTo-SecureString -AsPlainText -Force\n$creds = New-Object System.Management.Automation.PSCredential -ArgumentList \"#{username}\", $secure_pwd\nConnect-ExchangeOnline -Credential $creds\n$test = Get-AntiPhishRule\nDisable-AntiPhishRule -Identity $test.Name -Confirm:$false\nGet-AntiPhishRule\n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "8685b706-7b00-4bc0-86f6-e5fcf1d9b1b6",
      "type": "execute-command",
      "data": {
        "command": "if(\"#{password}\" -ne \"\") {\n$secure_pwd = (\"#{password}\" + \"\") | ConvertTo-SecureString -AsPlainText -Force\n$creds = New-Object System.Management.Automation.PSCredential -ArgumentList \"#{username}\", $secure_pwd\nConnect-ExchangeOnline -Credential $creds\n$test = Get-AntiPhishRule\nEnable-AntiPhishRule -Identity $test.Name -Confirm:$false\nGet-AntiPhishRule\n} \n",
        "command_type": "powershell"
      },
      "elevation_required": false
    },
    {
      "id": "569852f0-2f1a-4dca-8371-54c388be0f21",
      "name": "Check dependency 1/1",
      "description": "ExchangeOnlineManagement PowerShell module must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "$RequiredModule = Get-Module -Name ExchangeOnlineManagement -ListAvailable\nif (-not $RequiredModule) {exit 1}\nif (-not $RequiredModule.ExportedCommands['Connect-ExchangeOnline']) {exit 1} else {exit 0}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "f600465c-a501-46af-bbcd-7968d0832d63",
      "name": "Resolve dependency 1/1",
      "description": "ExchangeOnlineManagement PowerShell module must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "Install-Module -Name ExchangeOnlineManagement\nImport-Module ExchangeOnlineManagement\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "41ec8136-cc43-4c94-b358-666d20d4d133",
      "name": "Re-check dependency 1/1",
      "description": "ExchangeOnlineManagement PowerShell module must be installed\n",
      "type": "execute-command",
      "data": {
        "command": "$RequiredModule = Get-Module -Name ExchangeOnlineManagement -ListAvailable\nif (-not $RequiredModule) {exit 1}\nif (-not $RequiredModule.ExportedCommands['Connect-ExchangeOnline']) {exit 1} else {exit 0}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
