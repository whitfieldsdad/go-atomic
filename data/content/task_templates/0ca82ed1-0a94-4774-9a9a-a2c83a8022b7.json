{
  "id": "0ca82ed1-0a94-4774-9a9a-a2c83a8022b7",
  "name": "Stop/Start Packet Filter",
  "description": "Stop the Packet Filter if installed.\n",
  "platforms": [
    "linux"
  ],
  "steps": [
    {
      "id": "b77aceb1-f7a8-48f0-9a97-47d43b5f915c",
      "type": "execute-command",
      "data": {
        "command": "service pf stop\nservice pf disable\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "17d1f31c-8a7a-4382-8c79-9cf87af905f8",
      "type": "execute-command",
      "data": {
        "command": "service pf enable\nservice pf start\nservice pf status\n",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "923fd4e4-7100-42de-9000-cfeb6b5e54dd",
      "name": "Check dependency 1/1",
      "description": "Check if pfctl is installed on the machine.\n",
      "type": "execute-command",
      "data": {
        "command": "if [ ! -x \"$(command -v pfctl)\" ]; then echo -e \"\\n***** PF NOT installed *****\\n\"; exit 1; fi\nif [ \"$(kldstat -n pf)\" = \"\" ]; then echo -e \"\\n***** PF inactive *****\\n\"; exit 1; fi \n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "6ffb69c6-055e-4014-aa89-154e7e61b1e8",
      "name": "Resolve dependency 1/1",
