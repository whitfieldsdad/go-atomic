{
  "id": "20ef1523-8758-4898-b5a2-d026cc3d2c52",
  "name": "Set a file's modification timestamp",
  "description": "Stomps on the modification timestamp of a file\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "edbf2c12-9aab-4c46-adf4-031d3e0bd4d0",
      "type": "execute-command",
      "data": {
        "command": "touch -m -t 197001010000.00 #{target_filename}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "3c5488f0-e5ab-46d5-89d1-14cc3a8f8812",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{target_filename}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "0d466d0e-4eaa-4e1c-8fd7-b32c7a762a24",
      "name": "Check dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{target_filename} \u0026\u0026 exit 0 || exit 1",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "e2547583-798e-4edd-b11d-ba035bdd4914",
      "name": "Resolve dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "echo 'T1070.006 file modification timestomp test' \u003e #{target_filename}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "fbe24163-860d-4626-9770-1ac0fd9b3bbe",
      "name": "Re-check dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{target_filename} \u0026\u0026 exit 0 || exit 1",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "edbf2c12-9aab-4c46-adf4-031d3e0bd4d0",
      "p": "on-success",
      "o": "3c5488f0-e5ab-46d5-89d1-14cc3a8f8812"
    },
    {
      "s": "edbf2c12-9aab-4c46-adf4-031d3e0bd4d0",
      "p": "on-failure",
      "o": "3c5488f0-e5ab-46d5-89d1-14cc3a8f8812"
    },
    {
      "s": "0d466d0e-4eaa-4e1c-8fd7-b32c7a762a24",
      "p": "on-success",
      "o": "edbf2c12-9aab-4c46-adf4-031d3e0bd4d0"
    },
    {
      "s": "0d466d0e-4eaa-4e1c-8fd7-b32c7a762a24",
      "p": "on-failure",
      "o": "e2547583-798e-4edd-b11d-ba035bdd4914"
    },
    {
      "s": "e2547583-798e-4edd-b11d-ba035bdd4914",
      "p": "on-success",
      "o": "fbe24163-860d-4626-9770-1ac0fd9b3bbe"
    },
    {
      "s": "fbe24163-860d-4626-9770-1ac0fd9b3bbe",
      "p": "on-failure",
      "o": "edbf2c12-9aab-4c46-adf4-031d3e0bd4d0"
    }
  ],
  "tags": [
    "T1070",
    "T1070.006"
  ],
  "input_arguments": [
    {
      "name": "target_filename",
      "type": "path",
      "description": "Path of file that we are going to stomp on last access time",
      "value": "/tmp/T1070.006-modification.txt"
    }
  ]
}