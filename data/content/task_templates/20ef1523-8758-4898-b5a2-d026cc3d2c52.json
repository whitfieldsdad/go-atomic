{
  "id": "20ef1523-8758-4898-b5a2-d026cc3d2c52",
  "name": "Set a file's modification timestamp",
  "description": "Stomps on the modification timestamp of a file\n",
  "platforms": [
    "linux",
    "darwin"
  ],
  "steps": [
    {
      "id": "7341c67d-4340-4c94-87b4-7c82a06d8967",
      "type": "execute-command",
      "data": {
        "command": "touch -m -t 197001010000.00 #{target_filename}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "17b8d2d4-8e37-45aa-b92f-43e02b4ce684",
      "type": "execute-command",
      "data": {
        "command": "rm -f #{target_filename}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "726d4a75-0f72-4d13-a786-d62be4de876b",
      "name": "Check dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{target_filename} \u0026\u0026 exit 0 || exit 1",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "32dbf033-34fe-470f-a7d9-682bf3d40b35",
      "name": "Resolve dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "echo 'T1070.006 file modification timestomp test' \u003e #{target_filename}",
        "command_type": "sh"
      },
      "elevation_required": false
    },
    {
      "id": "ef1bff66-50b4-448c-8563-a4ce20ae28a9",
      "name": "Re-check dependency 1/1",
      "description": "The file must exist in order to be timestomped\n",
      "type": "execute-command",
      "data": {
        "command": "test -e #{target_filename} \u0026\u0026 exit 0 || exit 1",
        "command_type": "sh"
      },
      "elevation_required": false
    }
  ],
  "flows": [
    {
      "s": "7341c67d-4340-4c94-87b4-7c82a06d8967",
      "p": "on-success",
      "o": "17b8d2d4-8e37-45aa-b92f-43e02b4ce684"
    },
    {
      "s": "7341c67d-4340-4c94-87b4-7c82a06d8967",
      "p": "on-failure",
      "o": "17b8d2d4-8e37-45aa-b92f-43e02b4ce684"
    },
    {
      "s": "726d4a75-0f72-4d13-a786-d62be4de876b",
      "p": "on-success",
      "o": "7341c67d-4340-4c94-87b4-7c82a06d8967"
    },
    {
      "s": "726d4a75-0f72-4d13-a786-d62be4de876b",
      "p": "on-failure",
      "o": "32dbf033-34fe-470f-a7d9-682bf3d40b35"
    },
    {
      "s": "32dbf033-34fe-470f-a7d9-682bf3d40b35",
      "p": "on-success",
      "o": "ef1bff66-50b4-448c-8563-a4ce20ae28a9"
    },
