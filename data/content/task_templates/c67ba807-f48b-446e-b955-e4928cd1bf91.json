{
  "id": "c67ba807-f48b-446e-b955-e4928cd1bf91",
  "name": "Windows Internal pktmon capture",
  "description": "Will start a packet capture and store log file as t1040.etl.\nhttps://lolbas-project.github.io/lolbas/Binaries/Pktmon/",
  "platforms": [
    "windows"
  ],
  "steps": [
    {
      "id": "b304c572-cd30-4b11-8479-2ebf017fdcd7",
      "type": "execute-command",
      "data": {
        "command": "pktmon.exe start --etw  -f %TEMP%\\t1040.etl\nTIMEOUT /T 5 \u003enul 2\u003e\u00261\npktmon.exe stop\n",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    },
    {
      "id": "1def709f-a9c0-4f7d-9276-c70ef4449f6a",
      "type": "execute-command",
      "data": {
        "command": "del %TEMP%\\t1040.etl",
        "command_type": "command_prompt"
      },
      "elevation_required": true
    }
  ],
  "flows": [
    {
      "s": "b304c572-cd30-4b11-8479-2ebf017fdcd7",
      "p": "on-success",
      "o": "1def709f-a9c0-4f7d-9276-c70ef4449f6a"
    },
    {
      "s": "b304c572-cd30-4b11-8479-2ebf017fdcd7",
      "p": "on-failure",
      "o": "1def709f-a9c0-4f7d-9276-c70ef4449f6a"
    }
  ],
  "tags": [
    "T1040"
  ]
}