{
    "id": "c955a599-3653-4fe5-b631-f11c00eb0397",
    "name": "View accounts with UID 0",
    "description": "View accounts with UID 0",
    "platforms": [
        "linux", 
        "darwin"
    ],
    "input_arguments": [
        {
            "name": "output_file",
            "type": "path",
            "description": "Path where captured results will be placed",
            "value": "/tmp/T1087.001.txt"
        }
    ],
    "steps": [
        {
            "id": "d4b50bf5-34b0-401a-9ac8-3e3b513c2a27",
            "type": "execute-command",
            "data": {
                "command": "grep 'x:0:' /etc/passwd > #{output_file}\ngrep '*:0:' /etc/passwd >> #{output_file}\ncat #{output_file} 2>/dev/null",
                "command_type": "sh"
            }        
        },
        {
            "id": "55ec1b1c-e357-40a4-a28c-94fa2c452114",
            "type": "execute-command",
            "data": {
                "command": "rm -f #{output_file} 2>/dev/null",
                "command_type": "sh"
            }
        }
    ],
    "dependencies": [
